          ;;
        28)
          _arguments \
            "-SHAPES[<str>Shapes]:file:_files" "-COPY[<str> Copy]:file:_files"  
          ;;
        29)
          _arguments \
            "-POINTS[<str> Points]:file:_files" "-STRIKE[<str> Strike]:file:_files" "-DIP[<str> Dip]:file:_files" "-RAKE[<str> Rake]:file:_files" "-SIZE[<str> Size]:file:_files" "-SIZE_DEF[<double> Default]:file:_files" "-SIZE_RANGE_MIN[<double>Scale to...]:file:_files" "-SIZE_RANGE_MAX[<double>Scale to...]:file:_files" "-PLOTS[<str> Focal Mechanism Beachballs]:file:_files" "-DARC[<double> Arc Vertex Distance (Degree)]:file:_files" "-STYLE[<str> Style]:file:_files"  
          ;;
        30)
          _arguments \
            "-POINTS[<str> Points]:file:_files" "-DIRECTION[<str> Direction]:file:_files" "-LENGTH[<str> Length]:file:_files" "-X_COMP[<str> X-Component]:file:_files" "-Y_COMP[<str> Y-Component]:file:_files" "-DEFINITION[<str>Gradient Definition]:file:_files" "-SCALING[<double>Length Scaling Factor]:file:_files" "-GRADIENTS[<str> Gradient]:file:_files" "-TARGETS[<str> Target Points]:file:_files"  
