# ------------------------------------------------------------------------------
# INFO:
# This script provides zsh command completion for saga_cmd.
# The completion requires the following order: saga_cmd <opt> <library> <tool> <parameter>.
# ------------------------------------------------------------------------------
# SAGA Version: 9.6.0 
# ------------------------------------------------------------------------------

_saga_cmd() {
  local line state
  _arguments -C "1: :->cmds" "*:: :->args"
  case "$state" in
    cmds)
      _values "saga_cmd commands" \
        "climate_tools[Tools for the processing and analysis of climate and weather data.]" \
        "contrib_perego[Contributions from Alessandro Perego. Go to www.alspergis.altervista.org(http://www.alspergis.altervista.org/software/software.html#saga) for further information.]" \
        "db_odbc[Database access via Open Data Base Connection (ODBC) interface. Based on the OTL (Oracle, Odbc and DB2-CLI Template Library), Version 4.0: http://otl.sourceforge.net/(http://otl.sourceforge.net/)]" \
        "db_pgsql[Accessing PostgreSQL databases.]" \
        "docs_html[Reports and document creation in Hypertext Mark Up Language (HTML) format.]" \
        "docs_pdf[Create reports in the Portable Document Format (PDF). The export tools are using the free open source library libHaru(https://github.com/libharu/libharu) version 2.4.3]" \
        "garden_fractals[Fractals]" \
        "garden_webservices[Web Services]" \
        "grid_analysis[Some Grid Analysis Tools.]" \
        "grid_calculus[Grid based or related calculations.]" \
        "grid_calculus_bsl[Boehners Simple Language (BSL) is a macro script language for grid based calculations.]" \
        "grid_filter[Various filter and convolution tools for raster data.]" \
        "grid_gridding[Tools for the gridding of points and other vector data.]" \
        "grid_spline[Several spline interpolation/approximation methods for the gridding of scattered data. In most cases the Multilevel B-spline Interpolation might be the optimal choice. ]" \
        "grids_tools[Tools for grid collections.]" \
        "grid_tools[Tools for the manipulation of gridded data.]" \
        "grid_visualisation[Visualization tools for grids.]" \
        "imagery_classification[Classification tools for grids.]" \
        "imagery_isocluster[ISODATA Clustering tools]" \
        "imagery_maxent[Maximum entropy based classification and prediction.]" \
        "imagery_opencv[OpenCV - Open Source Computer Vision Library]" \
        "imagery_photogrammetry[Photogrammetry tools.]" \
        "imagery_segmentation[Image segmentation algorithms.]" \
        "imagery_svm[Interface to LIBSVM - A Library for Support Vector Machines.]" \
        "imagery_tools[Image processing and analysis tools.]" \
        "imagery_vigra[ViGrA - Vision with Generic Algorithms]" \
        "io_esri_e00[Import and export filter for ESRIs E00 file exchange format.]" \
        "io_gdal[Interface to Frank Warmerdams Geospatial Data Abstraction Library (GDAL).]" \
        "io_gps[Tools for GPS data handling.]" \
        "io_grid[Tools for the import and export of gridded data.]" \
        "io_grid_image[Image Import/Export.]" \
        "io_pdal[Tools that use the Point Data Abstraction Library (PDAL).]" \
        "io_shapes[Tools for the import and export of vector data.]" \
        "io_table[Tools for the import and export of tables.]" \
        "io_virtual[Tools for the handling of virtual datasets.]" \
        "io_webservices[Web Services]" \
        "pj_georeference[Tools for the georeferencing of spatial data.]" \
        "pj_proj4[Projection routines make use of the PROJ generic coordinate transformation software.]" \
        "pointcloud_tools[Tools for point clouds.]" \
        "shapes_grid[Tools related to gridded and vector data (conversions, combinations, etc.).]" \
        "shapes_lines[Tools for lines.]" \
        "shapes_points[Tools for the manipulation of point vector data.]" \
        "shapes_polygons[Tools for polygons.]" \
        "shapes_tools[Tools for the manipulation of vector data.]" \
        "shapes_transect[A SAGA tool to create transects through polygon files.]" \
        "sim_air_flow[Air Flow Simulations]" \
        "sim_cellular_automata[Cellular Automata]" \
        "sim_ecosystems_hugget[Numerical models for ecological processes. Examples have been taken from:]" \
        "sim_erosion[Modelling erosion processes.]" \
        "sim_geomorphology[This library contains tools for the simulation of geomorphological processes.]" \
        "sim_hydrology[Simulation of hydrological processes.]" \
        "sim_ihacres[The metric conceptual rainfall-runoff model IHACRES (Identification of unit Hydrographs and Component flows from Rainfall, Evaporation and Streamflow data) has been implemented as a module library. The IHACRES model was developed by: Jakeman, A.J. and G.M. Hornberger (1993); Jakeman et al. (1990).]" \
        "sim_landscape_evolution[Tools for landscape evolution modelling.]" \
        "sim_qm_of_esp[Quantitative Modeling of Earth Surface Processes.]" \
        "sim_rivflow[RivFlow.]" \
        "statistics_grid[Tools for spatial and geostatistical analyses.]" \
        "statistics_kriging[Kriging - tools for the geostatistical interpolation of irregularly distributed point data.]" \
        "statistics_points[Spatial and geostatistical analyses of point data.]" \
        "statistics_regression[Tools for correlation and regression analyses.]" \
        "table_calculus[Tools for table based analyses and calculations.]" \
        "table_tools[Tools for the creation and manipulation of tables.]" \
        "ta_channels[Tools for (grid based) digital terrain analysis.]" \
        "ta_cliffmetrics[CliffMetrics (Automatic Cliff Metrics delineation) delineates the location of the coastline, coastline normals, and cliff top and toe location along these normals. ]" \
        "ta_compound[Terrain Analysis: Compound Analyses.]" \
        "ta_hydrology[Tools for digital terrain analysis.]" \
        "ta_lighting[Lighting and visibility calculations for digital terrain models.]" \
        "ta_morphometry[Tools for (grid based) digital terrain analysis.]" \
        "ta_preprocessor[Tools for the preprocessing of digital terrain models.]" \
        "ta_profiles[Simple, flow path and swath profiles.]" \
        "ta_slope_stability[Tools for slope stability analyses. Developed by Andreas G&uuml;nther, BGR, B2.2]" \
        "tin_tools[Tools for Triangulated Irregular Network (TIN) processing.]" \
        "_tool_chains_uncategorized[]" \
        "tta_tools[ The travel time tools were created to join together and automate a number of processing steps in order to simplify service access modelling. The purpose of these tools is to create a simple open source way of calculating travel time to services to facilitate rapid testing of multiple travel scenarios.]" \
        "vis_3d_viewer[3D Viewer.]" \

      ;;
    args)
      case $line[1] in
        climate_tools) 
          _climate_tools 
          ;;
        contrib_perego) 
          _contrib_perego 
          ;;
        db_odbc) 
          _db_odbc 
          ;;
        db_pgsql) 
          _db_pgsql 
          ;;
        docs_html) 
          _docs_html 
          ;;
        docs_pdf) 
          _docs_pdf 
          ;;
        garden_fractals) 
          _garden_fractals 
          ;;
        garden_webservices) 
          _garden_webservices 
          ;;
        grid_analysis) 
          _grid_analysis 
          ;;
        grid_calculus) 
          _grid_calculus 
          ;;
        grid_calculus_bsl) 
          _grid_calculus_bsl 
          ;;
        grid_filter) 
          _grid_filter 
          ;;
        grid_gridding) 
          _grid_gridding 
          ;;
        grid_spline) 
          _grid_spline 
          ;;
        grids_tools) 
          _grids_tools 
          ;;
        grid_tools) 
          _grid_tools 
          ;;
        grid_visualisation) 
          _grid_visualisation 
          ;;
        imagery_classification) 
          _imagery_classification 
          ;;
        imagery_isocluster) 
          _imagery_isocluster 
          ;;
        imagery_maxent) 
          _imagery_maxent 
          ;;
        imagery_opencv) 
          _imagery_opencv 
          ;;
        imagery_photogrammetry) 
          _imagery_photogrammetry 
          ;;
        imagery_segmentation) 
          _imagery_segmentation 
          ;;
        imagery_svm) 
          _imagery_svm 
          ;;
        imagery_tools) 
          _imagery_tools 
          ;;
        imagery_vigra) 
          _imagery_vigra 
          ;;
        io_esri_e00) 
          _io_esri_e00 
          ;;
        io_gdal) 
          _io_gdal 
          ;;
        io_gps) 
          _io_gps 
          ;;
        io_grid) 
          _io_grid 
          ;;
        io_grid_image) 
          _io_grid_image 
          ;;
        io_pdal) 
          _io_pdal 
          ;;
        io_shapes) 
          _io_shapes 
          ;;
        io_table) 
          _io_table 
          ;;
        io_virtual) 
          _io_virtual 
          ;;
        io_webservices) 
          _io_webservices 
          ;;
        pj_georeference) 
          _pj_georeference 
          ;;
        pj_proj4) 
          _pj_proj4 
          ;;
        pointcloud_tools) 
          _pointcloud_tools 
          ;;
        shapes_grid) 
          _shapes_grid 
          ;;
        shapes_lines) 
          _shapes_lines 
          ;;
        shapes_points) 
          _shapes_points 
          ;;
        shapes_polygons) 
          _shapes_polygons 
          ;;
        shapes_tools) 
          _shapes_tools 
          ;;
        shapes_transect) 
          _shapes_transect 
          ;;
        sim_air_flow) 
          _sim_air_flow 
          ;;
        sim_cellular_automata) 
          _sim_cellular_automata 
          ;;
        sim_ecosystems_hugget) 
          _sim_ecosystems_hugget 
          ;;
        sim_erosion) 
          _sim_erosion 
          ;;
        sim_geomorphology) 
          _sim_geomorphology 
          ;;
        sim_hydrology) 
          _sim_hydrology 
          ;;
        sim_ihacres) 
          _sim_ihacres 
          ;;
        sim_landscape_evolution) 
          _sim_landscape_evolution 
          ;;
        sim_qm_of_esp) 
          _sim_qm_of_esp 
          ;;
        sim_rivflow) 
          _sim_rivflow 
          ;;
        statistics_grid) 
          _statistics_grid 
          ;;
        statistics_kriging) 
          _statistics_kriging 
          ;;
        statistics_points) 
          _statistics_points 
          ;;
        statistics_regression) 
          _statistics_regression 
          ;;
        table_calculus) 
          _table_calculus 
          ;;
        table_tools) 
          _table_tools 
          ;;
        ta_channels) 
          _ta_channels 
          ;;
        ta_cliffmetrics) 
          _ta_cliffmetrics 
          ;;
        ta_compound) 
          _ta_compound 
          ;;
        ta_hydrology) 
          _ta_hydrology 
          ;;
        ta_lighting) 
          _ta_lighting 
          ;;
        ta_morphometry) 
          _ta_morphometry 
          ;;
        ta_preprocessor) 
          _ta_preprocessor 
          ;;
        ta_profiles) 
          _ta_profiles 
          ;;
        ta_slope_stability) 
          _ta_slope_stability 
          ;;
        tin_tools) 
          _tin_tools 
          ;;
        _tool_chains_uncategorized) 
          __tool_chains_uncategorized 
          ;;
        tta_tools) 
          _tta_tools 
          ;;
        vis_3d_viewer) 
          _vis_3d_viewer 
          ;;
      esac
      ;;
  esac
}


# individual tool library functions
_climate_tools(){
  _arguments -C "1: :->cmds" "*::arg:->args" 
 
  case "$state" in
    cmds)
      _values "climate_tools command" \
        "0[Multi Level to Surface Interpolation]" \
        "1[Multi Level to Points Interpolation]" \
        "2[Earths Orbital Parameters]" \
        "3[Annual Course of Daily Insolation]" \
        "4[Daily Insolation over Latitude]" \
        "5[Monthly Global by Latitude]" \
        "6[Evapotranspiration (Table)]" \
        "7[Daily to Hourly Evapotranspiration]" \
        "8[Evapotranspiration (Grid)]" \
        "9[Sunrise and Sunset]" \
        "10[Bioclimatic Variables]" \
        "11[Tree Growth Season]" \
        "13[Wind Effect Correction]" \
        "14[Frost Change Frequency]" \
        "15[Thermic Belt Classification]" \
        "17[Snow Cover]" \
        "18[Growing Degree Days]" \
        "19[Climate Classification]" \
        "20[Soil Water Balance (Annual)]" \
        "21[PhenIps (Table)]" \
        "22[PhenIps (Grids, Annual)]" \
        "23[PhenIps (Grids, Days)]" \
        "24[Soil Water Balance (Days)]" \
        "25[Cloud Overlap]" \
        "26[Temperature Lapse Rates]" \
        "27[Air Pressure Adjustment]" \
        "28[Land Surface Temperature]" \
        "29[Air Humidity Conversions]" \
        "30[Lapse Rate Based Temperature Downscaling]" \
        "31[Daily Solar Radiation]" \
        "32[Lapse Rate Based Temperature Interpolation]" \
        "temperature_downscaling[Lapse Rate Based Temperature Downscaling (Bulk Processing)]" \

      ;;
    args)
      case "$line[1]" in
        0)
          _arguments \
            "-VARIABLE[<str> Variable]:file:_files" "-X_SOURCE[<str> Get Heights from ...]:file:_files" "-X_GRIDS[<str> Level Heights]:file:_files" "-X_GRIDS_CHECK[<str>Minimum Height]:file:_files" "-X_TABLE[<str> Level Heights]:file:_files" "-H_METHOD[<str> Horizontal Interpolation Method]:file:_files" "-V_METHOD[<str> Vertical Interpolation Method]:file:_files" "-COEFFICIENTS[<str> Coefficient Interpolation]:file:_files" "-LINEAR_SORTED[<str>Sorted Levels]:file:_files" "-SPLINE_ALL[<str> Pre-analyze]:file:_files" "-TREND_ORDER[<num> Polynomial Order]:file:_files" "-SURFACE[<str> Surface]:file:_files" "-RESULT[<str> Interpolation]:file:_files"  
          ;;
        1)
          _arguments \
            "-VARIABLE[<str> Variable]:file:_files" "-X_SOURCE[<str> Get Heights from ...]:file:_files" "-X_GRIDS[<str> Level Heights]:file:_files" "-X_GRIDS_CHECK[<str>Minimum Height]:file:_files" "-X_TABLE[<str> Level Heights]:file:_files" "-H_METHOD[<str> Horizontal Interpolation Method]:file:_files" "-V_METHOD[<str> Vertical Interpolation Method]:file:_files" "-COEFFICIENTS[<str> Coefficient Interpolation]:file:_files" "-LINEAR_SORTED[<str>Sorted Levels]:file:_files" "-SPLINE_ALL[<str> Pre-analyze]:file:_files" "-TREND_ORDER[<num> Polynomial Order]:file:_files" "-POINTS[<str> Points]:file:_files" "-ZFIELD[<str> Height]:file:_files" "-RESULT[<str> Result]:file:_files" "-NAME[<str> Field Name]:file:_files"  
          ;;
        2)
          _arguments \
            "-ORBPAR[<str> Earth's Orbital Parameters]:file:_files" "-START[<double>Start (ka)]:file:_files" "-STOP[<double> Stop (ka)]:file:_files" "-STEP[<double> Step (ka)]:file:_files"  
          ;;
        3)
          _arguments \
            "-SOLARRAD[<str>Solar Radiation]:file:_files" "-START[<double>Start (ka)]:file:_files" "-STOP[<double> Stop (ka)]:file:_files" "-STEP[<double> Step (ka)]:file:_files" "-LAT[<double> Latitude (Degree)]:file:_files"  
          ;;
        4)
          _arguments \
            "-SOLARRAD[<str>Solar Radiation]:file:_files" "-START[<double>Start (ka)]:file:_files" "-STOP[<double> Stop (ka)]:file:_files" "-STEP[<double> Step (ka)]:file:_files" "-DLAT[<num> Latitude Increment (Degree)]:file:_files" "-DAY[<num> Day of Year]:file:_files"  
          ;;
        5)
          _arguments \
            "-SOLARRAD[<str>Solar Radiation]:file:_files" "-ALBEDO[<str> Albedo]:file:_files" "-FIELD[<str> Field]:file:_files" "-YEAR[<double> Year (ka)]:file:_files" "-DLAT[<num> Latitude Increment (Degree)]:file:_files"  
          ;;
        6)
          _arguments \
            "-TABLE[<str> Data]:file:_files" "-RESULT[<str>Results]:file:_files" "-T[<str> Mean Temperature]:file:_files" "-T_MIN[<str> Minimum Temperature]:file:_files" "-T_MAX[<str> Maximum Temperature]:file:_files" "-RH[<str> Relative Humidity]:file:_files" "-SR[<str> Solar Radiation]:file:_files" "-WS[<str> Wind Speed]:file:_files" "-P[<str> Air Pressure]:file:_files" "-DATE[<str> Date]:file:_files" "-ET[<str> Evapotranspiration]:file:_files" "-LAT[<double>Latitude]:file:_files" "-METHOD[<str>Method]:file:_files"  
          ;;
        7)
          _arguments \
            "-DAYS[<str> Daily Data]:file:_files" "-JD[<str> Julian Day]:file:_files" "-ET[<str> Evapotranspiration]:file:_files" "-P[<str> Precipitation]:file:_files" "-HOURS[<str> Hourly Data]:file:_files" "-LAT[<double>Latitude]:file:_files"  
          ;;
        8)
          _arguments \
            "-T[<str> Mean Temperature]:file:_files" "-T_DEFAULT[<double> Default]:file:_files" "-T_MIN[<str> Minimum Temperature]:file:_files" "-T_MIN_DEFAULT[<double>Default]:file:_files" "-T_MAX[<str> Maximum Temperature]:file:_files" "-T_MAX_DEFAULT[<double>Default]:file:_files" "-RH[<str> Relative Humidity]:file:_files" "-RH_DEFAULT[<double> Default]:file:_files" "-SR[<str> Solar Radiation]:file:_files" "-SR_DEFAULT[<double> Default]:file:_files" "-WS[<str> Wind Speed]:file:_files" "-WS_DEFAULT[<double> Default]:file:_files" "-P[<str> Air Pressure]:file:_files" "-P_DEFAULT[<double> Default]:file:_files" "-ET[<str> Potential Evapotranspiration]:file:_files" "-METHOD[<str> Method]:file:_files" "-SR_EST[<str> Estimate Solar Radiation]:file:_files" "-SUNSHINE[<double> Sunshine Duration]:file:_files" "-TIME[<str> Time]:file:_files" "-MONTH[<str> Month]:file:_files" "-DAY[<num> Day of Month]:file:_files" "-LAT[<double> Latitude]:file:_files"  
          ;;
        9)
          _arguments \
            "-TARGET[<str> Target System]:file:_files" "-SUNRISE[<str>Sunrise]:file:_files" "-SUNSET[<str> Sunset]:file:_files" "-LENGTH[<str> Day Length]:file:_files" "-DAY[<date> Day of Month]:file:_files" "-TIME[<str> Time]:file:_files"  
          ;;
        10)
          _arguments \
            "-TMEAN[<str> Mean Temperature]:file:_files" "-TMIN[<str> Minimum Temperature]:file:_files" "-TMAX[<str> Maximum Temperature]:file:_files" "-P[<str> Precipitation]:file:_files" "-BIO_01[<str> Annual Mean Temperature]:file:_files" "-BIO_02[<str> Mean Diurnal Range]:file:_files" "-BIO_03[<str> Isothermality]:file:_files" "-BIO_04[<str> Temperature Seasonality]:file:_files" "-BIO_05[<str> Maximum Temperature of Warmest Month]:file:_files" "-BIO_06[<str> Minimum Temperature of Coldest Month]:file:_files" "-BIO_07[<str> Temperature Annual Range]:file:_files" "-BIO_08[<str> Mean Temperature of Wettest Quarter]:file:_files" "-BIO_09[<str> Mean Temperature of Driest Quarter]:file:_files" "-BIO_10[<str> Mean Temperature of Warmest Quarter]:file:_files" "-BIO_11[<str> Mean Temperature of Coldest Quarter]:file:_files" "-BIO_12[<str> Annual Precipitation]:file:_files" "-BIO_13[<str> Precipitation of Wettest Month]:file:_files" "-BIO_14[<str> Precipitation of Driest Month]:file:_files" "-BIO_15[<str> Precipitation Seasonality]:file:_files" "-BIO_16[<str> Precipitation of Wettest Quarter]:file:_files" "-BIO_17[<str> Precipitation of Driest Quarter]:file:_files" "-BIO_18[<str> Precipitation of Warmest Quarter]:file:_files" "-BIO_19[<str> Precipitation of Coldest Quarter]:file:_files" "-QUARTER_COLDEST[<str>Coldest Quarter]:file:_files" "-QUARTER_WARMEST[<str>Warmest Quarter]:file:_files" "-QUARTER_DRIEST[<str> Driest Quarter]:file:_files" "-QUARTER_WETTEST[<str>Wettest Quarter]:file:_files" "-SEASONALITY[<str> Temperature Seasonality]:file:_files"  
          ;;
        11)
          _arguments \
            "-T[<str> Mean Temperature]:file:_files" "-TMIN[<str> Minimum Temperature]:file:_files" "-TMAX[<str> Maximum Temperature]:file:_files" "-P[<str> Precipitation]:file:_files" "-SWC[<str> Soil Water Capacity of Profile]:file:_files" "-SWC_DEFAULT[<double> Default]:file:_files" "-SWC_SURFACE[<double> Top Soil Water Capacity]:file:_files" "-SW1_RESIST[<double> Transpiration Resistance]:file:_files" "-LAT_DEF[<double> Default Latitude]:file:_files" "-SMT[<str> Mean Temperature]:file:_files" "-SMP[<str> Precipitation Sum]:file:_files" "-LGS[<str> Length]:file:_files" "-FIRST[<str> First Growing Day]:file:_files" "-LAST[<str> Last Growing Day]:file:_files" "-TLH[<str> Tree Line Height]:file:_files" "-DT_MIN[<double> Threshold Temperature]:file:_files" "-SW_MIN[<double> Minimum Soil Water Content (Percent)]:file:_files" "-LGS_MIN[<num> Minimum Length]:file:_files" "-SMT_MIN[<double> Minimum Mean Temperature]:file:_files" "-TLH_MAX_DIFF[<double>Maximum Tree Line Height Difference]:file:_files"  
          ;;
        13)
          _arguments \
            "-BOUNDARY[<str> Boundary Layer]:file:_files" "-WIND[<str> Wind Effect]:file:_files" "-OBSERVED[<str> Observations]:file:_files" "-B_GRID[<str> Calibrated Scaling Factor]:file:_files" "-WINDCORR[<str> Corrected Wind Effect]:file:_files" "-B_SOURCE[<str> Scaling Factor]:file:_files" "-B_CONST[<double> Constant Scaling Factor]:file:_files" "-B_MAX[<double> Maximum Scaling Factor]:file:_files" "-B_STEPS[<num> Number of Steps]:file:_files" "-KERNEL_TYPE[<str>Kernel Type]:file:_files" "-KERNEL_SIZE[<num>Kernel Size]:file:_files"  
          ;;
        14)
          _arguments \
            "-TMIN[<str> Minimum Temperature]:file:_files" "-TMAX[<str> Maximum Temperature]:file:_files" "-FREQUENCY[<str>Frost Change Frequency]:file:_files" "-DT_MEAN[<str> Mean Temperature Span]:file:_files" "-DT_MAX[<str> Maximum Temperature Span]:file:_files" "-DT_STDV[<str> Standard Deviation of Temperature Span]:file:_files" "-TMIN_MEAN[<str>Mean Minimum Temperature]:file:_files" "-TMIN_MIN[<str> Minimum Temperature]:file:_files"  
          ;;
        15)
          _arguments \
            "-GSL[<str> Growing Season Length]:file:_files" "-GST[<str> Mean Temperature]:file:_files" "-FROST[<str> Frost occurence]:file:_files" "-NIVAL_TEMP[<double>Minimum Temperature Nival]:file:_files" "-TREE_TEMP[<double> Minimum Temperature Treeline]:file:_files" "-ATB[<str> Thermal Belts]:file:_files"  
          ;;
        17)
          _arguments \
            "-T[<str> Mean Temperature]:file:_files" "-P[<str> Precipitation]:file:_files" "-DAYS[<str> Snow Cover Days]:file:_files" "-MEAN[<str> Average]:file:_files" "-MAXIMUM[<str> Maximum]:file:_files" "-QUANTILE[<str> Quantile]:file:_files" "-QUANT_VAL[<double>Value]:file:_files" "-TIME[<str> Time]:file:_files" "-MONTH[<str> Month]:file:_files"  
          ;;
        18)
          _arguments \
            "-TMEAN[<str> Mean Monthly Temperatures]:file:_files" "-TBASE[<double> Base Temperature]:file:_files" "-NGDD[<str> Number of Days above Base Temperature]:file:_files" "-TSUM[<str> Growing Degree Days]:file:_files" "-FIRST[<str> First Growing Degree Day]:file:_files" "-LAST[<str> Last Growing Degree Day]:file:_files" "-TARGET[<str> Degree Sum Target Day]:file:_files" "-TTARGET[<double>Degree Sum]:file:_files"  
          ;;
        19)
          _arguments \
            "-T[<str> Temperature]:file:_files" "-P[<str> Precipitation]:file:_files" "-CLASSES[<str>Climate Classification]:file:_files" "-METHOD[<str> Classification]:file:_files"  
          ;;
        20)
          _arguments \
            "-T[<str> Mean Temperature]:file:_files" "-TMIN[<str> Minimum Temperature]:file:_files" "-TMAX[<str> Maximum Temperature]:file:_files" "-P[<str> Precipitation]:file:_files" "-SWC[<str> Soil Water Capacity of Profile]:file:_files" "-SWC_DEFAULT[<double>Default]:file:_files" "-SWC_SURFACE[<double>Top Soil Water Capacity]:file:_files" "-SW1_RESIST[<double> Transpiration Resistance]:file:_files" "-LAT_DEF[<double> Default Latitude]:file:_files" "-SNOW[<str> Snow Depth]:file:_files" "-ETP[<str> Evapotranspiration]:file:_files" "-SW_0[<str> Soil Water (Upper Layer)]:file:_files" "-SW_1[<str> Soil Water (Lower Layer)]:file:_files"  
          ;;
        21)
          _arguments \
            "-WEATHER[<str> Weather Data]:file:_files" "-ATmean[<str> Mean Temperature]:file:_files" "-ATmax[<str> Maximum Temperature]:file:_files" "-SIrel[<str> Solar Irradiation]:file:_files" "-PHENOLOGY[<str> Phenology]:file:_files" "-SUMMARY[<str> Summary]:file:_files" "-LATITUDE[<double> Latitude]:file:_files" "-LIMIT[<str> Limit]:file:_files" "-DToptimum[<double> Developmental Optimum Temperature]:file:_files" "-DTminimum[<double> Developmental Minimum Temperature]:file:_files" "-FAminimum[<double> Minimum Temperature for Flight Activity]:file:_files" "-DayLength[<double> Minimum Day Length for Flight Activity]:file:_files" "-DDminimum[<double> Minimum Thermal Sum for Infestation]:file:_files" "-DDtotal[<double> Thermal Sum for Total Development]:file:_files" "-Risk_DayMax[<double>Day of Maximum Risk after Onset]:file:_files" "-Risk_Decay[<double> Decay of Risk after Maximum]:file:_files" "-YD_Begin[<date> Begin of Parental Development]:file:_files" "-YD_End_Onset[<date> End of Breeding]:file:_files" "-YD_End[<date> End of Development]:file:_files"  
          ;;
        22)
          _arguments \
            "-ATmean[<str> Mean Temperature]:file:_files" "-ATmax[<str> Maximum Temperature]:file:_files" "-SIrel[<str> Solar Irradiation]:file:_files" "-GENERATIONS[<str> Potential Number of Generations]:file:_files" "-ONSET[<str> Onset Day of Infestation]:file:_files" "-ONSET_FILIAL_1[<str>Onset Day, 1. Filial Generation]:file:_files" "-ONSET_SISTER_1[<str>Onset Day, 1. Sister Generation]:file:_files" "-ONSET_FILIAL_2[<str>Onset Day, 2. Filial Generation]:file:_files" "-ONSET_SISTER_2[<str>Onset Day, 2. Sister Generation]:file:_files" "-ONSET_FILIAL_3[<str>Onset Day, 3. Filial Generation]:file:_files" "-ONSET_SISTER_3[<str>Onset Day, 3. Sister Generation]:file:_files" "-BTSUM_FILIAL_1[<str>State, 1. Filial Generation]:file:_files" "-BTSUM_SISTER_1[<str>State, 1. Sister Generation]:file:_files" "-BTSUM_FILIAL_2[<str>State, 2. Filial Generation]:file:_files" "-BTSUM_SISTER_2[<str>State, 2. Sister Generation]:file:_files" "-BTSUM_FILIAL_3[<str>State, 3. Filial Generation]:file:_files" "-BTSUM_SISTER_3[<str>State, 3. Sister Generation]:file:_files" "-LAT_GRID[<str> Latitude]:file:_files" "-LAT_CONST[<double> Latitude]:file:_files" "-DToptimum[<double> Developmental Optimum Temperature]:file:_files" "-DTminimum[<double> Developmental Minimum Temperature]:file:_files" "-FAminimum[<double> Minimum Temperature for Flight Activity]:file:_files" "-DayLength[<double> Minimum Day Length for Flight Activity]:file:_files" "-DDminimum[<double> Minimum Thermal Sum for Infestation]:file:_files" "-DDtotal[<double> Thermal Sum for Total Development]:file:_files" "-Risk_DayMax[<double>Day of Maximum Risk after Onset]:file:_files" "-Risk_Decay[<double> Decay of Risk after Maximum]:file:_files" "-YD_Begin[<date> Begin of Parental Development]:file:_files" "-YD_End_Onset[<date> End of Breeding]:file:_files" "-YD_End[<date> End of Development]:file:_files"  
          ;;
        23)
          _arguments \
            "-ATmean[<str> Mean Temperature]:file:_files" "-ATmax[<str> Maximum Temperature]:file:_files" "-SIrel[<str> Solar Irradiation]:file:_files" "-GENERATIONS[<str> Potential Number of Generations]:file:_files" "-ONSET[<str> Onset Day of Infestation]:file:_files" "-ONSET_FILIAL_1[<str>Onset Day, 1. Filial Generation]:file:_files" "-ONSET_SISTER_1[<str>Onset Day, 1. Sister Generation]:file:_files" "-ONSET_FILIAL_2[<str>Onset Day, 2. Filial Generation]:file:_files" "-ONSET_SISTER_2[<str>Onset Day, 2. Sister Generation]:file:_files" "-ONSET_FILIAL_3[<str>Onset Day, 3. Filial Generation]:file:_files" "-ONSET_SISTER_3[<str>Onset Day, 3. Sister Generation]:file:_files" "-BTSUM_FILIAL_1[<str>State, 1. Filial Generation]:file:_files" "-BTSUM_SISTER_1[<str>State, 1. Sister Generation]:file:_files" "-BTSUM_FILIAL_2[<str>State, 2. Filial Generation]:file:_files" "-BTSUM_SISTER_2[<str>State, 2. Sister Generation]:file:_files" "-BTSUM_FILIAL_3[<str>State, 3. Filial Generation]:file:_files" "-BTSUM_SISTER_3[<str>State, 3. Sister Generation]:file:_files" "-LAT_GRID[<str> Latitude]:file:_files" "-LAT_CONST[<double> Latitude]:file:_files" "-DToptimum[<double> Developmental Optimum Temperature]:file:_files" "-DTminimum[<double> Developmental Minimum Temperature]:file:_files" "-FAminimum[<double> Minimum Temperature for Flight Activity]:file:_files" "-DayLength[<double> Minimum Day Length for Flight Activity]:file:_files" "-DDminimum[<double> Minimum Thermal Sum for Infestation]:file:_files" "-DDtotal[<double> Thermal Sum for Total Development]:file:_files" "-Risk_DayMax[<double>Day of Maximum Risk after Onset]:file:_files" "-Risk_Decay[<double> Decay of Risk after Maximum]:file:_files" "-YD_Begin[<date> Begin of Parental Development]:file:_files" "-YD_End_Onset[<date> End of Breeding]:file:_files" "-YD_End[<date> End of Development]:file:_files" "-ATSUM_EFF[<str> Air Temperature Sum]:file:_files" "-RISK[<str> Risk]:file:_files" "-RESET[<str> Reset]:file:_files" "-DAY[<date> Start Day]:file:_files"  
          ;;
        24)
          _arguments \
            "-TAVG[<str> Mean Temperature]:file:_files" "-TMIN[<str> Minimum Temperature]:file:_files" "-TMAX[<str> Maximum Temperature]:file:_files" "-PSUM[<str> Precipitation]:file:_files" "-SNOW[<str> Snow Depth]:file:_files" "-SW_0[<str> Soil Water (Upper Layer)]:file:_files" "-SW_1[<str> Soil Water (Lower Layer)]:file:_files" "-LAT_GRID[<str> Latitude]:file:_files" "-LAT_CONST[<double> Default Latitude]:file:_files" "-SWC[<str> Soil Water Capacity of Profile]:file:_files" "-SWC_DEFAULT[<double>Default]:file:_files" "-SWC_SURFACE[<double>Top Soil Water Capacity]:file:_files" "-SWT_RESIST[<double> Transpiration Resistance]:file:_files" "-RESET[<str> Reset]:file:_files" "-DAY[<date> Start Day]:file:_files"  
          ;;
        25)
          _arguments \
            "-COVERS[<str> Cloud Fractions]:file:_files" "-HEIGHTS[<str> Heights]:file:_files" "-GROUND[<str> Surface Elevation]:file:_files" "-WIND[<str> Wind effect]:file:_files" "-CBASE[<str> Cloud Base]:file:_files" "-COVER[<str> Total Cloud Cover]:file:_files" "-BLOCKS[<str> Number of Cloud Blocks]:file:_files" "-INTERVAL[<double>Interval]:file:_files" "-MINCOVER[<double>Minimum Cloud Cover Fraction]:file:_files"  
          ;;
        26)
          _arguments \
            "-TEMP[<str> Atmospheric Lapse Rates]:file:_files" "-TGROUND[<str> Surface Temperature]:file:_files" "-LAPSE[<str> Temperature Lapse Rate at Extreme]:file:_files" "-TEXTREME[<str>Daily Extreme Temperature]:file:_files" "-TIME[<str> Hour of Daily Extreme Temperature]:file:_files" "-EXTREME[<str> Temperature extreme]:file:_files"  
          ;;
        27)
          _arguments \
            "-P[<str> Air Pressure]:file:_files" "-P_DEFAULT[<double>Default]:file:_files" "-Z[<str> Air Pressure Elevation]:file:_files" "-Z_DEFAULT[<double>Default]:file:_files" "-T[<str> Temperature]:file:_files" "-T_DEFAULT[<double>Default]:file:_files" "-L[<str> Temperature Lapse Rate]:file:_files" "-L_DEFAULT[<double>Default]:file:_files" "-DEM[<str> Elevation]:file:_files" "-P_ADJ[<str> Adjusted Air Pressure]:file:_files"  
          ;;
        28)
          _arguments \
            "-IRRADIANCE[<str> Global Irradiance]:file:_files" "-IRRADIANCE_DEFAULT[<double>Default]:file:_files" "-ALBEDO[<str> Albedo]:file:_files" "-ALBEDO_DEFAULT[<double> Default]:file:_files" "-EMISSIVITY[<str> Emissivity]:file:_files" "-EMISSIVITY_DEFAULT[<double>Default]:file:_files" "-CONVECTION[<str> Convection Coefficient]:file:_files" "-CONVECTION_DEFAULT[<double>Default]:file:_files" "-T_AIR[<str> Ambient Air Temperature]:file:_files" "-T_AIR_DEFAULT[<double> Default]:file:_files" "-T_SKY[<str> Radiant Sky Temperature]:file:_files" "-T_SKY_DEFAULT[<double> Default]:file:_files" "-T_INITIAL[<str> Initial Temperature Estimation]:file:_files" "-T_INITIAL_DEFAULT[<double> Default]:file:_files" "-LST[<str> Land Surface Temperature]:file:_files" "-UNIT[<str> Unit]:file:_files" "-ITERATIONS[<num> Iterations]:file:_files"  
          ;;
        29)
          _arguments \
            "-T[<str> Temperature]:file:_files" "-T_DEFAULT[<double> Default]:file:_files" "-P[<str> Air Pressure]:file:_files" "-P_DEFAULT[<double> Default]:file:_files" "-IN_VP[<str> Vapor Pressure]:file:_files" "-IN_VP_DEFAULT[<double>Default]:file:_files" "-IN_SH[<str> Specific Humidity]:file:_files" "-IN_SH_DEFAULT[<double>Default]:file:_files" "-IN_RH[<str> Relative Humidity]:file:_files" "-IN_RH_DEFAULT[<double>Default]:file:_files" "-IN_DP[<str> Dew Point]:file:_files" "-IN_DP_DEFAULT[<double>Default]:file:_files" "-OUT_VPSAT[<str> Saturation Pressure]:file:_files" "-OUT_VP[<str> Vapor Pressure]:file:_files" "-OUT_VPDIF[<str> Vapor Pressure Deficit]:file:_files" "-OUT_RH[<str> Relative Humidity]:file:_files" "-OUT_SH[<str> Specific Humidity]:file:_files" "-OUT_DP[<str> Dew Point]:file:_files" "-OUT_DPDIF[<str> Dew Point Difference]:file:_files" "-CONVERSION[<str> Conversion from...]:file:_files" "-VPSAT_METHOD[<str> Saturation Pressure]:file:_files"  
          ;;
        30)
          _arguments \
            "-LORES_DEM[<str> Elevation]:file:_files" "-LORES_T[<str> Temperature]:file:_files" "-LORES_LAPSE[<str> Lapse Rate]:file:_files" "-LORES_SLT[<str> Sea Level Temperature]:file:_files" "-HIRES_DEM[<str> Elevation]:file:_files" "-HIRES_T[<str> Temperature]:file:_files" "-LAPSE_METHOD[<str> Lapse Rate]:file:_files" "-REGRS_SUMMARY[<str> Regression Summary]:file:_files" "-REGRS_LAPSE[<str> Regression]:file:_files" "-LIMIT_LAPSE[<str> Limit Minimum Lapse Rate]:file:_files" "-CONST_LAPSE[<double>Constant Lapse Rate]:file:_files"  
          ;;
        31)
          _arguments \
            "-LATITUDE[<str> Latitude]:file:_files" "-SOLARRAD[<str> Solar Radiation]:file:_files" "-MONTH[<str> Month]:file:_files" "-DAY[<num> Day of Month]:file:_files" "-SUNSHINE[<double>Sunshine Duration]:file:_files"  
          ;;
        32)
          _arguments \
            "-POINTS[<str> Observations]:file:_files" "-DEM[<str> Elevation]:file:_files" "-TEMPERATURE[<str> Temperature]:file:_files" "-SLT[<str> Sea Level Temperature]:file:_files" "-FIELD_T[<str> Temperature]:file:_files" "-FIELD_Z[<str> Elevation]:file:_files" "-INTERPOLATION[<str> Interpolation]:file:_files" "-IDW_POWER[<double> Power]:file:_files" "-LAPSE_METHOD[<str> Lapse Rate]:file:_files" "-REGRS_SUMMARY[<str> Regression Summary]:file:_files" "-REGRS_LAPSE[<str> Regression]:file:_files" "-LIMIT_LAPSE[<str> Limit Minimum Lapse Rate]:file:_files" "-CONST_LAPSE[<double>Constant Lapse Rate]:file:_files"  
          ;;
        temperature_downscaling)
          _arguments \
            "-LORES_DEM[<str> Elevation]:file:_files" "-LORES_LAPSE[<str> Lapse Rate]:file:_files" "-LORES_T[<str> Temperature]:file:_files" "-HIRES_DEM[<str> Elevation]:file:_files" "-HIRES_T[<str> Temperature]:file:_files" "-LAPSE_METHOD[<str> Lapse Rate]:file:_files" "-CONST_LAPSE[<double>Constant or Minimum Lapse Rate]:file:_files" "-REGRS_LAPSE[<str> Regression]:file:_files" "-LIMIT_LAPSE[<str> Limit Minimum Lapse Rate]:file:_files" "-MINIM_LAPSE[<double>Minimum Lapse Rate]:file:_files"  
          ;;
     esac
     ;;
   esac
}

_contrib_perego(){
  _arguments -C "1: :->cmds" "*::arg:->args" 
 
  case "$state" in
    cmds)
      _values "contrib_perego command" \
        "0[Average With Thereshold 1]" \
        "1[Average With Thereshold 2]" \
        "2[Average With Thereshold 3]" \
        "3[Average With Mask 1]" \
        "4[Average With Mask 2]" \
        "5[Destriping]" \
        "6[Destriping with Mask]" \
        "7[Directional Average]" \

      ;;
    args)
      case "$line[1]" in
        0)
          _arguments \
            "-INPUT[<str> Input]:file:_files" "-RESULT[<str> AWT Grid]:file:_files" "-RX[<num> Radius X]:file:_files" "-RY[<num> Radius Y]:file:_files" "-THRESH[<double>Threshold]:file:_files"  
          ;;
        1)
          _arguments \
            "-INPUT[<str> Input]:file:_files" "-RESULT[<str> AWT Grid]:file:_files" "-RX[<num> Radius X]:file:_files" "-RY[<num> Radius Y]:file:_files" "-THRESH[<double>Threshold]:file:_files"  
          ;;
        2)
          _arguments \
            "-INPUT[<str> Input]:file:_files" "-RESULT[<str> AWT Grid]:file:_files" "-RX[<num> Radius X]:file:_files" "-RY[<num> Radius Y]:file:_files" "-THRESH[<double>Threshold]:file:_files"  
          ;;
        3)
          _arguments \
            "-INPUT[<str> Input]:file:_files" "-MASK[<str> Mask Grid]:file:_files" "-RESULT[<str>AWM1 Grid]:file:_files" "-V[<double> Mask value]:file:_files" "-RX[<num> Radius X]:file:_files" "-RY[<num> Radius Y]:file:_files"  
          ;;
        4)
          _arguments \
            "-INPUT[<str> Input]:file:_files" "-MASK[<str> Mask Grid]:file:_files" "-RESULT[<str>AWM2 Grid]:file:_files" "-V[<double> Mask value]:file:_files" "-RX[<num> Radius X]:file:_files" "-RY[<num> Radius Y]:file:_files"  
          ;;
        5)
          _arguments \
            "-INPUT[<str> Grid]:file:_files" "-MASK[<str> Mask]:file:_files" "-RESULT3[<str>Destriped Grid]:file:_files" "-RESULT1[<str>Low-pass 1]:file:_files" "-RESULT2[<str>Low-pass 2]:file:_files" "-STRIPES[<str>Stripes]:file:_files" "-ANG[<double> Angle]:file:_files" "-R[<double> Radius]:file:_files" "-D[<double> Stripes Distance]:file:_files"  
          ;;
        6)
          _arguments \
            "-INPUT[<str> Grid]:file:_files" "-MASK[<str> Mask]:file:_files" "-RESULT3[<str>Destriped Grid]:file:_files" "-RESULT1[<str>Low-pass 1]:file:_files" "-RESULT2[<str>Low-pass 2]:file:_files" "-STRIPES[<str>Stripes]:file:_files" "-ANG[<double> Angle]:file:_files" "-R[<double> Radius]:file:_files" "-D[<double> Stripes Distance]:file:_files" "-MIN[<double> Minimum]:file:_files" "-MAX[<double> Maximum]:file:_files" "-MMIN[<double>Mask Minimum]:file:_files" "-MMAX[<double>Mask Maximum]:file:_files"  
          ;;
        7)
          _arguments \
            "-INPUT[<str> Input]:file:_files" "-RESULT[<str>Output Grid]:file:_files" "-ANG[<double>Angle (in degrees)]:file:_files" "-R1[<double> Main Radius]:file:_files" "-R2[<double> Transversal radius]:file:_files"  
          ;;
     esac
     ;;
   esac
}

_db_odbc(){
  _arguments -C "1: :->cmds" "*::arg:->args" 
 
  case "$state" in
    cmds)
      _values "db_odbc command" \
        "0[Connect to ODBC Source]" \
        "1[Disconnect from ODBC Source]" \
        "2[Commit/Rollback Transaction]" \
        "3[Execute SQL]" \
        "4[List Table Fields]" \
        "5[Import Table]" \
        "6[Export Table]" \
        "7[Drop Table]" \
        "8[Import Table from Query]" \
        "9[List Data Sources]" \
        "10[List Tables]" \
        "11[Disconnect All]" \

      ;;
    args)
      case "$line[1]" in
        0)
          _arguments \
            "-DSN[<str> Data Source]:file:_files" "-USER[<str> User]:file:_files" "-PASSWORD[<str>Password]:file:_files"  
          ;;
        1)
          _arguments \
            "-ODBC_DSN[<str>Data Source]:file:_files" "-ODBC_USR[<str>User]:file:_files" "-ODBC_PWD[<str>Password]:file:_files" "-TRANSACT[<str>Transactions]:file:_files"  
          ;;
        2)
          _arguments \
            "-ODBC_DSN[<str>Data Source]:file:_files" "-ODBC_USR[<str>User]:file:_files" "-ODBC_PWD[<str>Password]:file:_files" "-SOURCE[<str> Source]:file:_files" "-TRANSACT[<str>Transactions]:file:_files"  
          ;;
        3)
          _arguments \
            "-ODBC_DSN[<str>Data Source]:file:_files" "-ODBC_USR[<str>User]:file:_files" "-ODBC_PWD[<str>Password]:file:_files" "-SQL[<str> SQL Statment]:file:_files" "-COMMIT[<str> Commit]:file:_files" "-STOP[<str> Stop on Error]:file:_files"  
          ;;
        4)
          _arguments \
            "-ODBC_DSN[<str>Data Source]:file:_files" "-ODBC_USR[<str>User]:file:_files" "-ODBC_PWD[<str>Password]:file:_files" "-FIELDS[<str> Fields]:file:_files" "-TABLE[<str> Table Source]:file:_files"  
          ;;
        5)
          _arguments \
            "-ODBC_DSN[<str>Data Source]:file:_files" "-ODBC_USR[<str>User]:file:_files" "-ODBC_PWD[<str>Password]:file:_files" "-TABLE[<str> Table]:file:_files" "-SOURCE[<str> Table Source]:file:_files"  
          ;;
        6)
          _arguments \
            "-ODBC_DSN[<str>Data Source]:file:_files" "-ODBC_USR[<str>User]:file:_files" "-ODBC_PWD[<str>Password]:file:_files" "-TABLE[<str> Table]:file:_files" "-TABLE_PK[<str>Primary Key]:file:_files" "-TABLE_NN[<str>Not Null]:file:_files" "-TABLE_UQ[<str>Unique]:file:_files" "-NAME[<str> Table Name]:file:_files" "-EXISTS[<str> If table exists...]:file:_files"  
          ;;
        7)
          _arguments \
            "-ODBC_DSN[<str>Data Source]:file:_files" "-ODBC_USR[<str>User]:file:_files" "-ODBC_PWD[<str>Password]:file:_files" "-TABLE[<str> Table]:file:_files"  
          ;;
        8)
          _arguments \
            "-ODBC_DSN[<str>Data Source]:file:_files" "-ODBC_USR[<str>User]:file:_files" "-ODBC_PWD[<str>Password]:file:_files" "-TABLE[<str> Table]:file:_files" "-TABLES[<str> Tables]:file:_files" "-FIELDS[<str> Fields]:file:_files" "-WHERE[<str> Where]:file:_files" "-GROUP[<str> Group]:file:_files" "-HAVING[<str> Having]:file:_files" "-ORDER[<str> Order]:file:_files" "-DISTINCT[<str>Distinct]:file:_files"  
          ;;
        9)
          _arguments \
            "-SOURCES[<str> Data Sources]:file:_files" "-CONNECTED[<str>Only List Connected Sources]:file:_files"  
          ;;
        10)
          _arguments \
            "-ODBC_DSN[<str>Data Source]:file:_files" "-ODBC_USR[<str>User]:file:_files" "-ODBC_PWD[<str>Password]:file:_files" "-TABLES[<str> Tables]:file:_files"  
          ;;
        11)
          _arguments \
            "-TRANSACT[<str>Transactions]:file:_files"  
          ;;
     esac
     ;;
   esac
}

_db_pgsql(){
  _arguments -C "1: :->cmds" "*::arg:->args" 
 
  case "$state" in
    cmds)
      _values "db_pgsql command" \
        "0[List PostgreSQL Connections]" \
        "1[Connect to PostgreSQL]" \
        "2[Disconnect from PostgreSQL]" \
        "3[Disconnect All]" \
        "4[Begin Transaction]" \
        "5[Commit/Rollback Transaction]" \
        "6[Execute SQL]" \
        "10[List Tables]" \
        "11[List Table Fields]" \
        "12[Import Table]" \
        "13[Export Table]" \
        "14[Drop Table]" \
        "15[Import Table from Query]" \
        "16[Import Table from Query (GUI)]" \
        "20[Import Shapes]" \
        "21[Export Shapes]" \
        "22[Update Shapes SRID]" \
        "23[Import Shapes with Joined Data (GUI)]" \
        "24[Import Shapes with Joined Data]" \
        "30[Import Raster]" \
        "31[Export Raster]" \
        "32[Update Raster SRID]" \
        "33[Import Single Raster Band]" \
        "34[Export Grid Collection]" \
        "35[Create Database]" \
        "36[Drop Database]" \

      ;;
    args)
      case "$line[1]" in
        0)
          _arguments \
            "-CONNECTIONS[<str>Connections]:file:_files"  
          ;;
        1)
          _arguments \
            "-PG_HOST[<str>Host]:file:_files" "-PG_PORT[<num>Port]:file:_files" "-PG_DB[<str> Database]:file:_files" "-PG_USER[<str>User]:file:_files" "-PG_PWD[<str> Password]:file:_files"  
          ;;
