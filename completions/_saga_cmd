_sim_qm_of_esp(){
  _arguments -C "1: :->cmds" "*::arg:->args" 
 
  case "$state" in
    cmds)
      _values "sim_qm_of_esp command" \
        "0[Diffusive Hillslope Evolution (FTCS)]" \
        "1[Fill Sinks (QM of ESP)]" \
        "2[Flow Accumulation (QM of ESP)]" \
        "3[Successive Flow Routing]" \
        "4[Diffusive Hillslope Evolution (ADI)]" \

      ;;
    args)
      case "$line[1]" in
        0)
          _arguments \
            "-DEM[<str> Elevation]:file:_files" "-MODEL[<str> Modelled Elevation]:file:_files" "-DIFF[<str> Elevation Difference]:file:_files" "-UPDATE[<str> Update]:file:_files" "-KAPPA[<double> Diffusivity (m2 / kyr)]:file:_files" "-DURATION[<double>Simulation Time (kyr)]:file:_files" "-TIMESTEP[<str> Time Step]:file:_files" "-DTIME[<double> Time Step (kyr)]:file:_files" "-NEIGHBOURS[<str> Neighbourhood]:file:_files"  
          ;;
        1)
          _arguments \
            "-DEM[<str> DEM]:file:_files" "-FILLED[<str> DEM without Sinks]:file:_files" "-SINKS[<str> Sinks]:file:_files" "-DZFILL[<double>Fill Increment]:file:_files"  
          ;;
        2)
          _arguments \
            "-DEM[<str> DEM]:file:_files" "-FLOW[<str> Contributing Area]:file:_files" "-PREPROC[<str> Preprocessing]:file:_files" "-DZFILL[<double>Fill Increment]:file:_files"  
          ;;
        3)
          _arguments \
            "-DEM[<str> DEM]:file:_files" "-FLOW[<str> Flow]:file:_files" "-ITERATIONS[<num>Iterations]:file:_files" "-RUNOFF[<double> Runoff]:file:_files" "-MANNING[<double>Manning's Roughness]:file:_files"  
