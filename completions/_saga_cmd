            "-POLAR[<str> Polar Coordinates]:file:_files" "-F_EXAGG[<str> Exaggeration]:file:_files" "-D_EXAGG[<double>Exaggeration Factor]:file:_files" "-CARTES[<str> Cartesion Coordinates]:file:_files" "-RADIUS[<double> Radius]:file:_files" "-DEGREE[<str> Degree]:file:_files"  
