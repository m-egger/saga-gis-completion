            "-INPUT[<str> Gelaendemodell (DTM)]:file:_files" "-INPUT2[<str> Hoehengradienten (Grad)]:file:_files" "-INPUT10[<str> Flusseinzugsgebiet (BasinShare)]:file:_files" "-INPUT8[<str> HG-Raster (HGGrid)]:file:_files" "-INPUT9[<str> HG-Hoehengradienten (HGGrad)]:file:_files" "-INPUT11[<str> Referenzverlauf Flussbilanz]:file:_files" "-INPUT12[<str> Zuflussrasterzellen (NumInFlowCells)]:file:_files" "-INPUT3[<str> statische Wassserentnahme (statWUse)]:file:_files" "-INPUT5[<str> Fluss-Speicher einlesen]:file:_files" "-INPUT6[<str> Oberflaechenabfluss-Speicher einlesen]:file:_files" "-INPUT7[<str> Grundwasserabfluss-Speicher einlesen]:file:_files" "-OUTPUT[<str> Fluss-Speicher ausgeben]:file:_files" "-OUTPUT2[<str> Oberflaechenabfluss-Speicher ausgeben]:file:_files" "-OUTPUT3[<str> Grundwasserabfluss-Speicher ausgeben]:file:_files" "-OUTPUT4[<str> Wasserflussvolumen]:file:_files" "-OUTPUT5[<str> SumRunoffDrainage]:file:_files" "-OUTPUT6[<str> DynWUse]:file:_files" "-onlyRB[<str> Berechnung nur im Flusseinzugsgebiet]:file:_files" "-TimeStep[<num> Zeitschrittdauer (s)]:file:_files" "-CalcT[<str> automatisierter max. Zeitschritt]:file:_files" "-sYear[<num> Startjahr]:file:_files" "-DayNum[<num> Anzahl der Simulationstage]:file:_files" "-Folder2[<str> Speicherordner]:file:_files" "-Folder1[<str> Pfad LS-Daten]:file:_files" "-stRedFacR[<double>Reduzierungsfaktor Surface Runoff (% / 100)]:file:_files" "-autoFacD[<str> Ausgleich der Surface Runoff Reduktion]:file:_files" "-stRedFacD[<double>Reduzierungsfaktor Drainage (% / 100)]:file:_files" "-OffsetR[<double> Offsetwert Surface Runoff (m3/s)]:file:_files" "-OffsetD[<double> Offsetwert Drainage (m3/s)]:file:_files" "-CacheUse[<str> Initiierung mit vorhandenen Speichern]:file:_files" "-Folder4[<str> NCache einlesen]:file:_files" "-wNC[<str> Ausgabe NCache]:file:_files" "-ParamC[<double> Gerinne-Parameter cG]:file:_files" "-ParamG[<double> Oberflaechen-Parameter cO]:file:_files" "-ParamB[<double> Grundwasser-Parameter pB]