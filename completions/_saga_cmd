_docs_html(){
  _arguments -C "1: :->cmds" "*::arg:->args" 
 
  case "$state" in
    cmds)
      _values "docs_html command" \
        "1[SVG Interactive Map]" \

      ;;
    args)
      case "$line[1]" in
        1)
          _arguments \
            "-FILENAME[<str>(FLD) SVG File]:file:_files" "-LIST[<str> Shapes Layers]:file:_files" "-SHAPES[<str> (FLD) Index Layer]:file:_files"  
          ;;
     esac
     ;;
   esac
}

