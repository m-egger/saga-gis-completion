            "-SHAPES[<str> Shapes]:file:_files" "-ATTRIBUTE[<str> Attribute]:file:_files" "-METHOD[<str> Expression Type]:file:_files" "-EXPRESSION[<str>Expression]:file:_files" "-COMPARE[<str> Select if...]:file:_files" "-CASE[<str> Case Sensitive]:file:_files"  
          ;;
     esac
     ;;
   esac
}

_shapes_transect(){
  _arguments -C "1: :->cmds" "*::arg:->args" 
 
  case "$state" in
    cmds)
      _values "shapes_transect command" \
        "0[Transect through polygon shapefile]" \

      ;;
    args)
      case "$line[1]" in
        0)
          _arguments \
            "-TRANSECT[<str> Line Transect(s)]:file:_files" "-THEME[<str> Theme]:file:_files" "-THEME_FIELD[<str> Theme Field]:file:_files" "-TRANSECT_RESULT[<str>Result table]:file:_files"  
          ;;
     esac
     ;;
   esac
}

_sim_air_flow(){
  _arguments -C "1: :->cmds" "*::arg:->args" 
 
  case "$state" in
    cmds)
      _values "sim_air_flow command" \
        "0[Cold Air Flow]" \

      ;;
    args)
      case "$line[1]" in
        0)
          _arguments \
