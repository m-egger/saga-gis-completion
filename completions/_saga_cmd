            "-POINTS[<str> Point Cloud]:file:_files" "-T[<str> gps-time]:file:_files" "-r[<str> number of the return]:file:_files" "-n[<str> number of returns of given pulse]:file:_files" "-i[<str> intensity]:file:_files" "-c[<str> classification]:file:_files" "-sCH[<str> scanner channel]:file:_files" "-R[<str> red channel color]:file:_files" "-G[<str> green channel color]:file:_files" "-B[<str> blue channel color]:file:_files" "-C[<str> SAGA RGB color]:file:_files" "-NIR[<str> near infrared]:file:_files" "-a[<str> scan angle]:file:_files" "-d[<str> direction of scan flag]:file:_files" "-e[<str> edge of flight line flag]:file:_files" "-u[<str> user data]:file:_files" "-p[<str> point source ID]:file:_files" "-FILE[<str> Output File]:file:_files" "-FILE_FORMAT[<str>File Format]:file:_files" "-FORMAT[<str> Point Data Record Format]:file:_files" "-RGB_RANGE[<str> Input R,G,B (and NIR) Value Range]:file:_files" "-OFF_X[<double> Offset X]:file:_files" "-OFF_Y[<double> Offset Y]:file:_files" "-OFF_Z[<double> Offset Z]:file:_files" "-SCALE_X[<double> Scale X]:file:_files" "-SCALE_Y[<double> Scale Y]:file:_files" "-SCALE_Z[<double> Scale Z]:file:_files"  
