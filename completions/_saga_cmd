            "-CATEGORIES[<str> Categories]:file:_files" "-COUNT[<str> Number of Categories]:file:_files" "-INDEX[<str> Simpson Index]:file:_files" "-KERNEL_TYPE[<str> Kernel Type]:file:_files" "-KERNEL_RADIUS[<num>Radius]:file:_files"  
          ;;
        24)
