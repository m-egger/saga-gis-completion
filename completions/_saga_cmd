     ;;
   esac
}

_imagery_vigra(){
  _arguments -C "1: :->cmds" "*::arg:->args" 
 
  case "$state" in
    cmds)
      _values "imagery_vigra command" \
        "Library:ViGrAdeprecated[]" \
        "0[Smoothing (ViGrA)]" \
        "1[Edge Detection (ViGrA)]" \
        "2[Morphological Filter (ViGrA)]" \
        "3[Distance (ViGrA)]" \
        "4[Watershed Segmentation (ViGrA)]" \
        "5[Fourier Transform (ViGrA)]" \
        "6[Fourier Transform Inverse (ViGrA)]" \
        "8[Fourier Filter (ViGrA)]" \
        "9[Random Forest Classification (ViGrA)]" \
        "10[Random Forest Presence Prediction (ViGrA)]" \
        "11[Random Forest Table Classification (ViGrA)]" \

      ;;
    args)
      case "$line[1]" in
        Library:ViGrAdeprecated)
          _arguments \
             
          ;;
        0)
          _arguments \
            "-INPUT[<str> Input]:file:_files" "-OUTPUT[<str> Output]:file:_files" "-TYPE[<str> Type of smoothing]:file:_files" "-SCALE[<double>Size of smoothing filter]:file:_files" "-EDGE[<double> Edge threshold for nonlinear smoothing]:file:_files"  
          ;;
        1)
          _arguments \
            "-INPUT[<str> Input]:file:_files" "-OUTPUT[<str> Edges]:file:_files" "-TYPE[<str> Detector type]:file:_files" "-SCALE[<double> Operator scale]:file:_files" "-THRESHOLD[<double>Gradient threshold]:file:_files"  
          ;;
        2)
          _arguments \
            "-INPUT[<str> Input]:file:_files" "-OUTPUT[<str> Output]:file:_files" "-TYPE[<str> Operation]:file:_files" "-RADIUS[<num> Radius (cells)]:file:_files" "-RANK[<double>User defined rank]:file:_files" "-RESCALE[<str>Rescale Values (0-255)]:file:_files"  
          ;;
        3)
          _arguments \
            "-INPUT[<str> Features]:file:_files" "-OUTPUT[<str>Distance]:file:_files" "-NORM[<str> Type of distance calculation]:file:_files"  
          ;;
        4)
          _arguments \
            "-INPUT[<str> Input]:file:_files" "-OUTPUT[<str> Segmentation]:file:_files" "-SCALE[<double>Width of gradient filter]:file:_files" "-RGB[<str> RGB coded data]:file:_files" "-EDGES[<str> Edges]:file:_files"  
          ;;
        5)
          _arguments \
            "-INPUT[<str> Input]:file:_files" "-REAL[<str> Real]:file:_files" "-IMAG[<str> Imaginary]:file:_files" "-CENTER[<str>Centered]:file:_files"  
          ;;
        6)
          _arguments \
            "-REAL[<str> Real]:file:_files" "-IMAG[<str> Imaginary]:file:_files" "-OUTPUT[<str>Output]:file:_files" "-CENTER[<str>Centered]:file:_files"  
          ;;
        8)
          _arguments \
            "-INPUT[<str> Input]:file:_files" "-OUTPUT[<str> Output]:file:_files" "-SCALE[<double> Size of smoothing filter]:file:_files" "-POWER[<double> Power]:file:_files" "-RANGE_MIN[<double>Range]:file:_files" "-RANGE_MAX[<double>Range]:file:_files" "-FILTER[<str> Filter]:file:_files"  
          ;;
        9)
          _arguments \
            "-FEATURES[<str> Features]:file:_files" "-CLASSES[<str> Random Forest Classification]:file:_files" "-PROBABILITY[<str> Prediction Probability]:file:_files" "-BPROBABILITIES[<str> Feature Probabilities]:file:_files" "-PROBABILITIES[<str> Feature Probabilities]:file:_files" "-IMPORTANCES[<str> Feature Importances]:file:_files" "-TRAINING[<str> Training Areas]:file:_files" "-FIELD[<str> Label Field]:file:_files" "-LABEL_AS_ID[<str> Use Label as Identifier]:file:_files" "-RF_TREE_COUNT[<num> Tree Count]:file:_files" "-RF_TREE_SAMPLES[<double>Samples per Tree]:file:_files" "-RF_REPLACE[<str> Sample with Replacement]:file:_files" "-RF_SPLIT_MIN_SIZE[<num> Minimum Node Split Size]:file:_files" "-RF_NODE_FEATURES[<str> Features per Node]:file:_files" "-RF_STRATIFICATION[<str> Stratification]:file:_files"  
          ;;
        10)
          _arguments \
            "-FEATURES[<str> Features]:file:_files" "-PREDICTION[<str> Presence Prediction]:file:_files" "-PROBABILITY[<str> Presence Probability]:file:_files" "-PRESENCE[<str> Presence Data]:file:_files" "-BACKGROUND[<double> Background Sample Density (Percent)]:file:_files" "-RF_TREE_COUNT[<num> Tree Count]:file:_files" "-RF_TREE_SAMPLES[<double>Samples per Tree]:file:_files" "-RF_REPLACE[<str> Sample with Replacement]:file:_files" "-RF_SPLIT_MIN_SIZE[<num> Minimum Node Split Size]:file:_files" "-RF_NODE_FEATURES[<str> Features per Node]:file:_files" "-RF_STRATIFICATION[<str> Stratification]:file:_files"  
          ;;
        11)
          _arguments \
            "-TABLE[<str> Table]:file:_files" "-FEATURES[<str> Features]:file:_files" "-PREDICTION[<str> Prediction]:file:_files" "-TRAINING[<str> Training]:file:_files" "-LABEL_AS_ID[<str> Use Label as Identifier]:file:_files" "-IMPORTANCES[<str> Feature Importances]:file:_files" "-RF_TREE_COUNT[<num> Tree Count]:file:_files" "-RF_TREE_SAMPLES[<double>Samples per Tree]:file:_files" "-RF_REPLACE[<str> Sample with Replacement]:file:_files" "-RF_SPLIT_MIN_SIZE[<num> Minimum Node Split Size]:file:_files" "-RF_NODE_FEATURES[<str> Features per Node]:file:_files" "-RF_STRATIFICATION[<str> Stratification]:file:_files"  
          ;;
     esac
     ;;
   esac
}

