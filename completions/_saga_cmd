_pointcloud_tools(){
  _arguments -C "1: :->cmds" "*::arg:->args" 
 
  case "$state" in
    cmds)
      _values "pointcloud_tools command" \
        "0[Point Cloud Cutter]" \
        "2[Point Cloud from Grid Points]" \
        "3[Point Cloud from Shapes]" \
        "4[Point Cloud to Grid]" \
        "5[Point Cloud to Shapes]" \
        "6[Point Cloud Reclassifier / Subset Extractor]" \
        "7[Drop Point Cloud Attributes]" \
        "8[Transform Point Cloud]" \
        "9[Point Cloud Thinning (Simple)]" \
        "10[Point Cloud Attribute Calculator]" \
        "11[Cluster Analysis for Point Clouds]" \
        "12[Merge Point Clouds]" \
        "13[Point Cloud from Table]" \
        "14[Select Point Cloud from List]" \
        "15[Ground Classification]" \
        "16[Isolated Points Filter]" \

      ;;
    args)
      case "$line[1]" in
        0)
          _arguments \
            "-POINTS[<str> Points]:file:_files" "-CUT[<str> Cut]:file:_files" "-AREA[<str> Choose Cut from ...]:file:_files" "-INVERSE[<str> Inverse]:file:_files" "-XMIN[<double> Left]:file:_files" "-XMAX[<double> Right]:file:_files" "-YMIN[<double> Bottom]:file:_files" "-YMAX[<double> Top]:file:_files" "-GRID_D[<double>Cell Size]:file:_files" "-GRID_X[<double>Lower Left Center Cell X-Coordinate]:file:_files" "-GRID_Y[<double>Lower Left Center Cell Y-Coordinate]:file:_files" "-GRID_NX[<num> Number of Columns]:file:_files" "-GRID_NY[<num> Number of Rows]:file:_files" "-GRID_FILE[<str>Grid File]:file:_files" "-EXTENT[<str> Shapes Extent]:file:_files" "-POLYGONS[<str> Polygons]:file:_files"  
          ;;
        2)
          _arguments \
            "-GRID[<str> Z Value]:file:_files" "-GRIDS[<str> Additional Values]:file:_files" "-POINTS[<str>Points]:file:_files"  
          ;;
        3)
          _arguments \
            "-SHAPES[<str>Shapes]:file:_files" "-ZFIELD[<str>Z Value]:file:_files" "-POINTS[<str>Points]:file:_files" "-OUTPUT[<str>Output]:file:_files"  
          ;;
        4)
          _arguments \
            "-POINTS[<str> Points]:file:_files" "-GRID[<str> Z Value]:file:_files" "-COUNT[<str> Number of Points in Cell]:file:_files" "-GRIDS[<str> Attributes]:file:_files" "-OUTPUT[<str> Output]:file:_files" "-AGGREGATION[<str>Aggregation]:file:_files" "-CELLSIZE[<double>Cellsize]:file:_files"  
          ;;
        5)
          _arguments \
            "-POINTS[<str>Points]:file:_files" "-SHAPES[<str>Shapes]:file:_files"  
          ;;
        6)
          _arguments \
            "-INPUT[<str> Point Cloud]:file:_files" "-ATTRIB[<str> Attribute]:file:_files" "-RESULT[<str> Result]:file:_files" "-MODE[<str> Mode of Operation]:file:_files" "-CREATE_ATTRIB[<str>Create new Attribute]:file:_files" "-METHOD[<str> Selection Method]:file:_files" "-OLD[<double> Value]:file:_files" "-NEW[<double> New Value]:file:_files" "-SOPERATOR[<str> Operator]:file:_files" "-MIN[<double> Minimum Value]:file:_files" "-MAX[<double> Maximum Value]:file:_files" "-RNEW[<double> New Value]:file:_files" "-ROPERATOR[<str> Operator]:file:_files" "-RETAB[<str> Lookup Table]:file:_files" "-TOPERATOR[<str> Operator]:file:_files" "-RETAB_2[<str> Lookup Table]:file:_files" "-F_MIN[<str> Minimum Value]:file:_files" "-F_MAX[<str> Maximum Value]:file:_files" "-F_CODE[<str> New Value]:file:_files" "-NODATAOPT[<str> No-Data Values]:file:_files" "-NODATA[<double> New Value]:file:_files" "-OTHEROPT[<str> Other Values]:file:_files" "-OTHERS[<double> New Value]:file:_files"  
          ;;
        7)
          _arguments \
            "-INPUT[<str> Input]:file:_files" "-FIELDS[<str>Attributes]:file:_files" "-OUTPUT[<str>Output]:file:_files"  
          ;;
        8)
          _arguments \
            "-IN[<str> Input]:file:_files" "-OUT[<str> Output]:file:_files" "-DX[<double> dX]:file:_files" "-DY[<double> dY]:file:_files" "-DZ[<double> dZ]:file:_files" "-ANGLEX[<double> Angle X]:file:_files" "-ANGLEY[<double> Angle Y]:file:_files" "-ANGLEZ[<double> Angle Z]:file:_files" "-SCALEX[<double> Scale Factor X]:file:_files" "-SCALEY[<double> Scale Factor Y]:file:_files" "-SCALEZ[<double> Scale Factor Z]:file:_files" "-ANCHORX[<double>X]:file:_files" "-ANCHORY[<double>Y]:file:_files" "-ANCHORZ[<double>Z]:file:_files"  
          ;;
        9)
          _arguments \
            "-INPUT[<str> Points]:file:_files" "-RESULT[<str> Thinned Points]:file:_files" "-PERCENT[<double>Percent]:file:_files"  
          ;;
        10)
          _arguments \
            "-PC_IN[<str> Point Cloud]:file:_files" "-PC_OUT[<str> Result]:file:_files" "-FORMULA[<str> Formula]:file:_files" "-NAME[<str> Output Field Name]:file:_files" "-FNAME[<str> Take Formula]:file:_files" "-TYPE[<str> Data Type]:file:_files" "-USE_NODATA[<str>Use NoData]:file:_files"  
          ;;
        11)
          _arguments \
            "-PC_IN[<str> Point Cloud]:file:_files" "-FIELDS[<str> Attributes]:file:_files" "-PC_OUT[<str> Result]:file:_files" "-STATISTICS[<str>Statistics]:file:_files" "-METHOD[<str> Method]:file:_files" "-NCLUSTER[<num> Clusters]:file:_files" "-NORMALISE[<str> Normalise]:file:_files"  
          ;;
        12)
          _arguments \
            "-PC_LAYERS[<str> Point Clouds]:file:_files" "-PC_OUT[<str> Merged Point Cloud]:file:_files" "-DEL_LAYERS[<str> Delete Input]:file:_files" "-ADD_IDENTIFIER[<str>Add Input Identifier]:file:_files" "-START_VALUE[<num> Start Value]:file:_files"  
          ;;
        13)
          _arguments \
            "-TAB_IN[<str> Table]:file:_files" "-FIELD_X[<str>X]:file:_files" "-FIELD_Y[<str>Y]:file:_files" "-FIELD_Z[<str>Z]:file:_files" "-FIELDS[<str> Attributes]:file:_files" "-PC_OUT[<str> Point Cloud]:file:_files"  
          ;;
        14)
          _arguments \
            "-PC_LIST[<str>Point Cloud List]:file:_files" "-PC[<str> Point Cloud]:file:_files" "-INDEX[<num> Index]:file:_files"  
          ;;
        15)
          _arguments \
            "-PC_IN[<str> Point Cloud]:file:_files" "-PC_OUT[<str> Point Cloud Classified]:file:_files" "-RADIUS[<double> Filter Radius]:file:_files" "-TERRAINSLOPE[<double>Terrain Slope (%)]:file:_files" "-FILTERMOD[<str> Filter Modification]:file:_files" "-STDDEV[<double> Standard Deviation]:file:_files"  
          ;;
        16)
          _arguments \
            "-PC_IN[<str> Points]:file:_files" "-PC_OUT[<str> Filtered Points]:file:_files" "-RADIUS[<double> Filter Radius]:file:_files" "-MAX_POINTS[<num>Maximum Number of Points]:file:_files" "-METHOD[<str> Method]:file:_files"  
          ;;
     esac
     ;;
   esac
}

_shapes_grid(){
  _arguments -C "1: :->cmds" "*::arg:->args" 
 
  case "$state" in
    cmds)
      _values "shapes_grid command" \
        "0[Add Grid Values to Points]" \
        "1[Add Grid Values to Shapes]" \
        "2[Grid Statistics for Polygons]" \
        "3[Grid Cells to Points/Polygons]" \
        "4[Grid Values to Points (randomly)]" \
        "5[Contour Lines from Grid]" \
        "6[Vectorizing Grid Classes]" \
        "7[Clip Grid with Polygon]" \
        "8[Grid Statistics for Points]" \
        "9[Local Minima and Maxima]" \
        "10[Grid System Extent]" \
        "11[Clip Grid with Rectangle]" \
        "15[Gradient Vectors from Surface]" \
        "16[Gradient Vectors from Direction and Length]" \
        "17[Gradient Vectors from Directional Components]" \
        "18[Grid Classes Area for Polygons]" \
        "19[Boundary Cells to Polygons]" \
        "20[Line Direction]" \
        "grid_and_polygon_to_points[Grid Values and Polygon Attributes to Points]" \

      ;;
    args)
      case "$line[1]" in
        0)
          _arguments \
            "-SHAPES[<str> Points]:file:_files" "-GRIDS[<str> Grids]:file:_files" "-RESULT[<str> Result]:file:_files" "-RESAMPLING[<str>Resampling]:file:_files"  
          ;;
        1)
          _arguments \
            "-SHAPES[<str> Shapes]:file:_files" "-GRIDS[<str> Grids]:file:_files" "-RESULT[<str> Result]:file:_files" "-RESAMPLING[<str>Resampling]:file:_files"  
          ;;
        2)
          _arguments \
            "-GRIDS[<str> Grids]:file:_files" "-POLYGONS[<str> Polygons]:file:_files" "-NAMING[<str> Field Naming]:file:_files" "-METHOD[<str> Method]:file:_files" "-PARALLELIZED[<str>Use Multiple Cores]:file:_files" "-RESULT[<str> Statistics]:file:_files" "-COUNT[<str> Number of Cells]:file:_files" "-MIN[<str> Minimum]:file:_files" "-MAX[<str> Maximum]:file:_files" "-RANGE[<str> Range]:file:_files" "-SUM[<str> Sum]:file:_files" "-MEAN[<str> Mean]:file:_files" "-VAR[<str> Variance]:file:_files" "-STDDEV[<str> Standard Deviation]:file:_files" "-GINI[<str> Gini]:file:_files" "-QUANTILES[<str> Percentiles]:file:_files"  
          ;;
        3)
          _arguments \
            "-GRIDS[<str> Grids]:file:_files" "-POLYGONS[<str> Polygons]:file:_files" "-ATTRIBUTE[<str>Attribute]:file:_files" "-POINTS[<str> Points]:file:_files" "-CELLS[<str> Points]:file:_files" "-NODATA[<str> No-Data Cells]:file:_files" "-TYPE[<str> Type]:file:_files"  
          ;;
        4)
          _arguments \
            "-GRID[<str> Grid]:file:_files" "-FREQ[<num> Frequency]:file:_files" "-POINTS[<str>Points]:file:_files"  
          ;;
        5)
          _arguments \
            "-GRID[<str> Grid]:file:_files" "-CONTOUR[<str> Contour]:file:_files" "-VERTEX[<str> Vertex Type]:file:_files" "-LINE_OMP[<str> Parallel Processing]:file:_files" "-LINE_PARTS[<str> Split Line Parts]:file:_files" "-POLYGONS[<str> Polygons]:file:_files" "-POLY_OMP[<str> Parallel Processing]:file:_files" "-POLY_PARTS[<str> Split Polygon Parts]:file:_files" "-PRECISION[<str> Coordinate Precision Fix]:file:_files" "-SCALE[<double> Interpolation Scale]:file:_files" "-BOUNDARY[<str> Boundary Extension]:file:_files" "-MINLENGTH[<double>Minimum Length]:file:_files" "-INTERVALS[<str> Interval Definition]:file:_files" "-ZMIN[<double> Base Contour Value]:file:_files" "-ZMAX[<double> Maximum Contour Value]:file:_files" "-ZSTEP[<double> Contour Interval]:file:_files" "-ZLIST[<str> Contour Values]:file:_files"  
          ;;
        6)
          _arguments \
            "-GRID[<str> Grid]:file:_files" "-POLYGONS[<str> Polygons]:file:_files" "-CLASS_ALL[<str> Class Selection]:file:_files" "-CLASS_ID[<double>Class Identifier]:file:_files" "-SPLIT[<str> Vectorised class as...]:file:_files" "-ALLVERTICES[<str>Keep Vertices on Straight Lines]:file:_files"  
          ;;
        7)
          _arguments \
            "-INPUT[<str> Input]:file:_files" "-OUTPUT[<str> Output]:file:_files" "-POLYGONS[<str>Polygons]:file:_files" "-EXTENT[<str> Target Extent]:file:_files"  
          ;;
        8)
          _arguments \
            "-GRIDS[<str> Grids]:file:_files" "-POINTS[<str> Points]:file:_files" "-KERNEL_TYPE[<str>Kernel Type]:file:_files" "-KERNEL_SIZE[<num>Kernel Size]:file:_files" "-NAMING[<str> Field Naming]:file:_files" "-RESULT[<str> Statistics]:file:_files" "-COUNT[<str> Number of Cells]:file:_files" "-MIN[<str> Minimum]:file:_files" "-MAX[<str> Maximum]:file:_files" "-RANGE[<str> Range]:file:_files" "-SUM[<str> Sum]:file:_files" "-MEAN[<str> Mean]:file:_files" "-VAR[<str> Variance]:file:_files" "-STDDEV[<str> Standard Deviation]:file:_files" "-QUANTILE[<num> Percentile]:file:_files"  
          ;;
        9)
          _arguments \
            "-GRID[<str> Grid]:file:_files" "-MINIMA[<str> Minima]:file:_files" "-MAXIMA[<str> Maxima]:file:_files" "-IDENTITY[<str>Identical Values]:file:_files" "-ABSOLUTE[<str>Absolute]:file:_files" "-BOUNDARY[<str>Boundary Cells]:file:_files"  
          ;;
        10)
          _arguments \
            "-GRID_SYSTEM_D[<double>Cell Size]:file:_files" "-GRID_SYSTEM_X[<double>Lower Left Center Cell X-Coordinate]:file:_files" "-GRID_SYSTEM_Y[<double>Lower Left Center Cell Y-Coordinate]:file:_files" "-GRID_SYSTEM_NX[<num> Number of Columns]:file:_files" "-GRID_SYSTEM_NY[<num> Number of Rows]:file:_files" "-GRID_SYSTEM_FILE[<str>Grid File]:file:_files" "-EXTENT[<str> Extent]:file:_files" "-BORDER[<str> Border]:file:_files"  
          ;;
        11)
          _arguments \
            "-INPUT[<str> Input]:file:_files" "-SHAPES[<str>Extent]:file:_files" "-BORDER[<str>Border]:file:_files" "-OUTPUT[<str>Output]:file:_files"  
          ;;
        15)
          _arguments \
            "-SURFACE[<str> Surface]:file:_files" "-VECTORS[<str> Gradient Vectors]:file:_files" "-STEP[<num> Step]:file:_files" "-SIZE_MIN[<double>Size Range]:file:_files" "-SIZE_MAX[<double>Size Range]:file:_files" "-AGGR[<str> Aggregation]:file:_files" "-STYLE[<str> Style]:file:_files"  
          ;;
        16)
          _arguments \
            "-DIR[<str> Direction]:file:_files" "-LEN[<str> Length]:file:_files" "-VECTORS[<str> Gradient Vectors]:file:_files" "-STEP[<num> Step]:file:_files" "-SIZE_MIN[<double>Size Range]:file:_files" "-SIZE_MAX[<double>Size Range]:file:_files" "-AGGR[<str> Aggregation]:file:_files" "-STYLE[<str> Style]:file:_files"  
          ;;
        17)
          _arguments \
            "-X[<str> X Component]:file:_files" "-Y[<str> Y Component]:file:_files" "-VECTORS[<str> Gradient Vectors]:file:_files" "-STEP[<num> Step]:file:_files" "-SIZE_MIN[<double>Size Range]:file:_files" "-SIZE_MAX[<double>Size Range]:file:_files" "-AGGR[<str> Aggregation]:file:_files" "-STYLE[<str> Style]:file:_files"  
          ;;
        18)
          _arguments \
            "-POLYGONS[<str> Polygons]:file:_files" "-RESULT[<str> Result]:file:_files" "-GRID[<str> Grid]:file:_files" "-PROCESS[<str> Processing Order]:file:_files" "-METHOD[<str> Cell Area Intersection]:file:_files" "-OUTPUT[<str> Output Measurment]:file:_files" "-GRID_VALUES[<str> Class Definition]:file:_files" "-GRID_LUT[<str> Look-up Table]:file:_files" "-GRID_LUT_MIN[<str>Value]:file:_files" "-GRID_LUT_MAX[<str>Value (Maximum)]:file:_files" "-GRID_LUT_NAM[<str>Name]:file:_files"  
          ;;
        19)
          _arguments \
            "-GRID[<str> Grid]:file:_files" "-POLYGONS[<str> Polygons]:file:_files" "-BOUNDARY_CELLS[<str> Boundary Cells]:file:_files" "-BOUNDARY_VALUE[<double>Value]:file:_files" "-ALLVERTICES[<str> Keep Vertices on Straight Lines]:file:_files"  
          ;;
        20)
          _arguments \
            "-INPUT[<str> Shapes]:file:_files" "-ORDER_FIELD[<str> Order Field]:file:_files" "-TARGET_DEFINITION[<str> Target Grid System]:file:_files" "-TARGET_USER_SIZE[<double>Cellsize]:file:_files" "-TARGET_USER_XMIN[<double>West]:file:_files" "-TARGET_USER_XMAX[<double>East]:file:_files" "-TARGET_USER_YMIN[<double>South]:file:_files" "-TARGET_USER_YMAX[<double>North]:file:_files" "-TARGET_USER_COLS[<num> Columns]:file:_files" "-TARGET_USER_ROWS[<num> Rows]:file:_files" "-TARGET_USER_FLAT[<str> Rounding]:file:_files" "-TARGET_USER_FITS[<str> Fit]:file:_files" "-TARGET_TEMPLATE[<str> Target System]:file:_files" "-GRID[<str> Grid]:file:_files"  
          ;;
        grid_and_polygon_to_points)
          _arguments \
            "-GRID[<str> Grid]:file:_files" "-POLYGONS[<str> Polygons]:file:_files" "-ATTRIBUTE[<str>Attribute]:file:_files" "-POINTS[<str> Points Table]:file:_files"  
          ;;
     esac
     ;;
   esac
}

_shapes_lines(){
  _arguments -C "1: :->cmds" "*::arg:->args" 
 
  case "$state" in
    cmds)
      _values "shapes_lines command" \
        "0[Convert Polygons to Lines]" \
        "1[Convert Points to Line(s)]" \
        "2[Line Properties]" \
        "3[Line-Polygon Intersection]" \
        "4[Line Simplification]" \
        "5[Line Dissolve]" \
        "6[Split Lines with Lines]" \
        "7[Line Smoothing]" \
        "8[Split Lines at Points]" \
        "9[Line Crossings]" \
        "10[Extract Closed Lines]" \
        "11[Split Lines]" \
        "12[Line Parts to Separate Lines]" \
        "13[Flip Line Direction]" \
        "14[Merge Line Parts to Lines]" \
        "15[Line Density]" \
        "16[Topologize Polylines]" \
        "17[Upstream Edges]" \

      ;;
    args)
      case "$line[1]" in
        0)
          _arguments \
            "-POLYGONS[<str>Polygons]:file:_files" "-LINES[<str> Lines]:file:_files"  
          ;;
        1)
          _arguments \
            "-POINTS[<str> Points]:file:_files" "-LINES[<str> Lines]:file:_files" "-ORDER[<str> Order by...]:file:_files" "-SEPARATE[<str> Separate by...]:file:_files" "-ELEVATION[<str> Elevation]:file:_files" "-MEASURE[<str> Measure]:file:_files" "-MAXDIST[<double>Maximum Distance]:file:_files"  
