          ;;
        8)
          _arguments \
            "-POINTS[<str> Points]:file:_files" "-Z_FIELD[<str> Z]:file:_files" "-Z_SCALE[<double> Z Factor]:file:_files" "-V_FIELD[<str> Value]:file:_files" "-TARGET_DEFINITION[<str> Target Grid System]:file:_files" "-TARGET_USER_SIZE[<double> Cellsize]:file:_files" "-TARGET_USER_XMIN[<double> West]:file:_files" "-TARGET_USER_XMAX[<double> East]:file:_files" "-TARGET_USER_YMIN[<double> South]:file:_files" "-TARGET_USER_YMAX[<double> North]:file:_files" "-TARGET_USER_COLS[<num> Columns]:file:_files" "-TARGET_USER_ROWS[<num> Rows]:file:_files" "-TARGET_USER_FLAT[<str> Rounding]:file:_files" "-TARGET_USER_FITS[<str> Fit]:file:_files" "-TARGET_TEMPLATE[<str> Target System]:file:_files" "-GRIDS[<str> Grid Collection]:file:_files" "-TARGET_USER_ZSIZE[<double>Cellsize]:file:_files" "-TARGET_USER_ZMIN[<double> Bottom]:file:_files" "-TARGET_USER_ZMAX[<double> Top]:file:_files" "-TARGET_USER_ZNUM[<num> Levels]:file:_files" "-EPSILON[<double> Threshold Error]:file:_files" "-LEVEL_MAX[<num> Maximum Level]:file:_files"  
          ;;
     esac
     ;;
   esac
}

_grids_tools(){
  _arguments -C "1: :->cmds" "*::arg:->args" 
 
  case "$state" in
    cmds)
      _values "grids_tools command" \
        "0[Create a Grid Collection]" \
        "1[Extract Grids from a Grid Collection]" \
        "2[Delete Grids from a Grid Collection]" \
        "3[Extract a Grid from a Grid Collection]" \
        "4[Add a Grid to a Grid Collection]" \
        "5[Nearest Neighbour (3D)]" \
        "6[Inverse Distance Weighted (3D)]" \
        "7[Grid Collection Masking]" \
        "filter_gaussian[Gaussian Filter]" \
        "filter_laplacian[Laplacian Filter]" \
        "filter_rank[Rank Filter]" \
        "filter_simple[Simple Filter]" \
        "longitudinal_range[Change Longitudinal Range for a Grid Collection]" \

      ;;
    args)
      case "$line[1]" in
        0)
          _arguments \
            "-LIST[<str> Single Grids]:file:_files" "-GRIDS[<str> Grid Collection]:file:_files" "-NAME[<str> Name]:file:_files" "-DELETE[<str> Delete]:file:_files" "-ATTRIBUTES[<str> Attribute Definition]:file:_files" "-TABLE[<str> Attributes]:file:_files" "-TABLE_Z[<str> Z Attribute]:file:_files" "-NFIELDS[<num> Number of Attributes]:file:_files" "-ZFIELD[<num> Z Attribute]:file:_files" "-FIELDS_NAME0[<str>Name]:file:_files" "-FIELDS_TYPE0[<str>Type]:file:_files" "-FIELDS_NAME1[<str>Name]:file:_files" "-FIELDS_TYPE1[<str>Type]:file:_files" "-COPY[<str> Copy from Grid Collection]:file:_files"  
          ;;
        1)
          _arguments \
            "-GRIDS[<str> Grid Collection]:file:_files" "-LIST[<str> Single Grids]:file:_files" "-SELECTION[<str>Selection]:file:_files"  
          ;;
        2)
          _arguments \
            "-GRIDS[<str> Grid Collection]:file:_files" "-SELECTION[<str>Selection]:file:_files"  
          ;;
        3)
          _arguments \
            "-GRIDS[<str> Grid Collection]:file:_files" "-GRID[<str> Grid]:file:_files" "-Z_LEVEL[<str> Z]:file:_files" "-Z_LEVEL_DEFAULT[<double>Default]:file:_files" "-RESAMPLING[<str> Resampling]:file:_files"  
          ;;
        4)
          _arguments \
            "-GRID[<str> Grid]:file:_files" "-GRIDS[<str> Grid Collection]:file:_files" "-Z_LEVEL[<double>Z]:file:_files" "-DELETE[<str> Delete]:file:_files"  
          ;;
        5)
          _arguments \
            "-POINTS[<str> Points]:file:_files" "-Z_FIELD[<str> Z]:file:_files" "-Z_SCALE[<double> Z Factor]:file:_files" "-V_FIELD[<str> Value]:file:_files" "-TARGET_DEFINITION[<str> Target Grid System]:file:_files" "-TARGET_USER_SIZE[<double> Cellsize]:file:_files" "-TARGET_USER_XMIN[<double> West]:file:_files" "-TARGET_USER_XMAX[<double> East]:file:_files" "-TARGET_USER_YMIN[<double> South]:file:_files" "-TARGET_USER_YMAX[<double> North]:file:_files" "-TARGET_USER_COLS[<num> Columns]:file:_files" "-TARGET_USER_ROWS[<num> Rows]:file:_files" "-TARGET_USER_FLAT[<str> Rounding]:file:_files" "-TARGET_USER_FITS[<str> Fit]:file:_files" "-TARGET_TEMPLATE[<str> Target System]:file:_files" "-GRIDS[<str> Grid Collection]:file:_files" "-TARGET_USER_ZSIZE[<double>Cellsize]:file:_files" "-TARGET_USER_ZMIN[<double> Bottom]:file:_files" "-TARGET_USER_ZMAX[<double> Top]:file:_files" "-TARGET_USER_ZNUM[<num> Levels]:file:_files"  
          ;;
        6)
          _arguments \
            "-POINTS[<str> Points]:file:_files" "-Z_FIELD[<str> Z]:file:_files" "-Z_SCALE[<double> Z Factor]:file:_files" "-V_FIELD[<str> Value]:file:_files" "-TARGET_DEFINITION[<str> Target Grid System]:file:_files" "-TARGET_USER_SIZE[<double> Cellsize]:file:_files" "-TARGET_USER_XMIN[<double> West]:file:_files" "-TARGET_USER_XMAX[<double> East]:file:_files" "-TARGET_USER_YMIN[<double> South]:file:_files" "-TARGET_USER_YMAX[<double> North]:file:_files" "-TARGET_USER_COLS[<num> Columns]:file:_files" "-TARGET_USER_ROWS[<num> Rows]:file:_files" "-TARGET_USER_FLAT[<str> Rounding]:file:_files" "-TARGET_USER_FITS[<str> Fit]:file:_files" "-TARGET_TEMPLATE[<str> Target System]:file:_files" "-GRIDS[<str> Grid Collection]:file:_files" "-TARGET_USER_ZSIZE[<double>Cellsize]:file:_files" "-TARGET_USER_ZMIN[<double> Bottom]:file:_files" "-TARGET_USER_ZMAX[<double> Top]:file:_files" "-TARGET_USER_ZNUM[<num> Levels]:file:_files" "-SEARCH_RANGE[<str> Search Range]:file:_files" "-SEARCH_RADIUS[<double> Maximum Search Distance]:file:_files" "-SEARCH_POINTS_ALL[<str> Number of Points]:file:_files" "-SEARCH_POINTS_MIN[<num> Minimum]:file:_files" "-SEARCH_POINTS_MAX[<num> Maximum]:file:_files" "-DW_WEIGHTING[<str> Weighting Function]:file:_files" "-DW_IDW_POWER[<double> Power]:file:_files" "-DW_BANDWIDTH[<double> Bandwidth]:file:_files"  
          ;;
        7)
          _arguments \
            "-GRIDS[<str> Grid Collection]:file:_files" "-MASKED[<str> Masked Grid Collection]:file:_files" "-LOWER[<str> Lower Surface]:file:_files" "-UPPER[<str> Upper Surface]:file:_files" "-MASKING[<str>Masking]:file:_files"  
          ;;
        filter_gaussian)
          _arguments \
            "-GRIDS[<str> Grid Collection]:file:_files" "-FILTERED[<str> Filtered]:file:_files" "-SIGMA[<double> Standard Deviation]:file:_files" "-KERNEL_RADIUS[<num>Kernel Radius]:file:_files"  
          ;;
        filter_laplacian)
          _arguments \
            "-GRIDS[<str> Grid Collection]:file:_files" "-FILTERED[<str> Filtered]:file:_files" "-METHOD[<str> Method]:file:_files" "-SIGMA[<double> Standard Deviation]:file:_files" "-KERNEL_RADIUS[<num>Kernel Radius]:file:_files"  
          ;;
        filter_rank)
          _arguments \
            "-GRIDS[<str> Grid Collection]:file:_files" "-FILTERED[<str> Filtered]:file:_files" "-RANK[<double> Rank]:file:_files" "-KERNEL_TYPE[<str> Kernel Type]:file:_files" "-KERNEL_RADIUS[<num>Kernel Radius]:file:_files"  
          ;;
        filter_simple)
          _arguments \
            "-INPUT[<str> Grid Collection]:file:_files" "-OUTPUT[<str> Filtered]:file:_files" "-OUTPUT_FLOAT[<str> Filtered]:file:_files" "-KEEP_DEPTH[<str> Keep Input Data Depth]:file:_files" "-METHOD[<str> Filter]:file:_files" "-KERNEL_TYPE[<str> Kernel Type]:file:_files" "-KERNEL_RADIUS[<num>Kernel Radius]:file:_files"  
          ;;
        longitudinal_range)
          _arguments \
            "-GRIDS[<str> Grid Collection]:file:_files" "-CHANGED[<str> Changed]:file:_files" "-DIRECTION[<str>Direction]:file:_files"  
          ;;
     esac
     ;;
   esac
}

_grid_tools(){
  _arguments -C "1: :->cmds" "*::arg:->args" 
 
  case "$state" in
    cmds)
      _values "grid_tools command" \
        "0[Resampling]" \
        "1[Aggregate]" \
        "3[Mosaicking]" \
        "4[Constant Grid]" \
        "5[Patching]" \
        "6[Close One Cell Gaps]" \
        "7[Close Gaps]" \
        "8[Grid Buffer]" \
        "9[Threshold Buffer]" \
        "10[Grid Proximity Buffer]" \
        "11[Change Data Storage]" \
        "12[Change Grid Values]" \
        "15[Reclassify Grid Values]" \
        "17[Crop to Data]" \
        "18[Invert Data/No-Data]" \
        "20[Combine Grids]" \
        "21[Grid Cell Index]" \
        "22[Grids from Classified Grid and Table]" \
        "23[Create Grid System]" \
        "24[Grid Masking]" \
        "25[Close Gaps with Spline]" \
        "26[Proximity Grid]" \
        "27[Tiling]" \
        "28[Shrink and Expand]" \
        "29[Close Gaps with Stepwise Resampling]" \
        "30[Transpose Grids]" \
        "31[Clip Grids]" \
        "32[Select Grid from List]" \
        "33[Copy Grid]" \
        "34[Invert Grid]" \
        "35[Mirror Grid]" \
        "36[Change a Grids No-Data Value]" \
        "37[Combine Classes]" \
        "38[Mosaicking (Grid Collections)]" \
        "39[Change Grid Values - Flood Fill]" \
        "41[Shrink and Expand (Grid Collection)]" \
        "42[Change Grid System]" \
        "bulk_nodata_change[Change a Grids No-Data Value [Bulk Processing]]" \
        "grid_tools_bulk_weighted_mean[Two Grids Weighted Mean [Bulk Processing]]" \

      ;;
    args)
      case "$line[1]" in
        0)
          _arguments \
            "-INPUT[<str> Grids]:file:_files" "-OUTPUT[<str> Resampled Grids]:file:_files" "-KEEP_TYPE[<str> Preserve Data Type]:file:_files" "-SCALE_UP[<str> Upscaling Method]:file:_files" "-SCALE_DOWN[<str> Downscaling Method]:file:_files" "-TARGET_DEFINITION[<str> Target Grid System]:file:_files" "-TARGET_USER_SIZE[<double>Cellsize]:file:_files" "-TARGET_USER_XMIN[<double>West]:file:_files" "-TARGET_USER_XMAX[<double>East]:file:_files" "-TARGET_USER_YMIN[<double>South]:file:_files" "-TARGET_USER_YMAX[<double>North]:file:_files" "-TARGET_USER_COLS[<num> Columns]:file:_files" "-TARGET_USER_ROWS[<num> Rows]:file:_files" "-TARGET_USER_FLAT[<str> Rounding]:file:_files" "-TARGET_USER_FITS[<str> Fit]:file:_files" "-TARGET_TEMPLATE[<str> Target System]:file:_files"  
