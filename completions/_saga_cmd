            "-GRIDS[<str>Grids]:file:_files" "-FILE[<str> File]:file:_files"  
          ;;
        14)
          _arguments \
            "-GRIDS[<str> Grids]:file:_files" "-FILE[<str> Directory]:file:_files" "-DATATYPE[<str> Data Type]:file:_files" "-TYPE[<str> Type]:file:_files" "-NAME_DIGITS[<num>Filename Digits]:file:_files" "-MISSING[<double> Missing Value]:file:_files" "-SCALE[<double> Scale Factor]:file:_files" "-UNITS[<str> Units]:file:_files" "-DESCRIPTION[<str>Description]:file:_files" "-MMINLU[<str> Look Up Section]:file:_files" "-TILE_BDR[<num> Halo Width]:file:_files" "-PROJECTION[<str> Projection]:file:_files" "-SDTLON[<double> Standard Longitude]:file:_files" "-TRUELAT1[<double>True Latitude 1]:file:_files" "-TRUELAT2[<double>True Latitude 2]:file:_files" "-ISWATER[<num> Water]:file:_files" "-ISLAKE[<num> Lake]:file:_files" "-ISICE[<num> Ice]:file:_files" "-ISURBAN[<num> Urban]:file:_files" "-ISOILWATER[<num> Soil Water]:file:_files"  
          ;;
        16)
          _arguments \
            "-GRIDS[<str> Grids]:file:_files" "-FILES[<str> Files]:file:_files" "-KEEP_TYPE[<str> Preserve Data Type]:file:_files" "-NODATA[<str> User Defined No-Data Value]:file:_files" "-NODATA_VAL[<double>No-Data Value]:file:_files" "-CLIP[<str> Region of Interest]:file:_files" "-RESAMPLE[<str> Resample]:file:_files" "-CELLSIZE[<double> Cell Size]:file:_files" "-SCALE_UP[<str> Upscaling Method]:file:_files" "-SCALE_DOWN[<str> Downscaling Method]:file:_files"  
          ;;
        17)
          _arguments \
            "-GRIDS[<str>Grids]:file:_files" "-FILE[<str> File]:file:_files" "-SHIFT[<str>Shift]:file:_files"  
          ;;
        18)
          _arguments \
            "-FILE[<str> File]:file:_files" "-FIELD[<str> Layer Field]:file:_files" "-TABLE[<str> Table]:file:_files" "-POINTS[<str> Cell Centers]:file:_files" "-BLAYERS[<str>Layers Grid Collection]:file:_files" "-NLAYERS[<num>Number of Grids]:file:_files" "-LAYERS[<str> Layers]:file:_files" "-GRIDS[<str> Grids]:file:_files"  
          ;;
     esac
     ;;
   esac
}

