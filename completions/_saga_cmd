_grid_analysis(){
  _arguments -C "1: :->cmds" "*::arg:->args" 
 
  case "$state" in
    cmds)
      _values "grid_analysis command" \
        "0[Accumulated Cost]" \
        "5[Least Cost Paths]" \
        "6[Change Vector Analysis]" \
        "7[Covered Distance]" \
        "8[Pattern Analysis]" \
        "9[Layer of extreme value]" \
        "10[Analytical Hierarchy Process]" \
        "11[Ordered Weighted Averaging]" \
        "12[Aggregation Index]" \
        "13[Cross-Classification and Tabulation]" \
        "14[Soil Texture Classification]" \
        "15[Fragmentation (Standard)]" \
        "16[Fragmentation (Alternative)]" \
        "17[Fragmentation Classes from Density and Connectivity]" \
        "18[Accumulation Functions]" \
        "19[IMCORR - Feature Tracking]" \
        "20[Soil Texture Classification for Tables]" \
        "21[Diversity of Categories]" \
        "22[Shannon Index]" \
        "23[Simpson Index]" \
        "24[Raos Q Diversity Index (Classic)]" \
        "25[Raos Q Diversity Index]" \
        "26[Coverage of Categories]" \
        "27[Soil Water Capacity]" \
        "28[Soil Water Capacity (Grid Collections)]" \
        "29[Iterative Truncation]" \
        "30[Object Enumeration]" \

      ;;
    args)
      case "$line[1]" in
        0)
          _arguments \
            "-DEST_TYPE[<str> Destinations]:file:_files" "-DEST_POINTS[<str> Destinations]:file:_files" "-DEST_GRID[<str> Destinations]:file:_files" "-COST[<str> Local Cost]:file:_files" "-COST_BMIN[<str> Minimum Cost]:file:_files" "-COST_MIN[<double> Threshold]:file:_files" "-DIR_MAXCOST[<str> Direction of Maximum Cost]:file:_files" "-DIR_UNIT[<str> Units of Direction]:file:_files" "-DIR_K[<double> K Factor]:file:_files" "-ACCUMULATED[<str> Accumulated Cost]:file:_files" "-ALLOCATION[<str> Allocation]:file:_files" "-THRESHOLD[<double>Threshold for different route]:file:_files"  
          ;;
        5)
          _arguments \
            "-SOURCE[<str>Source Point(s)]:file:_files" "-DEM[<str> Accumulated Cost Surface]:file:_files" "-VALUES[<str>Values]:file:_files" "-POINTS[<str>Profile Points]:file:_files" "-LINE[<str> Profile Lines]:file:_files"  
          ;;
        6)
          _arguments \
            "-A[<str> Initial State]:file:_files" "-B[<str> Final State]:file:_files" "-DIST[<str>Distance]:file:_files" "-DIR[<str> Angle]:file:_files" "-C[<str> Change Vector]:file:_files"  
          ;;
        7)
          _arguments \
            "-INPUT[<str> Grids]:file:_files" "-RESULT[<str>Covered Distance]:file:_files"  
          ;;
        8)
          _arguments \
            "-INPUT[<str> Input Grid]:file:_files" "-NDC[<str> Number of Classes]:file:_files" "-RELATIVE[<str> Relative Richness]:file:_files" "-FRAGMENTATION[<str>Fragmentation]:file:_files" "-CVN[<str> Center vs. Neighbours]:file:_files" "-DIVERSITY[<str> Diversity]:file:_files" "-DOMINANCE[<str> Dominance]:file:_files" "-MAXNUMCLASS[<num> Max. Number of Classes]:file:_files" "-KERNEL_TYPE[<str> Kernel Type]:file:_files" "-KERNEL_RADIUS[<num>Radius]:file:_files"  
          ;;
        9)
          _arguments \
            "-GRIDS[<str> Grids]:file:_files" "-RESULT[<str> Result]:file:_files" "-CRITERIA[<str>Method]:file:_files"  
          ;;
        10)
          _arguments \
            "-GRIDS[<str> Input Grids]:file:_files" "-TABLE[<str> Pairwise Comparisons Table ]:file:_files" "-OUTPUT[<str>Output Grid]:file:_files"  
          ;;
        11)
          _arguments \
            "-GRIDS[<str> Input Grids]:file:_files" "-OUTPUT[<str> Output Grid]:file:_files" "-ORDERED[<str>Ordered]:file:_files" "-WEIGHTS[<str>Weights]:file:_files"  
          ;;
        12)
          _arguments \
            "-INPUT[<str> Input Grid]:file:_files" "-MAXNUMCLASS[<num>Max. Number of Classes]:file:_files" "-RESULT[<str> Result]:file:_files"  
          ;;
        13)
          _arguments \
            "-INPUT[<str> Input Grid 1]:file:_files" "-INPUT2[<str> Input Grid 2]:file:_files" "-RESULTGRID[<str> Cross-Classification Grid]:file:_files" "-RESULTTABLE[<str>Cross-Tabulation Table]:file:_files" "-MAXNUMCLASS[<num>Max. Number of Classes]:file:_files"  
          ;;
        14)
          _arguments \
            "-SAND[<str> Sand]:file:_files" "-SILT[<str> Silt]:file:_files" "-CLAY[<str> Clay]:file:_files" "-TEXTURE[<str> Soil Texture]:file:_files" "-SUM[<str> Sum]:file:_files" "-SCHEME[<str> Classification]:file:_files" "-COLORS[<str> Default Colour Scheme]:file:_files" "-USER[<str> User Definition]:file:_files" "-POLYGONS[<str>Scheme as Polygons]:file:_files" "-XY_AXES[<str> X/Y Axes]:file:_files" "-TRIANGLE[<str>Triangle]:file:_files"  
          ;;
        15)
          _arguments \
            "-CLASSES[<str> Classification]:file:_files" "-DENSITY[<str> Density (Percent)]:file:_files" "-CONNECTIVITY[<str> Connectivity (Percent)]:file:_files" "-FRAGMENTATION[<str> Fragmentation]:file:_files" "-FRAGSTATS[<str> Summary]:file:_files" "-CLASS[<num> Class Identifier]:file:_files" "-NEIGHBORHOOD_MIN[<double>Neighborhood]:file:_files" "-NEIGHBORHOOD_MAX[<double>Neighborhood]:file:_files" "-AGGREGATION[<str> Level Aggregation]:file:_files" "-BORDER[<str> Add Border]:file:_files" "-WEIGHT[<double> Connectivity Weighting]:file:_files" "-DENSITY_MIN[<double> Minimum Density (Percent)]:file:_files" "-DENSITY_INT[<double> Minimum Density for Interior Forest (Percent)]:file:_files" "-CIRCULAR[<str> Neighbourhood]:file:_files" "-DIAGONAL[<str> Include diagonal neighbour relations]:file:_files"  
          ;;
        16)
          _arguments \
            "-CLASSES[<str> Classification]:file:_files" "-DENSITY[<str> Density (Percent)]:file:_files" "-CONNECTIVITY[<str> Connectivity (Percent)]:file:_files" "-FRAGMENTATION[<str> Fragmentation]:file:_files" "-FRAGSTATS[<str> Summary]:file:_files" "-CLASS[<num> Class Identifier]:file:_files" "-NEIGHBORHOOD_MIN[<double>Neighborhood]:file:_files" "-NEIGHBORHOOD_MAX[<double>Neighborhood]:file:_files" "-AGGREGATION[<str> Level Aggregation]:file:_files" "-BORDER[<str> Add Border]:file:_files" "-WEIGHT[<double> Connectivity Weighting]:file:_files" "-DENSITY_MIN[<double> Minimum Density (Percent)]:file:_files" "-DENSITY_INT[<double> Minimum Density for Interior Forest (Percent)]:file:_files" "-LEVEL_GROW[<double> Search Distance Increment]:file:_files" "-DENSITY_MEAN[<str> Density from Neighbourhood]:file:_files"  
          ;;
        17)
          _arguments \
            "-DENSITY[<str> Density (Percent)]:file:_files" "-CONNECTIVITY[<str> Connectivity (Percent)]:file:_files" "-FRAGMENTATION[<str> Fragmentation]:file:_files" "-BORDER[<str> Add Border]:file:_files" "-WEIGHT[<double> Connectivity Weighting]:file:_files" "-DENSITY_MIN[<double>Minimum Density (Percent)]:file:_files" "-DENSITY_INT[<double>Minimum Density for Interior Forest (Percent)]:file:_files"  
          ;;
        18)
          _arguments \
            "-SURFACE[<str> Surface]:file:_files" "-INPUT[<str> Input]:file:_files" "-STATE_IN[<str> State t]:file:_files" "-OPERATION_GRID[<str> Operation Grid]:file:_files" "-CONTROL[<str> Operation Control]:file:_files" "-CTRL_LINEAR[<str> Linear Flow Control Grid]:file:_files" "-FLUX[<str> Flux]:file:_files" "-STATE_OUT[<str> State t + 1]:file:_files" "-OPERATION[<str> Operation]:file:_files" "-LINEAR[<str> Switch to Linear Flow]:file:_files" "-THRES_LINEAR[<double>Threshold Linear Flow]:file:_files"  
          ;;
        19)
          _arguments \
            "-GRID_1[<str> Grid 1]:file:_files" "-GRID_2[<str> Grid 2]:file:_files" "-DTM_1[<str> DTM 1]:file:_files" "-DTM_2[<str> DTM 2]:file:_files" "-CORRPOINTS[<str> Correlated Points]:file:_files" "-CORRLINES[<str> Displacement Vector]:file:_files" "-SEARCH_CHIPSIZE[<str>Search Chip Size (Cells)]:file:_files" "-REF_CHIPSIZE[<str> Reference Chip Size (Cells)]:file:_files" "-GRID_SPACING[<double>Grid Spacing (Map Units)]:file:_files"  
          ;;
        20)
          _arguments \
            "-TABLE[<str> Table]:file:_files" "-SAND[<str> Sand]:file:_files" "-SILT[<str> Silt]:file:_files" "-CLAY[<str> Clay]:file:_files" "-TEXTURE[<str> Texture]:file:_files" "-SCHEME[<str> Classification]:file:_files" "-COLORS[<str> Default Colour Scheme]:file:_files" "-USER[<str> User Definition]:file:_files" "-POLYGONS[<str>Scheme as Polygons]:file:_files" "-XY_AXES[<str> X/Y Axes]:file:_files" "-TRIANGLE[<str>Triangle]:file:_files"  
          ;;
        21)
          _arguments \
            "-CATEGORIES[<str> Categories]:file:_files" "-COUNT[<str> Number of Categories]:file:_files" "-DIVERSITY[<str> Diversity]:file:_files" "-CONNECTIVITY[<str> Connectivity]:file:_files" "-CONNECTEDAVG[<str> Averaged Connectivity]:file:_files" "-NB_CASE[<str> Connectivity Neighbourhood]:file:_files" "-NORMALIZE[<str> Normalize]:file:_files" "-KERNEL_TYPE[<str> Kernel Type]:file:_files" "-KERNEL_RADIUS[<num> Radius]:file:_files" "-DW_WEIGHTING[<str> Weighting Function]:file:_files" "-DW_IDW_POWER[<double>Power]:file:_files" "-DW_BANDWIDTH[<double>Bandwidth]:file:_files"  
