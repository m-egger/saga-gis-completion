            "-TABLE[<str>Table / Shapes]:file:_files" "-FIELD[<str>Attribute]:file:_files" "-CUTS[<str> Cuts]:file:_files"  
          ;;
        18)
          _arguments \
            "-SHAPES[<str> Shapes]:file:_files" "-BUFFER[<str> Buffer]:file:_files" "-DIST_FIELD[<str> Buffer Distance]:file:_files" "-DIST_FIELD_DEFAULT[<double>Default]:file:_files" "-DIST_SCALE[<double> Scaling Factor for Attribute Value]:file:_files" "-DISSOLVE[<str> Dissolve Buffers]:file:_files" "-NZONES[<num> Number of Buffer Zones]:file:_files" "-POLY_INNER[<str> Inner Buffer]:file:_files" "-DARC[<double> Arc Vertex Distance (Degree)]:file:_files"  
