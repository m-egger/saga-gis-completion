          ;;
        25)
          _arguments \
            "-FIELDS[<str> Fields]:file:_files" "-FIELD_ID[<str> Field Identifier]:file:_files" "-SCENARIO[<str> Land Use Scenario]:file:_files" "-OUTPUT[<str> Output of...]:file:_files" "-STATISTICS[<str> Crop Statistics]:file:_files" "-KNOWN_CROPS[<str>Known Crops]:file:_files"  
