        import_text_tables)
          _arguments \
            "-TABLES[<str> Tables]:file:_files" "-FILES[<str> Files]:file:_files" "-HEADLINE[<str> Headline]:file:_files" "-SEPARATOR[<str>Separator]:file:_files" "-SEP_OTHER[<str>Separator (other)]:file:_files"  
