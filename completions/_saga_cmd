  case "$state" in
    cmds)
      _values "statistics_kriging command" \
        "0[Ordinary Kriging]" \
        "1[Simple Kriging]" \
        "2[Universal Kriging]" \
        "3[Regression Kriging]" \
        "4[Variogram (Dialog)]" \
        "5[Simple Kriging (3D)]" \
        "6[Ordinary Kriging (3D)]" \

      ;;
    args)
      case "$line[1]" in
        0)
