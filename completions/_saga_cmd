_ta_channels(){
  _arguments -C "1: :->cmds" "*::arg:->args" 
 
  case "$state" in
    cmds)
      _values "ta_channels command" \
        "0[Channel Network]" \
        "1[Watershed Basins]" \
        "2[Watershed Basins (Extended)]" \
        "3[Vertical Distance to Channel Network]" \
        "4[Overland Flow Distance to Channel Network]" \
        "5[Channel Network and Drainage Basins]" \
        "6[Strahler Order]" \
        "7[Valley Depth]" \

      ;;
    args)
      case "$line[1]" in
        0)
          _arguments \
            "-ELEVATION[<str> Elevation]:file:_files" "-SINKROUTE[<str> Flow Direction]:file:_files" "-CHNLNTWRK[<str> Channel Network]:file:_files" "-CHNLROUTE[<str> Channel Direction]:file:_files" "-SHAPES[<str> Channel Network]:file:_files" "-INIT_GRID[<str> Initiation Grid]:file:_files" "-INIT_METHOD[<str> Initiation Type]:file:_files" "-INIT_VALUE[<double>Initiation Threshold]:file:_files" "-DIV_GRID[<str> Divergence]:file:_files" "-DIV_CELLS[<num> Tracing[ Max. Divergence]:file:_files" "-TRACE_WEIGHT[<str> Tracing[ Weight]:file:_files" "-MINLEN[<num> Min. Segment Length]:file:_files"  
          ;;
        1)
          _arguments \
            "-ELEVATION[<str>Elevation]:file:_files" "-CHANNELS[<str> Channel Network]:file:_files" "-SINKROUTE[<str>Sink Route]:file:_files" "-BASINS[<str> Watershed Basins]:file:_files" "-MINSIZE[<num> Min. Size]:file:_files"  
          ;;
        2)
          _arguments \
            "-DEM[<str> DEM]:file:_files" "-CHANNELS[<str> Drainage Network]:file:_files" "-BASINS[<str> Basins]:file:_files" "-SUBBASINS[<str> Subbasins]:file:_files" "-V_BASINS[<str> Basins]:file:_files" "-V_SUBBASINS[<str>Subbasins]:file:_files" "-HEADS[<str> River Heads]:file:_files" "-MOUTHS[<str> River Mouths]:file:_files" "-DISTANCE[<str> Flow Distances]:file:_files"  
          ;;
        3)
          _arguments \
            "-ELEVATION[<str> Elevation]:file:_files" "-CHANNELS[<str> Channel Network]:file:_files" "-DISTANCE[<str> Vertical Distance to Channel Network]:file:_files" "-BASELEVEL[<str> Channel Network Base Level]:file:_files" "-THRESHOLD[<double> Tension Threshold]:file:_files" "-MAXITER[<num> Maximum Iterations]:file:_files" "-NOUNDERGROUND[<str>Keep Base Level below Surface]:file:_files"  
          ;;
        4)
          _arguments \
            "-ELEVATION[<str> Elevation]:file:_files" "-CHANNELS[<str> Channel Network]:file:_files" "-ROUTE[<str> Preferred Routing]:file:_files" "-DISTANCE[<str> Overland Flow Distance]:file:_files" "-DISTVERT[<str> Vertical Overland Flow Distance]:file:_files" "-DISTHORZ[<str> Horizontal Overland Flow Distance]:file:_files" "-TIME[<str> Flow Travel Time]:file:_files" "-SDR[<str> Sediment Yield Delivery Ratio]:file:_files" "-FIELDS[<str> Fields]:file:_files" "-PASSES[<str> Fields Visited]:file:_files" "-METHOD[<str> Flow Algorithm]:file:_files" "-BOUNDARY[<str> Boundary Cells]:file:_files" "-FLOW_B[<double> Beta]:file:_files" "-FLOW_K[<str> Manning-Strickler Coefficient]:file:_files" "-FLOW_K_DEFAULT[<double>Default]:file:_files" "-FLOW_R[<str> Flow Depth]:file:_files" "-FLOW_R_DEFAULT[<double>Default]:file:_files"  
          ;;
        5)
          _arguments \
            "-DEM[<str> Elevation]:file:_files" "-DIRECTION[<str> Flow Direction]:file:_files" "-CONNECTION[<str>Flow Connectivity]:file:_files" "-ORDER[<str> Strahler Order]:file:_files" "-BASIN[<str> Drainage Basins]:file:_files" "-SEGMENTS[<str> Channels]:file:_files" "-BASINS[<str> Drainage Basins]:file:_files" "-NODES[<str> Junctions]:file:_files" "-THRESHOLD[<num> Threshold]:file:_files" "-SUBBASINS[<str> Subbasins]:file:_files"  
          ;;
        6)
          _arguments \
            "-DEM[<str> Elevation]:file:_files" "-STRAHLER[<str>Strahler Order]:file:_files"  
          ;;
        7)
          _arguments \
            "-ELEVATION[<str> Elevation]:file:_files" "-VALLEY_DEPTH[<str> Valley Depth]:file:_files" "-RIDGE_LEVEL[<str> Ridge Level]:file:_files" "-THRESHOLD[<double> Tension Threshold]:file:_files" "-MAXITER[<num> Maximum Iterations]:file:_files" "-NOUNDERGROUND[<str>Keep Ridge Level above Surface]:file:_files" "-ORDER[<num> Ridge Detection Threshold]:file:_files"  
          ;;
     esac
     ;;
   esac
}

_ta_cliffmetrics(){
  _arguments -C "1: :->cmds" "*::arg:->args" 
 
  case "$state" in
    cmds)
      _values "ta_cliffmetrics command" \
        "0[CliffMetrics]" \
        "1[Coastal Profile Crossings]" \

      ;;
    args)
      case "$line[1]" in
        0)
          _arguments \
            "-DEM[<str> Elevation]:file:_files" "-SEDIMENT_TOP[<str> Sediment Top Elevation]:file:_files" "-RASTER_COAST[<str> Coastline]:file:_files" "-RASTER_NORMAL[<str> Normals]:file:_files" "-COAST_INITIAL[<str> User Defined Coastline Points]:file:_files" "-CoastSeaHandiness[<str> Sea handiness]:file:_files" "-StartEdgeUserCoastLine[<str>Start edge coastline]:file:_files" "-EndEdgeUserCoastLine[<str> End edge coastline]:file:_files" "-COAST[<str> Coastline]:file:_files" "-NORMALS[<str> Coastline-Normal Profiles]:file:_files" "-CLIFF_TOP[<str> Cliff Top Points]:file:_files" "-CLIFF_TOE[<str> Cliff Toe Points]:file:_files" "-COAST_POINT[<str> Coast Points]:file:_files" "-INVALID_NORMALS[<str> Invalid Coastline-Normal Profiles]:file:_files" "-COAST_CURVATURE[<str> Coastline Curvature]:file:_files" "-PROFILES[<str> Profile Data]:file:_files" "-StillWaterLevel[<double> Still Water Level]:file:_files" "-CoastSmooth[<str> Coastline Smoothing Algorithm]:file:_files" "-CoastSmoothWindow[<num> Coastline Smoothing Window Size]:file:_files" "-SavGolCoastPoly[<num> Polynomial Order for Savitsky-Golay]:file:_files" "-ScaleRasterOutput[<str> Scale Raster Output Values]:file:_files" "-RandomCoastEdgeSearch[<str> Random Edge for Coastline Search]:file:_files" "-CoastNormalLength[<double> Length of Coastline Normals]:file:_files" "-EleTolerance[<double> Vertical Tolerance]:file:_files" "-OutPath[<str> Main Output File Directory]:file:_files"  
          ;;
        1)
          _arguments \
            "-LINES_SeaSide[<str> SeaSide Profile Lines Layer]:file:_files" "-LINES_LandSide[<str> LandSide Profile Lines Layer]:file:_files" "-LINES_Coast[<str> Coast Lines Layer]:file:_files" "-CROSSINGS_SEASIDE[<str> Crossings at Sea Side]:file:_files" "-CROSSINGS_LANDSIDE[<str>Crossings at Land Side]:file:_files" "-ATTRIBUTES[<str> Parent Attributes]:file:_files" "-DISTANCES[<str> Distances to profile start point]:file:_files"  
          ;;
     esac
     ;;
   esac
}

_ta_compound(){
  _arguments -C "1: :->cmds" "*::arg:->args" 
 
  case "$state" in
    cmds)
      _values "ta_compound command" \
        "0[Basic Terrain Analysis]" \

      ;;
    args)
      case "$line[1]" in
        0)
          _arguments \
            "-ELEVATION[<str> Elevation]:file:_files" "-SHADE[<str> Analytical Hillshading]:file:_files" "-SLOPE[<str> Slope]:file:_files" "-ASPECT[<str> Aspect]:file:_files" "-HCURV[<str> Plan Curvature]:file:_files" "-VCURV[<str> Profile Curvature]:file:_files" "-CONVERGENCE[<str>Convergence Index]:file:_files" "-SINKS[<str> Closed Depressions]:file:_files" "-FLOW[<str> Total Catchment Area]:file:_files" "-WETNESS[<str> Topographic Wetness Index]:file:_files" "-LSFACTOR[<str> LS-Factor]:file:_files" "-CHANNELS[<str> Channel Network]:file:_files" "-BASINS[<str> Drainage Basins]:file:_files" "-CHNL_BASE[<str> Channel Network Base Level]:file:_files" "-CHNL_DIST[<str> Channel Network Distance]:file:_files" "-VALL_DEPTH[<str> Valley Depth]:file:_files" "-RSP[<str> Relative Slope Position]:file:_files" "-THRESHOLD[<num> Channel Density]:file:_files"  
          ;;
     esac
     ;;
   esac
}

_ta_hydrology(){
  _arguments -C "1: :->cmds" "*::arg:->args" 
 
  case "$state" in
    cmds)
      _values "ta_hydrology command" \
        "0[Flow Accumulation (Top-Down)]" \
        "1[Flow Accumulation (Recursive)]" \
        "2[Flow Accumulation (Flow Tracing)]" \
        "4[Upslope Area]" \
        "6[Flow Path Length]" \
        "7[Slope Length]" \
        "10[Cell Balance]" \
        "13[Edge Contamination]" \
        "15[SAGA Wetness Index]" \
        "18[Flow Accumulation (Mass-Flux Method)]" \
        "19[Flow Width and Specific Catchment Area]" \
        "20[Topographic Wetness Index]" \
        "21[Stream Power Index]" \
        "22[LS Factor]" \
        "23[Melton Ruggedness Number]" \
        "24[TCI Low]" \
        "25[LS-Factor, Field Based]" \
        "26[Slope Limited Flow Accumulation]" \
        "27[Maximum Flow Path Length]" \
        "28[Flow between fields]" \
        "29[Flow Accumulation (Parallelizable)]" \
        "30[Isochrones Variable Speed]" \
        "31[CIT Index]" \
        "32[Terrain Flooding]" \
        "flow_accumulation[Flow Accumulation (One Step)]" \
        "ls_factor[LS Factor (One Step)]" \
        "twi[Topographic Wetness Index (One Step)]" \
        "upslope_height[Upslope Height, Slope, Aspect]" \

      ;;
    args)
      case "$line[1]" in
        0)
          _arguments \
            "-ELEVATION[<str> Elevation]:file:_files" "-SINKROUTE[<str> Sink Routes]:file:_files" "-WEIGHTS[<str> Weights]:file:_files" "-FLOW[<str> Flow Accumulation]:file:_files" "-VAL_INPUT[<str> Input for Mean over Catchment]:file:_files" "-VAL_MEAN[<str> Mean over Catchment]:file:_files" "-ACCU_MATERIAL[<str> Material for Accumulation]:file:_files" "-ACCU_TARGET[<str> Accumulation Target]:file:_files" "-ACCU_TOTAL[<str> Accumulated Material]:file:_files" "-ACCU_LEFT[<str> Accumulated Material (Left Side)]:file:_files" "-ACCU_RIGHT[<str> Accumulated Material (Right Side)]:file:_files" "-STEP[<num> Step]:file:_files" "-FLOW_UNIT[<str> Flow Accumulation Unit]:file:_files" "-FLOW_LENGTH[<str> Flow Path Length]:file:_files" "-LINEAR_VAL[<str> Linear Flow Threshold Grid]:file:_files" "-LINEAR_DIR[<str> Channel Direction]:file:_files" "-WEIGHT_LOSS[<str> Loss through Negative Weights]:file:_files" "-METHOD[<str> Method]:file:_files" "-LINEAR_DO[<str> Thresholded Linear Flow]:file:_files" "-LINEAR_MIN[<num> Linear Flow Threshold]:file:_files" "-CONVERGENCE[<double>Convergence]:file:_files" "-MFD_CONTOUR[<str> Contour Length]:file:_files" "-NO_NEGATIVES[<str> Prevent Negative Flow Accumulation]:file:_files"  
          ;;
        1)
          _arguments \
            "-ELEVATION[<str> Elevation]:file:_files" "-SINKROUTE[<str> Sink Routes]:file:_files" "-WEIGHTS[<str> Weights]:file:_files" "-FLOW[<str> Flow Accumulation]:file:_files" "-VAL_INPUT[<str> Input for Mean over Catchment]:file:_files" "-VAL_MEAN[<str> Mean over Catchment]:file:_files" "-ACCU_MATERIAL[<str> Material for Accumulation]:file:_files" "-ACCU_TARGET[<str> Accumulation Target]:file:_files" "-ACCU_TOTAL[<str> Accumulated Material]:file:_files" "-ACCU_LEFT[<str> Accumulated Material (Left Side)]:file:_files" "-ACCU_RIGHT[<str> Accumulated Material (Right Side)]:file:_files" "-FLOW_UNIT[<str> Flow Accumulation Unit]:file:_files" "-TARGETS[<str> Target Areas]:file:_files" "-FLOW_LENGTH[<str> Flow Path Length]:file:_files" "-WEIGHT_LOSS[<str> Loss through Negative Weights]:file:_files" "-METHOD[<str> Method]:file:_files" "-CONVERGENCE[<double>Convergence]:file:_files" "-MFD_CONTOUR[<str> Contour Length]:file:_files" "-NO_NEGATIVES[<str> Prevent Negative Flow Accumulation]:file:_files"  
          ;;
        2)
          _arguments \
            "-ELEVATION[<str> Elevation]:file:_files" "-SINKROUTE[<str> Sink Routes]:file:_files" "-WEIGHTS[<str> Weights]:file:_files" "-FLOW[<str> Flow Accumulation]:file:_files" "-VAL_INPUT[<str> Input for Mean over Catchment]:file:_files" "-VAL_MEAN[<str> Mean over Catchment]:file:_files" "-ACCU_MATERIAL[<str>Material for Accumulation]:file:_files" "-ACCU_TARGET[<str> Accumulation Target]:file:_files" "-ACCU_TOTAL[<str> Accumulated Material]:file:_files" "-ACCU_LEFT[<str> Accumulated Material (Left Side)]:file:_files" "-ACCU_RIGHT[<str> Accumulated Material (Right Side)]:file:_files" "-STEP[<num> Step]:file:_files" "-FLOW_UNIT[<str> Flow Accumulation Unit]:file:_files" "-METHOD[<str> Method]:file:_files" "-CORRECT[<str> Flow Correction]:file:_files" "-MINDQV[<double> Minimum DQV]:file:_files"  
