            "-SAND[<str> Sand]:file:_files" "-SILT[<str> Silt]:file:_files" "-CLAY[<str> Clay]:file:_files" "-CORG[<str> Soil Organic Carbon]:file:_files" "-BULK[<str> Bulk Density]:file:_files" "-CEC[<str> Cation Exchange Capacity]:file:_files" "-PH[<str> pH]:file:_files" "-FC[<str> Field Capacity]:file:_files" "-PWP[<str> Permanent Wilting Point]:file:_files" "-THETA_S[<str> Water Capacity at Saturation]:file:_files" "-UNIT[<str> Output Unit]:file:_files" "-FUNCTION[<str> Pedo-Transfer Function]:file:_files" "-PSI_FC[<double> Soil Moisture Potential]:file:_files" "-PSI_PWP[<double> Soil Moisture Potential]:file:_files" "-ADJUST[<str> Adjustments]:file:_files" "-USERDEF[<str> User Defined Coefficients]:file:_files" "-COEFFICIENTS[<str>User Defined Coefficients]:file:_files"  
