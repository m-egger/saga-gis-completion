            "-DEM[<str> Elevation]:file:_files" "-CHANNELS[<str> Channel Mask]:file:_files" "-MODEL[<str> Modelled Elevation]:file:_files" "-DIFF[<str> Elevation Difference]:file:_files" "-UPDATE[<str> Update]:file:_files" "-KAPPA[<double> Diffusivity (m2 / kyr)]:file:_files" "-DURATION[<double>Simulation Time (kyr)]:file:_files" "-TIMESTEP[<str> Time Step]:file:_files" "-DTIME[<double> Time Step (kyr)]:file:_files"  
          ;;
     esac
     ;;
   esac
}

