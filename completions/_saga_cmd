_statistics_points(){
  _arguments -C "1: :->cmds" "*::arg:->args" 
 
  case "$state" in
    cmds)
      _values "statistics_points command" \
        "0[Variogram]" \
        "1[Variogram Cloud]" \
        "2[Variogram Surface]" \
        "3[Minimum Distance Analysis]" \
        "4[Spatial Point Pattern Analysis]" \

      ;;
    args)
      case "$line[1]" in
        0)
          _arguments \
            "-POINTS[<str> Points]:file:_files" "-FIELD[<str> Attribute]:file:_files" "-RESULT[<str> Sample Variogram]:file:_files" "-DISTCOUNT[<num> Initial Number of Distance Classes]:file:_files" "-DISTMAX[<double>Maximum Distance]:file:_files" "-NSKIP[<num> Skip Number]:file:_files"  
          ;;
        1)
          _arguments \
            "-POINTS[<str> Points]:file:_files" "-FIELD[<str> Attribute]:file:_files" "-RESULT[<str> Variogram Cloud]:file:_files" "-DISTMAX[<double>Maximum Distance]:file:_files" "-NSKIP[<num> Skip Number]:file:_files"  
          ;;
        2)
          _arguments \
            "-POINTS[<str> Points]:file:_files" "-FIELD[<str> Attribute]:file:_files" "-DISTCOUNT[<num> Number of Distance Classes]:file:_files" "-NSKIP[<num> Skip Number]:file:_files" "-COUNT[<str> Number of Pairs]:file:_files" "-VARIANCE[<str> Variogram Surface]:file:_files" "-COVARIANCE[<str>Covariance Surface]:file:_files"  
          ;;
        3)
          _arguments \
            "-POINTS[<str> Points]:file:_files" "-TABLE[<str> Minimum Distance Analysis]:file:_files" "-DIMENSION[<str>Distance Measurement]:file:_files"  
          ;;
        4)
          _arguments \
            "-POINTS[<str> Points]:file:_files" "-WEIGHT[<str> Weight]:file:_files" "-CENTRE[<str> Mean Centre]:file:_files" "-STDDIST[<str>Standard Distance]:file:_files" "-STEP[<double>Vertex Distance (Degree)]:file:_files" "-BBOX[<str> Bounding Box]:file:_files"  
          ;;
     esac
     ;;
   esac
}

