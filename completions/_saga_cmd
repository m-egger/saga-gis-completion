            "-POINTS[<str> Points]:file:_files" "-FIELD[<str> Attribute]:file:_files" "-TARGET_DEFINITION[<str> Target Grid System]:file:_files" "-TARGET_USER_SIZE[<double>Cellsize]:file:_files" "-TARGET_USER_XMIN[<double>West]:file:_files" "-TARGET_USER_XMAX[<double>East]:file:_files" "-TARGET_USER_YMIN[<double>South]:file:_files" "-TARGET_USER_YMAX[<double>North]:file:_files" "-TARGET_USER_COLS[<num> Columns]:file:_files" "-TARGET_USER_ROWS[<num> Rows]:file:_files" "-TARGET_USER_FLAT[<str> Rounding]:file:_files" "-TARGET_USER_FITS[<str> Fit]:file:_files" "-TARGET_TEMPLATE[<str> Target System]:file:_files" "-PREDICTION[<str> Prediction]:file:_files" "-VARIANCE[<str> Prediction Error]:file:_files" "-TQUALITY[<str> Error Measure]:file:_files" "-VAR_MAXDIST[<double> Maximum Distance]:file:_files" "-VAR_NCLASSES[<num> Lag Distance Classes]:file:_files" "-VAR_NSKIP[<num> Skip]:file:_files" "-VAR_MODEL[<str> Model]:file:_files" "-LOG[<str> Logarithmic Transformation]:file:_files" "-BLOCK[<str> Block Kriging]:file:_files" "-DBLOCK[<double> Block Size]:file:_files" "-CV_METHOD[<str> Cross Validation]:file:_files" "-CV_SUMMARY[<str> Cross Validation Summary]:file:_files" "-CV_RESIDUALS[<str> Cross Validation Residuals]:file:_files" "-CV_SAMPLES[<num> Cross Validation Subsamples]:file:_files" "-SEARCH_RANGE[<str> Search Range]:file:_files" "-SEARCH_RADIUS[<double> Maximum Search Distance]:file:_files" "-SEARCH_POINTS_ALL[<str> Number of Points]:file:_files" "-SEARCH_POINTS_MIN[<num> Minimum]:file:_files" "-SEARCH_POINTS_MAX[<num> Maximum]:file:_files"  
