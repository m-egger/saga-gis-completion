_grid_visualisation(){
  _arguments -C "1: :->cmds" "*::arg:->args" 
 
  case "$state" in
    cmds)
      _values "grid_visualisation command" \
        "0[Color Palette Rotation]" \
        "1[Grid Animation]" \
        "2[Fit Color Palette to Grid Values]" \
        "3[RGB Composite]" \
        "4[Create 3D Image]" \
        "5[Color Triangle Composite]" \
        "6[Histogram Surface]" \
        "7[Aspect-Slope Grid]" \
        "8[Terrain Map View]" \
        "9[Split RGB Composite]" \
        "10[Select Look-up Table for Grid Visualization]" \
        "11[Create a Table from Look-up Table]" \

      ;;
    args)
      case "$line[1]" in
        0)
          _arguments \
            "-GRID[<str> Grid]:file:_files" "-COLORS[<str>Colors]:file:_files" "-DOWN[<str> Down]:file:_files"  
          ;;
        1)
          _arguments \
            "-GRIDS[<str> Grids]:file:_files" "-GRID[<str> Grid Animation]:file:_files" "-FILE[<str> Save Frames]:file:_files" "-FILE_NODATA[<str> Set Transparency for No-Data]:file:_files" "-FILE_BGCOL[<str> Background Color]:file:_files" "-FILE_DELAY[<num> Delay]:file:_files" "-FILE_COLORS[<num> Color Depth]:file:_files" "-COLORS[<str> Colors]:file:_files" "-NSTEPS[<num> Interpolation Steps]:file:_files" "-PROGRESS[<str> Progress Bar]:file:_files" "-LOOP[<str> Loop]:file:_files" "-RANGE[<str> Histogram Stretch]:file:_files" "-RANGE_PERCENT[<double> Percent Stretch]:file:_files" "-RANGE_STDDEV[<double> Standard Deviation]:file:_files" "-RANGE_KEEP[<str> Keep in Range]:file:_files" "-RANGE_USER_MIN[<double>Range]:file:_files" "-RANGE_USER_MAX[<double>Range]:file:_files"  
          ;;
        2)
          _arguments \
            "-GRID[<str> Grid]:file:_files" "-COUNT[<num> Number of Colors]:file:_files" "-SCALE[<str> Scale]:file:_files" "-RANGE_MIN[<double>User defined range]:file:_files" "-RANGE_MAX[<double>User defined range]:file:_files"  
          ;;
        3)
          _arguments \
            "-R_GRID[<str> Red]:file:_files" "-G_GRID[<str> Green]:file:_files" "-B_GRID[<str> Blue]:file:_files" "-A_GRID[<str> Alpha]:file:_files" "-RGB[<str> Composite]:file:_files" "-METHOD[<str> Value Preparation]:file:_files" "-RANGE_MIN[<double> Rescale Range]:file:_files" "-RANGE_MAX[<double> Rescale Range]:file:_files" "-PERCTL_MIN[<double>Percentiles]:file:_files" "-PERCTL_MAX[<double>Percentiles]:file:_files" "-STDDEV[<double> Standard Deviation]:file:_files" "-NODATA[<str> Ignore No Data Cells]:file:_files"  
          ;;
        4)
          _arguments \
            "-DEM[<str> Elevation]:file:_files" "-IMAGE[<str> Overlay Image]:file:_files" "-SHAPES[<str> Shapes to project]:file:_files" "-ZEXAGG[<double> Exaggeration]:file:_files" "-ZEXAGG_MIN[<double> Minimum Exaggeration (%)]:file:_files" "-Z_ROTATE[<double> Image Rotation (Degree)]:file:_files" "-X_ROTATE[<double> Local Rotation (Degree)]:file:_files" "-X_ROTATE_LEVEL[<str>Local Rotation Base Level]:file:_files" "-PANBREAK[<double> Panorama Break (%)]:file:_files" "-BKCOLOR[<str> Background Color]:file:_files" "-PROJECTION[<str> Projection]:file:_files" "-NX[<num> 3D Image Width]:file:_files" "-NY[<num> 3D Image Height]:file:_files" "-RGB[<str> 3D Image]:file:_files" "-RGB_Z[<str> Projected Height]:file:_files"  
          ;;
        5)
          _arguments \
            "-A_GRID[<str> A]:file:_files" "-A_COLOR[<str> Color]:file:_files" "-A_METHOD[<str> Value Preparation]:file:_files" "-A_RANGE_MIN[<double> Rescale Range]:file:_files" "-A_RANGE_MAX[<double> Rescale Range]:file:_files" "-A_PERCTL_MIN[<double>Percentiles]:file:_files" "-A_PERCTL_MAX[<double>Percentiles]:file:_files" "-A_PERCENT[<double> Percentage of Standard Deviation]:file:_files" "-B_GRID[<str> B]:file:_files" "-B_COLOR[<str> Color]:file:_files" "-B_METHOD[<str> Value Preparation]:file:_files" "-B_RANGE_MIN[<double> Rescale Range]:file:_files" "-B_RANGE_MAX[<double> Rescale Range]:file:_files" "-B_PERCTL_MIN[<double>Percentiles]:file:_files" "-B_PERCTL_MAX[<double>Percentiles]:file:_files" "-B_PERCENT[<double> Percentage of standard deviation]:file:_files" "-C_GRID[<str> C]:file:_files" "-C_COLOR[<str> Color]:file:_files" "-C_METHOD[<str> Value Preparation]:file:_files" "-C_RANGE_MIN[<double> Rescale Range]:file:_files" "-C_RANGE_MAX[<double> Rescale Range]:file:_files" "-C_PERCTL_MIN[<double>Percentiles]:file:_files" "-C_PERCTL_MAX[<double>Percentiles]:file:_files" "-C_PERCENT[<double> Percentage of Standard Deviation]:file:_files" "-GRID[<str> Composite]:file:_files"  
          ;;
        6)
          _arguments \
            "-GRID[<str> Grid]:file:_files" "-HIST[<str> Histogram]:file:_files" "-METHOD[<str>Method]:file:_files"  
          ;;
        7)
          _arguments \
            "-ASPECT[<str> Aspect]:file:_files" "-SLOPE[<str> Slope]:file:_files" "-ASPECT_SLOPE[<str>Aspect-Slope]:file:_files" "-LUT[<str> Lookup Table]:file:_files"  
          ;;
        8)
          _arguments \
            "-DEM[<str> DEM]:file:_files" "-SHADE[<str> Shade]:file:_files" "-OPENNESS[<str> Openness]:file:_files" "-SLOPE[<str> Slope]:file:_files" "-CONTOURS[<str> Contours]:file:_files" "-METHOD[<str> Method]:file:_files" "-RADIUS[<double> Radial Limit]:file:_files" "-CONTOUR_LINES[<str> Contour Lines]:file:_files" "-EQUIDISTANCE[<double>Equidistance]:file:_files"  
          ;;
        9)
          _arguments \
            "-RGB[<str> RGB Composite]:file:_files" "-R[<str> Red]:file:_files" "-G[<str> Green]:file:_files" "-B[<str> Blue]:file:_files" "-A[<str> Alpha]:file:_files" "-NODATA[<str>Ignore No Data]:file:_files"  
          ;;
        10)
          _arguments \
            "-GRID[<str> Grid]:file:_files" "-LUT[<str> Look-up Table]:file:_files" "-NAME[<str> Name]:file:_files" "-VALUE[<str> Value]:file:_files" "-VALUE_MAX[<str> Value (Range Maximum)]:file:_files" "-DESCRIPTION[<str>Description]:file:_files" "-COLOR[<str> Color]:file:_files"  
          ;;
        11)
          _arguments \
            "-TABLE[<str>Table]:file:_files" "-LUT[<str> Look-up Table]:file:_files"  
          ;;
     esac
     ;;
   esac
}

_imagery_classification(){
  _arguments -C "1: :->cmds" "*::arg:->args" 
 
  case "$state" in
    cmds)
      _values "imagery_classification command" \
        "0[Supervised Image Classification]" \
        "1[K-Means Clustering for Grids]" \
        "2[Confusion Matrix (Two Grids)]" \
        "3[Decision Tree]" \
        "6[Confusion Matrix (Polygons / Grid)]" \
        "lczc[Local Climate Zone Classification]" \
        "classify_majority[Supervised Majority Choice Image Classification]" \

      ;;
    args)
      case "$line[1]" in
        0)
          _arguments \
            "-GRIDS[<str> Features]:file:_files" "-NORMALISE[<str> Normalize]:file:_files" "-CLASSES[<str> Classification]:file:_files" "-QUALITY[<str> Quality]:file:_files" "-CLASSES_LUT[<str> Look-up Table]:file:_files" "-TRAIN_WITH[<str> Training]:file:_files" "-TRAINING[<str> Training Areas]:file:_files" "-TRAINING_CLASS[<str> Class Identifier]:file:_files" "-TRAIN_BUFFER[<double> Buffer Size]:file:_files" "-TRAIN_SAMPLES[<str> Training Samples]:file:_files" "-FILE_LOAD[<str> Load Statistics from File...]:file:_files" "-FILE_SAVE[<str> Save Statistics to File...]:file:_files" "-METHOD[<str> Method]:file:_files" "-THRESHOLD_DIST[<double> Distance Threshold]:file:_files" "-THRESHOLD_ANGLE[<double>Spectral Angle Threshold (Degree)]:file:_files" "-THRESHOLD_PROB[<double> Probability Threshold]:file:_files" "-RELATIVE_PROB[<str> Probability Reference]:file:_files"  
          ;;
        1)
          _arguments \
            "-GRIDS[<str> Grids]:file:_files" "-CLUSTER[<str> Clusters]:file:_files" "-STATISTICS[<str>Statistics]:file:_files" "-METHOD[<str> Method]:file:_files" "-NCLUSTER[<num> Clusters]:file:_files" "-MAXITER[<num> Maximum Iterations]:file:_files" "-NORMALISE[<str> Normalise]:file:_files" "-INITIALIZE[<str>Start Partition]:file:_files" "-OLDVERSION[<str>Old Version]:file:_files"  
          ;;
        2)
          _arguments \
            "-ONE[<str> Classification 1]:file:_files" "-ONE_LUT[<str> Look-up Table]:file:_files" "-ONE_LUT_MIN[<str>Value]:file:_files" "-ONE_LUT_MAX[<str>Value (Maximum)]:file:_files" "-ONE_LUT_NAM[<str>Name]:file:_files" "-TWO[<str> Classification 2]:file:_files" "-TWO_LUT[<str> Look-up Table]:file:_files" "-TWO_LUT_MIN[<str>Value]:file:_files" "-TWO_LUT_MAX[<str>Value (Maximum)]:file:_files" "-TWO_LUT_NAM[<str>Name]:file:_files" "-COMBINED[<str> Combined Classes]:file:_files" "-NOCHANGE[<str> Report Unchanged Classes]:file:_files" "-NODATA[<str> Include Unclassified Cells]:file:_files" "-CONFUSION[<str> Confusion Matrix]:file:_files" "-OUTPUT[<str> Output as...]:file:_files" "-CLASSES[<str> Class Values]:file:_files" "-SUMMARY[<str> Summary]:file:_files"  
          ;;
        3)
          _arguments \
            "-CLASSES[<str> Decision Tree]:file:_files" "-ROOT_GRID[<str> Grid]:file:_files" "-ROOT_THRESHOLD[<double>Threshold]:file:_files" "-ROOT_A_NAME[<str> Name]:file:_files" "-ROOT_A_NODE[<str> Children]:file:_files" "-ROOT_B_NAME[<str> Name]:file:_files" "-ROOT_B_NODE[<str> Children]:file:_files"  
          ;;
        6)
          _arguments \
            "-GRID[<str> Classification]:file:_files" "-GRID_VALUES[<str> Value Interpretation]:file:_files" "-GRID_LUT[<str> Look-up Table]:file:_files" "-GRID_LUT_MIN[<str>Value]:file:_files" "-GRID_LUT_MAX[<str>Value (Maximum)]:file:_files" "-GRID_LUT_NAM[<str>Name]:file:_files" "-POLYGONS[<str> Polygons]:file:_files" "-FIELD[<str> Classes]:file:_files" "-CONFUSION[<str> Confusion Matrix]:file:_files" "-CLASSES[<str> Class Values]:file:_files" "-SUMMARY[<str> Summary]:file:_files" "-NO_CLASS[<str> Unclassified]:file:_files"  
          ;;
        lczc)
          _arguments \
            "-FEATURES[<str> Features]:file:_files" "-GRIDDEFILE[<str> grid definition for KML2shp conversion]:file:_files" "-FILE_TRAINING[<str> Training Areas]:file:_files" "-CLASSIFIER[<str> Classifier]:file:_files" "-CLASS_DEF_SRC[<str> Class Definition]:file:_files" "-CLASS_DEF_FILE[<str> Class Definition File]:file:_files" "-LCZC[<str> LCZC]:file:_files" "-LCZC_FILE[<str> KMZ File]:file:_files" "-LCZC_FILTERED[<str> LCZC (Filtered)]:file:_files" "-FILE_FILTERED_LCZC[<str>KMZ File]:file:_files" "-FILTER_RADIUS[<num> Majority Filter Radius]:file:_files"  
          ;;
        classify_majority)
          _arguments \
            "-FEATURES[<str> Features]:file:_files" "-NORMALIZE[<str> Normalize]:file:_files" "-MODEL_TRAIN[<str> Training]:file:_files" "-TRAIN_AREAS[<str> Training Areas]:file:_files" "-TRAIN_CLASS[<str> Class Identifier]:file:_files" "-TRAIN_BUFFER[<double> Buffer Size]:file:_files" "-TRAIN_SAMPLES[<str> Training Samples]:file:_files" "-CLASSES[<str> Majority Choice]:file:_files" "-MAJORITY_COUNT[<str> Majority Count]:file:_files" "-NUNIQUES[<str> Classes Count]:file:_files" "-UNAMBIGUOUS[<str> Unambiguous]:file:_files" "-CLASSIFY_BOX[<str> Parallel Epiped]:file:_files" "-CLASSIFY_MINDIST[<str> Minimum Distance]:file:_files" "-CLASSIFY_MAHALONOBIS[<str>Mahalonobis Distance]:file:_files" "-CLASSIFY_MAXLIKE[<str> Maximum Likelihood]:file:_files" "-CLASSIFY_SAM[<str> Spectral Angle Mapping]:file:_files" "-CLASSIFY_BAYES[<str> Normal Bayes]:file:_files" "-CLASSIFY_DT[<str> Decision Tree]:file:_files" "-CLASSIFY_RF[<str> Random Forest]:file:_files" "-CLASSIFY_SVM[<str> Support Vector Machine]:file:_files" "-CLASSIFY_KNN[<str> K-Nearest Neighbours]:file:_files" "-CLASSIFY_ANN[<str> Artificial Neural Network]:file:_files"  
          ;;
     esac
     ;;
   esac
}

