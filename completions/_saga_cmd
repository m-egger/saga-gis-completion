            "-PG_HOST[<str>Host]:file:_files" "-PG_PORT[<num>Port]:file:_files" "-PG_DB[<str> Database]:file:_files" "-PG_USER[<str>User]:file:_files" "-PG_PWD[<str> Password]:file:_files" "-TABLES[<str> Tables]:file:_files"  
          ;;
