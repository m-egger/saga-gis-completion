        8)
          _arguments \
            "-DEPENDENT[<str> Dependent Variable]:file:_files" "-PREDICTORS[<str>Predictors]:file:_files" "-REGRESSION[<str>Regression]:file:_files" "-RESIDUALS[<str> Residuals]:file:_files" "-INFO_COEFF[<str>Details[ Coefficients]:file:_files" "-INFO_MODEL[<str>Details[ Model]:file:_files" "-INFO_STEPS[<str>Details[ Steps]:file:_files" "-RESAMPLING[<str>Resampling]:file:_files" "-COORD_X[<str> Include X Coordinate]:file:_files" "-COORD_Y[<str> Include Y Coordinate]:file:_files" "-METHOD[<str> Method]:file:_files" "-P_VALUE[<double>Significance Level]:file:_files" "-CROSSVAL[<str> Cross Validation]:file:_files" "-CROSSVAL_K[<num>Cross Validation Subsamples]:file:_files"  
          ;;
        9)
          _arguments \
            "-Y_GRIDS[<str>Dependent Variables]:file:_files" "-COEFF[<str> Polynomial Coefficients]:file:_files" "-R[<str> Regression Coefficient]:file:_files" "-R2[<str> Determination Coefficient]:file:_files" "-R2ADJ[<str> Adjusted Determination Coefficient]:file:_files" "-P[<str> Significance Level]:file:_files" "-STDERR[<str> Standard Error]:file:_files" "-LINEAR[<str> Linear Trend]:file:_files" "-ORDER[<num> Polynomial Order]:file:_files" "-XSOURCE[<str>Get Independent Variable from ...]:file:_files" "-X_TABLE[<str>Independent Variable (per Grid)]:file:_files" "-X_GRIDS[<str>Independent Variable (per Grid and Cell)]:file:_files"  
          ;;
        10)
          _arguments \
            "-TABLE[<str> Table]:file:_files" "-FIELD_X[<str> X Values]:file:_files" "-FIELD_Y[<str> Y Values]:file:_files" "-FORMULA[<str> Formula]:file:_files" "-FORMULAS[<str>Pre-defined Formulas]:file:_files" "-TREND[<str> Table (with Trend)]:file:_files"  
          ;;
        11)
          _arguments \
            "-TABLE[<str> Shapes]:file:_files" "-FIELD_X[<str> X Values]:file:_files" "-FIELD_Y[<str> Y Values]:file:_files" "-FORMULA[<str> Formula]:file:_files" "-FORMULAS[<str>Pre-defined Formulas]:file:_files" "-TREND[<str> Table (with Trend)]:file:_files"  
          ;;
        12)
          _arguments \
            "-TABLE[<str> Table]:file:_files" "-RESULTS[<str> Results]:file:_files" "-DEPENDENT[<str> Dependent Variable]:file:_files" "-INFO_COEFF[<str>Details[ Coefficients]:file:_files" "-INFO_MODEL[<str>Details[ Model]:file:_files" "-INFO_STEPS[<str>Details[ Steps]:file:_files" "-METHOD[<str> Method]:file:_files" "-P_VALUE[<double>Significance Level]:file:_files" "-CROSSVAL[<str> Cross Validation]:file:_files" "-CROSSVAL_K[<num>Cross Validation Subsamples]:file:_files"  
          ;;
        13)
          _arguments \
            "-TABLE[<str> Shapes]:file:_files" "-RESULTS[<str> Results]:file:_files" "-DEPENDENT[<str> Dependent Variable]:file:_files" "-INFO_COEFF[<str>Details[ Coefficients]:file:_files" "-INFO_MODEL[<str>Details[ Model]:file:_files" "-INFO_STEPS[<str>Details[ Steps]:file:_files" "-METHOD[<str> Method]:file:_files" "-P_VALUE[<double>Significance Level]:file:_files" "-CROSSVAL[<str> Cross Validation]:file:_files" "-CROSSVAL_K[<num>Cross Validation Subsamples]:file:_files"  
          ;;
        14)
          _arguments \
            "-PREDICTORS[<str> Predictors]:file:_files" "-REGRESSION[<str> Regression]:file:_files" "-REG_RESCORR[<str> Regression with Residual Correction]:file:_files" "-DEPENDENT[<str> Dependent Variable]:file:_files" "-QUALITY[<str> Coefficient of Determination]:file:_files" "-RESIDUALS[<str> Residuals]:file:_files" "-MODEL[<str> Regression Parameters]:file:_files" "-LOGISTIC[<str> Logistic Regression]:file:_files" "-MODEL_OUT[<str> Output of Model Parameters]:file:_files" "-SEARCH_RANGE[<str> Search Range]:file:_files" "-SEARCH_RADIUS[<num> Search Distance (Cells)]:file:_files" "-DW_WEIGHTING[<str> Weighting Function]:file:_files" "-DW_IDW_POWER[<double>Power]:file:_files" "-DW_BANDWIDTH[<double>Bandwidth]:file:_files"  
          ;;
        15)
          _arguments \
            "-PREDICTORS[<str>Predictors]:file:_files" "-ZONES[<str> Zones]:file:_files" "-POINTS[<str> Points]:file:_files" "-ATTRIBUTE[<str> Dependent Variable]:file:_files" "-RESIDUALS[<str> Residuals]:file:_files" "-REGRESSION[<str>Regression]:file:_files" "-RESAMPLING[<str>Resampling]:file:_files" "-COORD_X[<str> Include X Coordinate]:file:_files" "-COORD_Y[<str> Include Y Coordinate]:file:_files" "-INTERCEPT[<str> Intercept]:file:_files" "-METHOD[<str> Method]:file:_files" "-P_VALUE[<double>Significance Level]:file:_files"  
          ;;
