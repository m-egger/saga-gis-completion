   esac
}

_ta_lighting(){
  _arguments -C "1: :->cmds" "*::arg:->args" 
 
  case "$state" in
    cmds)
      _values "ta_lighting command" \
        "0[Analytical Hillshading]" \
        "2[Potential Incoming Solar Radiation]" \
        "3[Sky View Factor]" \
        "5[Topographic Openness]" \
        "6[Visibility Analysis]" \
        "7[Potential Annual Insolation]" \
        "8[Geomorphons]" \

      ;;
    args)
      case "$line[1]" in
        0)
          _arguments \
            "-ELEVATION[<str> Elevation]:file:_files" "-SHADE[<str> Analytical Hillshading]:file:_files" "-METHOD[<str> Shading Method]:file:_files" "-POSITION[<str> Sun's Position]:file:_files" "-AZIMUTH[<double> Azimuth]:file:_files" "-DECLINATION[<double> Height]:file:_files" "-DATE[<date> Day]:file:_files" "-TIME[<double> Hour]:file:_files" "-EXAGGERATION[<double>Exaggeration]:file:_files" "-UNIT[<str> Unit]:file:_files" "-SHADOW[<str> Shadow]:file:_files" "-NDIRS[<num> Number of Directions]:file:_files" "-RADIUS[<double> Search Radius]:file:_files"  
          ;;
        2)
          _arguments \
            "-GRD_DEM[<str> Elevation]:file:_files" "-GRD_SVF[<str> Sky View Factor]:file:_files" "-GRD_VAPOUR[<str> Water Vapour Pressure (mbar)]:file:_files" "-GRD_VAPOUR_DEFAULT[<double>Default]:file:_files" "-GRD_LINKE[<str> Linke Turbidity Coefficient]:file:_files" "-GRD_LINKE_DEFAULT[<double> Default]:file:_files" "-GRD_DIRECT[<str> Direct Insolation]:file:_files" "-GRD_DIFFUS[<str> Diffuse Insolation]:file:_files" "-GRD_TOTAL[<str> Total Insolation]:file:_files" "-GRD_RATIO[<str> Direct to Diffuse Ratio]:file:_files" "-GRD_FLAT[<str> Compare to Flat Terrain]:file:_files" "-GRD_DURATION[<str> Duration of Insolation]:file:_files" "-GRD_SUNRISE[<str> Sunrise]:file:_files" "-GRD_SUNSET[<str> Sunset]:file:_files" "-SOLARCONST[<double> Solar Constant (W/m)]:file:_files" "-LOCALSVF[<str> Local Sky View Factor]:file:_files" "-UNITS[<str> Units]:file:_files" "-SHADOW[<str> Shadow]:file:_files" "-LOCATION[<str> Location]:file:_files" "-LATITUDE[<double> Latitude]:file:_files" "-PERIOD[<str> Time Period]:file:_files" "-DAY[<date> Day]:file:_files" "-DAY_STOP[<date> Last Day]:file:_files" "-DAYS_STEP[<num> Resolution (d)]:file:_files" "-MOMENT[<double> Moment (h)]:file:_files" "-HOUR_RANGE_MIN[<double> Time Span (h)]:file:_files" "-HOUR_RANGE_MAX[<double> Time Span (h)]:file:_files" "-HOUR_STEP[<double> Resolution (h)]:file:_files" "-METHOD[<str> Atmospheric Effects]:file:_files" "-ATMOSPHERE[<double> Height of Atmosphere (m)]:file:_files" "-PRESSURE[<double> Barometric Pressure (mbar)]:file:_files" "-WATER[<double> Water Content (cm)]:file:_files" "-DUST[<double> Dust (ppm)]:file:_files" "-LUMPED[<double> Lumped Atmospheric Transmittance (Percent)]:file:_files"  
          ;;
        3)
          _arguments \
            "-DEM[<str> Elevation]:file:_files" "-VISIBLE[<str> Visible Sky]:file:_files" "-SVF[<str> Sky View Factor]:file:_files" "-SIMPLE[<str> Sky View Factor (Simplified)]:file:_files" "-TERRAIN[<str> Terrain View Factor]:file:_files" "-DISTANCE[<str> Average View Distance]:file:_files" "-RADIUS[<double>Maximum Search Radius]:file:_files" "-NDIRS[<num> Number of Sectors]:file:_files" "-METHOD[<str> Method]:file:_files" "-DLEVEL[<double>Multi Scale Factor]:file:_files"  
          ;;
        5)
          _arguments \
            "-DEM[<str> Elevation]:file:_files" "-POS[<str> Positive Openness]:file:_files" "-NEG[<str> Negative Openness]:file:_files" "-RADIUS[<double> Radial Limit]:file:_files" "-DIRECTIONS[<str> Directions]:file:_files" "-DIRECTION[<double>Direction]:file:_files" "-NDIRS[<num> Number of Sectors]:file:_files" "-METHOD[<str> Method]:file:_files" "-DLEVEL[<double> Multi Scale Factor]:file:_files" "-UNIT[<str> Unit]:file:_files" "-NADIR[<str> Difference from Nadir]:file:_files"  
          ;;
        6)
          _arguments \
            "-ELEVATION[<str> Elevation]:file:_files" "-VISIBILITY[<str> Visibility]:file:_files" "-METHOD[<str> Output]:file:_files" "-UNIT[<str> Unit]:file:_files" "-CUMULATIVE[<str> Cumulative]:file:_files" "-NODATA[<str> Ignore No-Data]:file:_files" "-POINTS[<str> Points]:file:_files" "-HEIGHT[<str> Height]:file:_files" "-HEIGHT_DEFAULT[<double>Default]:file:_files"  
          ;;
        7)
          _arguments \
            "-DEM[<str> Elevation]:file:_files" "-INSOLATION[<str> Annual Insolation]:file:_files" "-STEPS[<num> Number of Steps]:file:_files" "-UNITS[<str> Units]:file:_files" "-HOUR_STEP[<double>Resolution (h)]:file:_files" "-YEAR[<num> Reference Year]:file:_files"  
          ;;
        8)
          _arguments \
            "-DEM[<str> Elevation]:file:_files" "-GEOMORPHONS[<str> Geomorphons]:file:_files" "-THRESHOLD[<double>Threshold Angle]:file:_files" "-RADIUS[<double> Radial Limit]:file:_files" "-METHOD[<str> Method]:file:_files" "-DLEVEL[<double> Multi Scale Factor]:file:_files"  
          ;;
     esac
     ;;
   esac
}

_ta_morphometry(){
  _arguments -C "1: :->cmds" "*::arg:->args" 
 
  case "$state" in
    cmds)
      _values "ta_morphometry command" \
        "0[Slope, Aspect, Curvature]" \
        "1[Convergence Index]" \
        "2[Convergence Index (Search Radius)]" \
        "3[Surface Specific Points]" \
        "4[Curvature Classification]" \
        "5[Hypsometry]" \
        "6[Real Surface Area]" \
        "7[Morphometric Protection Index]" \
        "8[Multiresolution Index of Valley Bottom Flatness (MRVBF)]" \
        "9[Downslope Distance Gradient]" \
        "10[Mass Balance Index]" \
        "11[Effective Air Flow Heights]" \
        "12[Diurnal Anisotropic Heat]" \
        "13[Land Surface Temperature (Lapse Rates)]" \
        "14[Relative Heights and Slope Positions]" \
        "15[Wind Effect (Windward / Leeward Index)]" \
        "16[Terrain Ruggedness Index (TRI)]" \
        "17[Vector Ruggedness Measure (VRM)]" \
        "18[Topographic Position Index (TPI)]" \
        "19[TPI Based Landform Classification]" \
        "20[Terrain Surface Texture]" \
        "21[Terrain Surface Convexity]" \
        "22[Terrain Surface Classification (Iwahashi and Pike)]" \
        "23[Morphometric Features]" \
        "24[Valley and Ridge Detection (Top Hat Approach)]" \
        "25[Fuzzy Landform Element Classification]" \
        "26[Upslope and Downslope Curvature]" \
        "27[Wind Exposition Index]" \
        "28[Multi-Scale Topographic Position Index (TPI)]" \
        "29[Wind Shelter Index]" \
        "clustering[Terrain Clustering]" \
        "segmentation[Terrain Segmentation]" \
        "summits[Summit Extraction]" \

      ;;
    args)
      case "$line[1]" in
        0)
          _arguments \
            "-ELEVATION[<str> Elevation]:file:_files" "-SLOPE[<str> Slope]:file:_files" "-ASPECT[<str> Aspect]:file:_files" "-C_GENE[<str> General Curvature]:file:_files" "-C_PROF[<str> Profile Curvature]:file:_files" "-C_PLAN[<str> Plan Curvature]:file:_files" "-C_TANG[<str> Tangential Curvature]:file:_files" "-C_LONG[<str> Longitudinal Curvature]:file:_files" "-C_CROS[<str> Cross-Sectional Curvature]:file:_files" "-C_MINI[<str> Minimal Curvature]:file:_files" "-C_MAXI[<str> Maximal Curvature]:file:_files" "-C_TOTA[<str> Total Curvature]:file:_files" "-C_ROTO[<str> Flow Line Curvature]:file:_files" "-METHOD[<str> Method]:file:_files" "-UNIT_SLOPE[<str> Unit]:file:_files" "-UNIT_ASPECT[<str>Unit]:file:_files"  
          ;;
        1)
          _arguments \
            "-ELEVATION[<str> Elevation]:file:_files" "-RESULT[<str> Convergence Index]:file:_files" "-METHOD[<str> Method]:file:_files" "-NEIGHBOURS[<str>Gradient Calculation]:file:_files"  
          ;;
        2)
          _arguments \
            "-ELEVATION[<str> Elevation]:file:_files" "-CONVERGENCE[<str> Convergence Index]:file:_files" "-SLOPE[<str> Gradient]:file:_files" "-DIFFERENCE[<str> Difference]:file:_files" "-RADIUS[<double> Radius (Cells)]:file:_files" "-DW_WEIGHTING[<str> Weighting Function]:file:_files" "-DW_IDW_POWER[<double>Power]:file:_files" "-DW_BANDWIDTH[<double>Bandwidth]:file:_files"  
          ;;
        3)
          _arguments \
            "-ELEVATION[<str> Elevation]:file:_files" "-RESULT[<str> Result]:file:_files" "-METHOD[<str> Method]:file:_files" "-THRESHOLD[<double>Threshold]:file:_files"  
          ;;
        4)
          _arguments \
            "-DEM[<str> Elevation]:file:_files" "-CLASSES[<str> Curvature Classification]:file:_files" "-STRAIGHT[<double>Threshold Radius]:file:_files" "-VERTICAL[<str> Vertical Curvature]:file:_files" "-HORIZONTAL[<str> Horizontal Curvature]:file:_files" "-SMOOTH[<num> Smoothing]:file:_files"  
          ;;
        5)
          _arguments \
            "-ELEVATION[<str> Elevation]:file:_files" "-TABLE[<str> Hypsometry]:file:_files" "-COUNT[<num> Number of Classes]:file:_files" "-SORTING[<str> Sort]:file:_files" "-METHOD[<str> Classification Constant]:file:_files" "-BZRANGE[<str> Use Z-Range]:file:_files" "-ZRANGE_MIN[<double>Z-Range]:file:_files" "-ZRANGE_MAX[<double>Z-Range]:file:_files"  
          ;;
        6)
          _arguments \
            "-DEM[<str> Elevation]:file:_files" "-AREA[<str>Surface Area]:file:_files"  
          ;;
        7)
          _arguments \
            "-DEM[<str> Elevation]:file:_files" "-PROTECTION[<str>Protection Index]:file:_files" "-RADIUS[<double> Radius]:file:_files"  
          ;;
        8)
          _arguments \
            "-DEM[<str> Elevation]:file:_files" "-MRVBF[<str> MRVBF]:file:_files" "-MRRTF[<str> MRRTF]:file:_files" "-T_SLOPE[<double> Initial Threshold for Slope]:file:_files" "-T_PCTL_V[<double>Threshold for Elevation Percentile (Lowness)]:file:_files" "-T_PCTL_R[<double>Threshold for Elevation Percentile (Upness)]:file:_files" "-P_SLOPE[<double> Shape Parameter for Slope]:file:_files" "-P_PCTL[<double> Shape Parameter for Elevation Percentile]:file:_files" "-UPDATE[<str> Update Views]:file:_files" "-CLASSIFY[<str> Classify]:file:_files" "-MAX_RES[<double> Maximum Resolution (Percentage)]:file:_files"  
          ;;
        9)
          _arguments \
            "-DEM[<str> Elevation]:file:_files" "-GRADIENT[<str> Gradient]:file:_files" "-DIFFERENCE[<str> Gradient Difference]:file:_files" "-DISTANCE[<double>Vertical Distance]:file:_files" "-OUTPUT[<str> Output]:file:_files"  
          ;;
        10)
          _arguments \
            "-DEM[<str> Elevation]:file:_files" "-HREL[<str> Vertical Distance to Channel Network]:file:_files" "-MBI[<str> Mass Balance Index]:file:_files" "-TSLOPE[<double>T Slope]:file:_files" "-TCURVE[<double>T Curvature]:file:_files" "-THREL[<double> T Vertical Distance to Channel Network]:file:_files"  
          ;;
        11)
          _arguments \
            "-DEM[<str> Elevation]:file:_files" "-DIR[<str> Wind Direction]:file:_files" "-DIR_UNITS[<str> Wind Direction Units]:file:_files" "-LEN[<str> Wind Speed]:file:_files" "-LEN_SCALE[<double>Scaling]:file:_files" "-AFH[<str> Effective Air Flow Heights]:file:_files" "-MAXDIST[<double> Search Distance (km)]:file:_files" "-DIR_CONST[<double>Constant Wind Direction]:file:_files" "-OLDVER[<str> Old Version]:file:_files" "-ACCEL[<double> Acceleration]:file:_files" "-PYRAMIDS[<str> Elevation Averaging]:file:_files" "-LEE[<double> Windward Factor]:file:_files" "-LUV[<double> Luv Factor]:file:_files"  
          ;;
        12)
          _arguments \
            "-DEM[<str> Elevation]:file:_files" "-DAH[<str> Diurnal Anisotropic Heating]:file:_files" "-ALPHA_MAX[<double>Alpha Max (Degree)]:file:_files"  
          ;;
        13)
          _arguments \
            "-DEM[<str> Elevation]:file:_files" "-SWR[<str> Short Wave Radiation Ratio]:file:_files" "-LAI[<str> Leaf Area Index]:file:_files" "-LAI_MAX[<double> Maximum LAI]:file:_files" "-LST[<str> Land Surface Temperature]:file:_files" "-Z_REFERENCE[<double>Elevation]:file:_files" "-T_REFERENCE[<double>Temperature]:file:_files" "-T_GRADIENT[<double> Temperature Lapse Rate]:file:_files" "-C_FACTOR[<double> C Factor]:file:_files"  
          ;;
        14)
          _arguments \
            "-DEM[<str> Elevation]:file:_files" "-HO[<str> Slope Height]:file:_files" "-HU[<str> Valley Depth]:file:_files" "-NH[<str> Normalized Height]:file:_files" "-SH[<str> Standardized Height]:file:_files" "-MS[<str> Mid-Slope Positon]:file:_files" "-W[<double>w]:file:_files" "-T[<double>t]:file:_files" "-E[<double>e]:file:_files"  
          ;;
        15)
          _arguments \
            "-DEM[<str> Elevation]:file:_files" "-DIR[<str> Wind Direction]:file:_files" "-DIR_UNITS[<str> Wind Direction Units]:file:_files" "-LEN[<str> Wind Speed]:file:_files" "-LEN_SCALE[<double>Scaling]:file:_files" "-EFFECT[<str> Wind Effect]:file:_files" "-AFH[<str> Effective Air Flow Heights]:file:_files" "-MAXDIST[<double> Search Distance (km)]:file:_files" "-DIR_CONST[<double>Constant Wind Direction]:file:_files" "-OLDVER[<str> Old Version]:file:_files" "-ACCEL[<double> Acceleration]:file:_files" "-PYRAMIDS[<str> Elevation Averaging]:file:_files"  
          ;;
        16)
          _arguments \
            "-DEM[<str> Elevation]:file:_files" "-TRI[<str> Terrain Ruggedness Index (TRI)]:file:_files" "-MODE[<str> Search Mode]:file:_files" "-RADIUS[<num> Search Radius]:file:_files" "-DW_WEIGHTING[<str> Weighting Function]:file:_files" "-DW_IDW_POWER[<double>Power]:file:_files" "-DW_BANDWIDTH[<double>Bandwidth]:file:_files"  
          ;;
        17)
          _arguments \
            "-DEM[<str> Elevation]:file:_files" "-VRM[<str> Vector Terrain Ruggedness (VRM)]:file:_files" "-MODE[<str> Search Mode]:file:_files" "-RADIUS[<num> Search Radius]:file:_files" "-DW_WEIGHTING[<str> Weighting Function]:file:_files" "-DW_IDW_POWER[<double>Power]:file:_files" "-DW_BANDWIDTH[<double>Bandwidth]:file:_files"  
          ;;
        18)
          _arguments \
            "-DEM[<str> Elevation]:file:_files" "-TPI[<str> Topographic Position Index]:file:_files" "-STANDARD[<str> Standardize]:file:_files" "-RADIUS_MIN[<double> Scale]:file:_files" "-RADIUS_MAX[<double> Scale]:file:_files" "-DW_WEIGHTING[<str> Weighting Function]:file:_files" "-DW_IDW_POWER[<double>Power]:file:_files" "-DW_BANDWIDTH[<double>Bandwidth]:file:_files"  
          ;;
        19)
          _arguments \
            "-DEM[<str> Elevation]:file:_files" "-LANDFORMS[<str> Landforms]:file:_files" "-RADIUS_A_MIN[<double>Small Scale]:file:_files" "-RADIUS_A_MAX[<double>Small Scale]:file:_files" "-RADIUS_B_MIN[<double>Large Scale]:file:_files" "-RADIUS_B_MAX[<double>Large Scale]:file:_files" "-DW_WEIGHTING[<str> Weighting Function]:file:_files" "-DW_IDW_POWER[<double>Power]:file:_files" "-DW_BANDWIDTH[<double>Bandwidth]:file:_files"  
          ;;
        20)
          _arguments \
            "-DEM[<str> Elevation]:file:_files" "-TEXTURE[<str> Texture]:file:_files" "-EPSILON[<double> Flat Area Threshold]:file:_files" "-SCALE[<num> Scale (Cells)]:file:_files" "-METHOD[<str> Method]:file:_files" "-DW_WEIGHTING[<str> Weighting Function]:file:_files" "-DW_IDW_POWER[<double>Power]:file:_files" "-DW_BANDWIDTH[<double>Bandwidth]:file:_files"  
          ;;
        21)
          _arguments \
            "-DEM[<str> Elevation]:file:_files" "-CONVEXITY[<str> Convexity]:file:_files" "-KERNEL[<str> Laplacian Filter Kernel]:file:_files" "-TYPE[<str> Type]:file:_files" "-EPSILON[<double> Flat Area Threshold]:file:_files" "-SCALE[<num> Scale (Cells)]:file:_files" "-METHOD[<str> Method]:file:_files" "-DW_WEIGHTING[<str> Weighting Function]:file:_files" "-DW_IDW_POWER[<double>Power]:file:_files" "-DW_BANDWIDTH[<double>Bandwidth]:file:_files"  
          ;;
        22)
          _arguments \
            "-DEM[<str> Elevation]:file:_files" "-SLOPE[<str> Slope]:file:_files" "-CONVEXITY[<str> Convexity]:file:_files" "-CONV_RECALC[<str> Recalculate]:file:_files" "-TEXTURE[<str> Texture]:file:_files" "-TEXT_RECALC[<str> Recalculate]:file:_files" "-LANDFORMS[<str> Landforms]:file:_files" "-TYPE[<str> Number of Classes]:file:_files" "-CONV_SCALE[<num> Scale (Cells)]:file:_files" "-CONV_KERNEL[<str> Laplacian Filter Kernel]:file:_files" "-CONV_TYPE[<str> Type]:file:_files" "-CONV_EPSILON[<double>Flat Area Threshold]:file:_files" "-TEXT_SCALE[<num> Scale (Cells)]:file:_files" "-TEXT_EPSILON[<double>Flat Area Threshold]:file:_files"  
          ;;
        23)
          _arguments \
            "-DEM[<str> Elevation]:file:_files" "-FEATURES[<str> Morphometric Features]:file:_files" "-ELEVATION[<str> Generalized Surface]:file:_files" "-SLOPE[<str> Slope]:file:_files" "-ASPECT[<str> Aspect]:file:_files" "-PROFC[<str> Profile Curvature]:file:_files" "-PLANC[<str> Plan Curvature]:file:_files" "-LONGC[<str> Longitudinal Curvature]:file:_files" "-CROSC[<str> Cross-Sectional Curvature]:file:_files" "-MAXIC[<str> Maximum Curvature]:file:_files" "-MINIC[<str> Minimum Curvature]:file:_files" "-SIZE[<num> Scale Radius (Cells)]:file:_files" "-TOL_SLOPE[<double>Slope Tolerance]:file:_files" "-TOL_CURVE[<double>Curvature Tolerance]:file:_files" "-EXPONENT[<double> Distance Weighting Exponent]:file:_files" "-ZSCALE[<double> Vertical Scaling]:file:_files" "-CONSTRAIN[<str> Constrain]:file:_files"  
          ;;
        24)
          _arguments \
            "-DEM[<str> Elevation]:file:_files" "-VALLEY[<str> Valley Depth]:file:_files" "-HILL[<str> Hill Height]:file:_files" "-VALLEY_IDX[<str> Valley Index]:file:_files" "-HILL_IDX[<str> Hill Index]:file:_files" "-SLOPE_IDX[<str> Hillslope Index]:file:_files" "-RADIUS_VALLEY[<double>Valley Radius]:file:_files" "-RADIUS_HILL[<double> Hill Radius]:file:_files" "-THRESHOLD[<double> Elevation Threshold]:file:_files" "-METHOD[<str> Slope Index]:file:_files"  
          ;;
        25)
          _arguments \
            "-ELEVATION[<str> Elevation]:file:_files" "-SLOPE[<str> Slope]:file:_files" "-MINCURV[<str> Minimum Curvature]:file:_files" "-MAXCURV[<str> Maximum Curvature]:file:_files" "-PCURV[<str> Profile Curvature]:file:_files" "-TCURV[<str> Tangential Curvature]:file:_files" "-FORM[<str> Landform]:file:_files" "-MEM[<str> Maximum Membership]:file:_files" "-ENTROPY[<str> Entropy]:file:_files" "-CI[<str> Confusion Index]:file:_files" "-PLAIN[<str> Plain]:file:_files" "-PIT[<str> Pit]:file:_files" "-PEAK[<str> Peak]:file:_files" "-RIDGE[<str> Ridge]:file:_files" "-CHANNEL[<str> Channel]:file:_files" "-SADDLE[<str> Saddle]:file:_files" "-BSLOPE[<str> Back Slope]:file:_files" "-FSLOPE[<str> Foot Slope]:file:_files" "-SSLOPE[<str> Shoulder Slope]:file:_files" "-HOLLOW[<str> Hollow]:file:_files" "-FHOLLOW[<str> Foot Hollow]:file:_files" "-SHOLLOW[<str> Shoulder Hollow]:file:_files" "-SPUR[<str> Spur]:file:_files" "-FSPUR[<str> Foot Spur]:file:_files" "-SSPUR[<str> Shoulder Spur]:file:_files" "-INPUT[<str> Input]:file:_files" "-MEMBERSHIP[<str> Memberships]:file:_files" "-SLOPETODEG[<str> Slope Grid Units]:file:_files" "-T_SLOPE_MIN[<double>Slope Thresholds (Degree)]:file:_files" "-T_SLOPE_MAX[<double>Slope Thresholds (Degree)]:file:_files" "-T_CURVE_MIN[<double>Curvature Thresholds (1000 / m)]:file:_files" "-T_CURVE_MAX[<double>Curvature Thresholds (1000 / m)]:file:_files"  
          ;;
        26)
          _arguments \
            "-DEM[<str> Elevation]:file:_files" "-C_LOCAL[<str> Local Curvature]:file:_files" "-C_UP[<str> Upslope Curvature]:file:_files" "-C_UP_LOCAL[<str> Local Upslope Curvature]:file:_files" "-C_DOWN[<str> Downslope Curvature]:file:_files" "-C_DOWN_LOCAL[<str>Local Downslope Curvature]:file:_files" "-WEIGHTING[<double>Upslope Weighting]:file:_files"  
          ;;
        27)
          _arguments \
            "-DEM[<str> Elevation]:file:_files" "-EXPOSITION[<str>Wind Exposition]:file:_files" "-MAXDIST[<double>Search Distance (km)]:file:_files" "-STEP[<double> Angular Step Size (Degree)]:file:_files" "-OLDVER[<str> Old Version]:file:_files" "-ACCEL[<double> Acceleration]:file:_files" "-PYRAMIDS[<str> Elevation Averaging]:file:_files"  
          ;;
        28)
          _arguments \
            "-DEM[<str> Elevation]:file:_files" "-TPI[<str> Topographic Position Index]:file:_files" "-SCALE_MIN[<num>Minimum Scale]:file:_files" "-SCALE_MAX[<num>Maximum Scale]:file:_files" "-SCALE_NUM[<num>Number of Scales]:file:_files"  
          ;;
        29)
          _arguments \
            "-ELEVATION[<str> Elevation]:file:_files" "-SHELTER[<str> Wind Shelter Index]:file:_files" "-UNIT[<str> Unit]:file:_files" "-DIRECTION[<double>Wind Direction]:file:_files" "-TOLERANCE[<double>Tolerance]:file:_files" "-DISTANCE[<double> Distance]:file:_files" "-QUANTILE[<double> Quantile]:file:_files" "-NEGATIVES[<str> Include Negative Slopes]:file:_files" "-METHOD[<str> Method]:file:_files"  
          ;;
        clustering)
          _arguments \
            "-ELEVATION[<str>Elevation]:file:_files" "-CLUSTER[<str> Clusters]:file:_files" "-NCLUSTER[<num> Number of Terrain Classes]:file:_files" "-MAXITER[<num> Maximum Iterations]:file:_files"  
          ;;
        segmentation)
          _arguments \
            "-ELEVATION[<str> Elevation]:file:_files" "-OBJECTS[<str> Objects]:file:_files" "-TPI_RADIUS_MIN[<double>Position Index Radius]:file:_files" "-TPI_RADIUS_MAX[<double>Position Index Radius]:file:_files" "-BAND_WIDTH[<double> Band Width]:file:_files" "-NCLUSTER[<num> Number of Clusters]:file:_files"  
          ;;
        summits)
          _arguments \
            "-SURFACE[<str> Surface]:file:_files" "-SUMMITS[<str> Summits]:file:_files" "-METHOD[<str> Trend Surface]:file:_files" "-SCALE_FILTER[<double>Scale]:file:_files" "-SCALE_TPI[<double> Scale]:file:_files" "-THRESHOLD[<double> Threshold]:file:_files"  
          ;;
     esac
     ;;
   esac
}

_ta_preprocessor(){
  _arguments -C "1: :->cmds" "*::arg:->args" 
 
  case "$state" in
    cmds)
      _values "ta_preprocessor command" \
        "0[Flat Detection]" \
        "1[Sink Drainage Route Detection]" \
        "2[Sink Removal]" \
        "3[Fill Sinks (Planchon/Darboux, 2001)]" \
        "4[Fill Sinks (Wang & Liu)]" \
        "5[Fill Sinks XXL (Wang & Liu)]" \
        "6[Burn Stream Network into DEM]" \
        "7[Breach Depressions]" \
        "8[Fill Minima]" \

      ;;
    args)
      case "$line[1]" in
        0)
          _arguments \
            "-DEM[<str> DEM]:file:_files" "-NOFLATS[<str> No Flats]:file:_files" "-FLATS[<str> Flat Areas]:file:_files" "-FLAT_OUTPUT[<str> Flat Area Values]:file:_files" "-NEIGHBOURHOOD[<str>Neighbourhood]:file:_files"  
          ;;
        1)
          _arguments \
            "-ELEVATION[<str> Elevation]:file:_files" "-SINKROUTE[<str> Sink Route]:file:_files" "-THRESHOLD[<str> Threshold]:file:_files" "-THRSHEIGHT[<double>Threshold Height]:file:_files"  
          ;;
        2)
          _arguments \
            "-DEM[<str> DEM]:file:_files" "-SINKROUTE[<str> Sink Route]:file:_files" "-DEM_PREPROC[<str> Preprocessed DEM]:file:_files" "-METHOD[<str> Method]:file:_files" "-THRESHOLD[<str> Threshold]:file:_files" "-THRSHEIGHT[<double>Threshold Height]:file:_files" "-EPSILON[<double> Epsilon]:file:_files"  
          ;;
        3)
          _arguments \
            "-DEM[<str> DEM]:file:_files" "-RESULT[<str> Filled DEM]:file:_files" "-MINSLOPE[<double>Minimum Slope (Degree)]:file:_files"  
          ;;
        4)
          _arguments \
            "-ELEV[<str> DEM]:file:_files" "-FILLED[<str> Filled DEM]:file:_files" "-FDIR[<str> Flow Directions]:file:_files" "-WSHED[<str> Watershed Basins]:file:_files" "-MINSLOPE[<double>Minimum Slope (Degree)]:file:_files"  
          ;;
        5)
          _arguments \
            "-ELEV[<str> DEM]:file:_files" "-FILLED[<str> Filled DEM]:file:_files" "-MINSLOPE[<double>Minimum Slope (Degree)]:file:_files"  
          ;;
        6)
          _arguments \
            "-DEM[<str> DEM]:file:_files" "-BURN[<str> Processed DEM]:file:_files" "-STREAM[<str> Streams]:file:_files" "-FLOWDIR[<str> Flow Direction]:file:_files" "-METHOD[<str> Method]:file:_files" "-EPSILON[<double>Epsilon]:file:_files"  
          ;;
        7)
          _arguments \
            "-DEM[<str> Elevation]:file:_files" "-NOSINKS[<str> Preprocessed]:file:_files" "-MAX_LENGTH[<num> Maximum Breach Channel Length]:file:_files" "-MAX_ZDEC[<double> Maximum Elevation Decrement]:file:_files" "-MIN_ZDROP[<double>Minimum Elevation Drop]:file:_files"  
          ;;
        8)
          _arguments \
            "-DEM[<str> DEM]:file:_files" "-RESULT[<str>Filled DEM]:file:_files"  
          ;;
     esac
     ;;
   esac
}

_ta_profiles(){
  _arguments -C "1: :->cmds" "*::arg:->args" 
 
  case "$state" in
    cmds)
      _values "ta_profiles command" \
        "3[Cross Profiles]" \
        "4[Profiles from Lines]" \
        "5[Profile from Points]" \

      ;;
    args)
      case "$line[1]" in
        3)
          _arguments \
            "-DEM[<str> Elevation]:file:_files" "-LINES[<str> Lines]:file:_files" "-PROFILES[<str> Cross Profiles]:file:_files" "-DIST_LINE[<double> Profile Distance]:file:_files" "-DIST_PROFILE[<double>Profile Length]:file:_files" "-NUM_PROFILE[<num> Profile Samples]:file:_files" "-INTERPOLATION[<str> Interpolation]:file:_files" "-OUTPUT[<str> Output]:file:_files"  
          ;;
        4)
          _arguments \
            "-DEM[<str> Elevation]:file:_files" "-VALUES[<str> Values]:file:_files" "-LINES[<str> Lines]:file:_files" "-NAME[<str> Name]:file:_files" "-PROFILE[<str> Profiles]:file:_files" "-PROFILES[<str>Profiles]:file:_files" "-SPLIT[<str> Each Line as new Profile]:file:_files"  
          ;;
        5)
          _arguments \
            "-GRID[<str> Elevation]:file:_files" "-VALUES[<str>Values]:file:_files" "-TABLE[<str> Coordinates Table]:file:_files" "-X[<str> X Coordinate]:file:_files" "-Y[<str> Y Coordinate]:file:_files" "-RESULT[<str>Profile]:file:_files"  
          ;;
     esac
     ;;
   esac
}

_ta_slope_stability(){
  _arguments -C "1: :->cmds" "*::arg:->args" 
 
  case "$state" in
    cmds)
      _values "ta_slope_stability command" \
        "0[SAFETYFACTOR]" \
        "1[TOBIA]" \
        "2[SHALSTAB]" \
        "3[WETNESS]" \
        "4[WEDGEFAIL]" \
        "5[ANGMAP]" \

      ;;
    args)
      case "$line[1]" in
        0)
          _arguments \
            "-A[<str> Slope grid (rad)]:file:_files" "-Bmin[<str> Min thickness grid (m) ]:file:_files" "-Bmax[<str> Max thickness grid (m) ]:file:_files" "-Cmin[<str> Min saturation grid (-) ]:file:_files" "-Cmax[<str> Max saturation grid (-) ]:file:_files" "-Dmin[<str> Min friction grid (degree) ]:file:_files" "-Dmax[<str> Max friction grid (degree) ]:file:_files" "-Emin[<str> Min density grid (g/cm3)]:file:_files" "-Emax[<str> Max density grid (g/cm3)]:file:_files" "-Fmin[<str> Min cohesion grid (MPa) ]:file:_files" "-Fmax[<str> Max cohesion grid (MPa) ]:file:_files" "-fBmin[<double>Min global thickness (m)]:file:_files" "-fBmax[<double>Max global thickness (m)]:file:_files" "-fCmin[<double>Min global saturation (-)]:file:_files" "-fCmax[<double>Max global saturation (-)]:file:_files" "-fDmin[<double>Min global friction (degree)]:file:_files" "-fDmax[<double>Max global friction (degree)]:file:_files" "-fEmin[<double>Min global density (g/cm3)]:file:_files" "-fEmax[<double>Max global density (g/cm3)]:file:_files" "-fFmin[<double>Min global cohesion (MPa)]:file:_files" "-fFmax[<double>Max global cohesion (MPa)]:file:_files" "-fI[<num> Parameter sampling runs]:file:_files" "-G[<str> FS values]:file:_files" "-H[<str> FS classes]:file:_files"  
          ;;
        1)
          _arguments \
            "-A[<str> Slope grid (rad)]:file:_files" "-B[<str> Aspect grid (rad)]:file:_files" "-C[<str> Dip grid (degrees) ]:file:_files" "-D[<str> Dip direction grid (degrees) ]:file:_files" "-fB[<double>Global structure dip (degrees)]:file:_files" "-fC[<double>Global structure dip direction (degrees)]:file:_files" "-E[<str> TOBIA classes]:file:_files" "-F[<str> TOBIA index]:file:_files"  
          ;;
        2)
          _arguments \
            "-A[<str> Slope grid (rad)]:file:_files" "-B[<str> Catchment area grid (m2)]:file:_files" "-Cmin[<str> Min Density grid (g/cm3)]:file:_files" "-Cmax[<str> Max Density grid (g/cm3)]:file:_files" "-Dmin[<str> Min Hydraulic conductivity grid (m/hr) ]:file:_files" "-Dmax[<str> Max Hydraulic conductivity grid (m/hr) ]:file:_files" "-Emin[<str> Min Thickness grid (m)]:file:_files" "-Emax[<str> Max Thickness grid (m)]:file:_files" "-Fmin[<str> Min Friction angle grid (degree) ]:file:_files" "-Fmax[<str> Max Friction angle grid (degree) ]:file:_files" "-Jmin[<str> Min Bulk cohesion grid (MPa) ]:file:_files" "-Jmax[<str> Max Bulk cohesion grid (MPa) ]:file:_files" "-fCmin[<double>Global minimum density (g/cm3)]:file:_files" "-fCmax[<double>Global maximum density (g/cm3)]:file:_files" "-fDmin[<double>Global minimum conductivity (m/hr)]:file:_files" "-fDmax[<double>Global maximum conductivity (m/hr)]:file:_files" "-fEmin[<double>Global minimum thickness (m)]:file:_files" "-fEmax[<double>Global maximum thickness (m)]:file:_files" "-fFmin[<double>Global minimum friction angle (degree)]:file:_files" "-fFmax[<double>Global maximum friction angle (degree)]:file:_files" "-fJmin[<double>Global minimum bulk cohesion (MPa)]:file:_files" "-fJmax[<double>Global maximum bulk cohesion (MPa)]:file:_files" "-fK[<num> Number of sampling runs]:file:_files" "-G[<str> CR values]:file:_files" "-H[<str> CR classes]:file:_files"  
          ;;
        3)
          _arguments \
            "-DEM[<str> DEM]:file:_files" "-Cmin[<str> Min hydraulic conductivity grid (m/hr) ]:file:_files" "-Cmax[<str> Max hydraulic conductivity grid (m/hr) ]:file:_files" "-Dmin[<str> Min groundwater recharge grid (m/hr) ]:file:_files" "-Dmax[<str> Max groundwater recharge grid (m/hr) ]:file:_files" "-Emin[<str> Min material depth grid (m)]:file:_files" "-Emax[<str> Max material depth grid (m)]:file:_files" "-fCmin[<double>Min global material conductivity (m/hr)]:file:_files" "-fCmax[<double>Max global material conductivity (m/hr)]:file:_files" "-fDmin[<double>Min global groundwater recharge (m/hr)]:file:_files" "-fDmax[<double>Max global groundwater recharge (m/hr)]:file:_files" "-fEmin[<double>Min global material depth (m)]:file:_files" "-fEmax[<double>Max global material depth (m)]:file:_files" "-fH[<num> Parameter sampling runs]:file:_files" "-F[<str> WI values]:file:_files" "-G[<str> WI classes]:file:_files" "-METHOD[<str> Catchment Area Calculation]:file:_files" "-PREPROC[<str> Preprocessing]:file:_files"  
          ;;
        4)
          _arguments \
            "-DEM[<str> DEM]:file:_files" "-C[<str> Dip/Plunge direction grid (degree) ]:file:_files" "-D[<str> Dip/Plunge grid (degree) ]:file:_files" "-Emin[<str> Min friction angle grid (degree) ]:file:_files" "-Emax[<str> Max friction angle grid (degree) ]:file:_files" "-fC[<double> Global dip/plunge direction (degree)]:file:_files" "-fD[<double> Global dip/plunge (degree)]:file:_files" "-fEmin[<double>Min global friction angle (degree)]:file:_files" "-fEmax[<double>Max global friction angle (degree)]:file:_files" "-ff[<num> Cone radius (degree)]:file:_files" "-F[<str> Failures]:file:_files" "-fI[<num> Parameter sampling runs]:file:_files" "-METHOD[<str> Mode]:file:_files"  
          ;;
        5)
          _arguments \
            "-DEM[<str> Elevation]:file:_files" "-C[<str> Dip grid (degrees) ]:file:_files" "-D[<str> Dip direction grid (degrees) ]:file:_files" "-fB[<double>Global structure dip (degrees)]:file:_files" "-fC[<double>Global structure dip direction (degrees)]:file:_files" "-E[<str> Angle]:file:_files" "-F[<str> CL dipdir]:file:_files" "-G[<str> CL dip]:file:_files"  
          ;;
     esac
     ;;
   esac
}

_tin_tools(){
  _arguments -C "1: :->cmds" "*::arg:->args" 
 
  case "$state" in
    cmds)
      _values "tin_tools command" \
        "0[Grid to TIN]" \
        "1[Grid to TIN (Surface Specific Points)]" \
        "2[Shapes to TIN]" \
        "3[TIN to Shapes]" \
        "4[Gradient]" \
        "5[Flow Accumulation (Trace)]" \
        "6[Flow Accumulation (Parallel)]" \

      ;;
    args)
      case "$line[1]" in
        0)
          _arguments \
            "-GRID[<str> Grid]:file:_files" "-VALUES[<str>Values]:file:_files" "-TIN[<str> TIN]:file:_files"  
          ;;
        1)
          _arguments \
            "-GRID[<str> Grid]:file:_files" "-VALUES[<str> Values]:file:_files" "-TIN[<str> TIN]:file:_files" "-METHOD[<str> Method]:file:_files" "-HIGH[<num> Mark Highest Neighbour]:file:_files" "-FLOW_MIN[<double>Flow Direction]:file:_files" "-FLOW_MAX[<double>Flow Direction]:file:_files" "-PEUCKER[<double> Peucker & Douglas]:file:_files"  
          ;;
        2)
          _arguments \
            "-SHAPES[<str>Shapes]:file:_files" "-TIN[<str> TIN]:file:_files"  
          ;;
        3)
          _arguments \
            "-TIN[<str> TIN]:file:_files" "-POINTS[<str> Points]:file:_files" "-CENTER[<str> Center of Triangles]:file:_files" "-EDGES[<str> Edges]:file:_files" "-TRIANGLES[<str>Triangles]:file:_files" "-POLYGONS[<str> Polygons]:file:_files"  
          ;;
        4)
          _arguments \
            "-TIN[<str> TIN]:file:_files" "-ZFIELD[<str> Z Values]:file:_files" "-GRADIENT[<str>TIN_Gradient]:file:_files" "-DEGREE[<str> Output Unit]:file:_files"  
          ;;
        5)
          _arguments \
            "-DEM[<str> TIN]:file:_files" "-ZFIELD[<str>Z Values]:file:_files" "-FLOW[<str> Flow Accumulation]:file:_files"  
          ;;
        6)
          _arguments \
            "-DEM[<str> TIN]:file:_files" "-ZFIELD[<str>Z Values]:file:_files" "-FLOW[<str> Flow Accumulation]:file:_files" "-METHOD[<str>Method]:file:_files"  
          ;;
     esac
     ;;
   esac
}

__tool_chains_uncategorized(){
  _arguments -C "1: :->cmds" "*::arg:->args" 
 
  case "$state" in
    cmds)
      _values "_tool_chains_uncategorized command" \

      ;;
    args)
      case "$line[1]" in
     esac
     ;;
   esac
}

