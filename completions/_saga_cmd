_grid_calculus_bsl(){
  _arguments -C "1: :->cmds" "*::arg:->args" 
 
  case "$state" in
    cmds)
      _values "grid_calculus_bsl command" \
        "0[BSL]" \
        "1[BSL from File]" \

      ;;
    args)
      case "$line[1]" in
        0)
          _arguments \
            "-PARAMETERS_GRID_SYSTEM_D[<double>Cell Size]:file:_files" "-PARAMETERS_GRID_SYSTEM_X[<double>Lower Left Center Cell X-Coordinate]:file:_files" "-PARAMETERS_GRID_SYSTEM_Y[<double>Lower Left Center Cell Y-Coordinate]:file:_files" "-PARAMETERS_GRID_SYSTEM_NX[<num> Number of Columns]:file:_files" "-PARAMETERS_GRID_SYSTEM_NY[<num> Number of Rows]:file:_files" "-PARAMETERS_GRID_SYSTEM_FILE[<str>Grid File]:file:_files" "-OUTPUT[<str> Output]:file:_files" "-BSL[<str> BSL Script]:file:_files" "-PROGRESS[<str> Show Progress]:file:_files"  
          ;;
        1)
          _arguments \
            "-PARAMETERS_GRID_SYSTEM_D[<double>Cell Size]:file:_files" "-PARAMETERS_GRID_SYSTEM_X[<double>Lower Left Center Cell X-Coordinate]:file:_files" "-PARAMETERS_GRID_SYSTEM_Y[<double>Lower Left Center Cell Y-Coordinate]:file:_files" "-PARAMETERS_GRID_SYSTEM_NX[<num> Number of Columns]:file:_files" "-PARAMETERS_GRID_SYSTEM_NY[<num> Number of Rows]:file:_files" "-PARAMETERS_GRID_SYSTEM_FILE[<str>Grid File]:file:_files" "-OUTPUT[<str> Output]:file:_files" "-BSL[<str> BSL Script]:file:_files" "-PROGRESS[<str> Show Progress]:file:_files"  
          ;;
     esac
     ;;
   esac
}

_grid_calculus(){
  _arguments -C "1: :->cmds" "*::arg:->args" 
 
  case "$state" in
    cmds)
      _values "grid_calculus command" \
        "0[Grid Normalization]" \
        "1[Grid Calculator]" \
        "2[Grid Volume]" \
        "3[Grid Difference]" \
        "4[Function Plotter]" \
        "5[Geometric Figures]" \
        "6[Random Terrain]" \
        "7[Random Field]" \
        "8[Grids Sum]" \
        "9[Grids Product]" \
        "10[Grid Standardization]" \
        "11[Fuzzify]" \
        "12[Fuzzy Intersection (AND)]" \
        "13[Fuzzy Union (OR)]" \
        "14[Metric Conversions]" \
        "15[Gradient Vector from Cartesian to Polar Coordinates]" \
        "16[Gradient Vector from Polar to Cartesian Coordinates]" \
        "17[Fractal Brownian Noise]" \
        "18[Grid Division]" \
        "19[Spherical Harmonic Synthesis]" \
        "20[Grid Collection Calculator]" \
        "21[Histogram Matching]" \
        "22[Grid Addition]" \
        "23[Grid Multiplication]" \

      ;;
    args)
      case "$line[1]" in
        0)
          _arguments \
            "-INPUT[<str> Grid]:file:_files" "-OUTPUT[<str> Normalized Grid]:file:_files" "-RANGE_MIN[<double>Target Range]:file:_files" "-RANGE_MAX[<double>Target Range]:file:_files"  
          ;;
        1)
          _arguments \
            "-RESAMPLING[<str>Resampling]:file:_files" "-FORMULA[<str> Formula]:file:_files" "-USE_NODATA[<str>Use No-Data]:file:_files" "-TYPE[<str> Data Type]:file:_files" "-GRIDS[<str> Grids]:file:_files" "-XGRIDS[<str> Grids from different Systems]:file:_files" "-RESULT[<str> Result]:file:_files"  
          ;;
        2)
          _arguments \
            "-GRID[<str> Grid]:file:_files" "-METHOD[<str> Method]:file:_files" "-LEVEL[<double>Base Level]:file:_files"  
          ;;
        3)
          _arguments \
            "-A[<str> Minuend]:file:_files" "-B[<str> Subtrahend]:file:_files" "-B_DEFAULT[<double>Default]:file:_files" "-C[<str> Difference]:file:_files"  
          ;;
        4)
          _arguments \
            "-FORMULA[<str> Formula]:file:_files" "-X_RANGE_MIN[<double> X Range]:file:_files" "-X_RANGE_MAX[<double> X Range]:file:_files" "-Y_RANGE_MIN[<double> Y Range]:file:_files" "-Y_RANGE_MAX[<double> Y Range]:file:_files" "-TARGET_DEFINITION[<str> Target Grid System]:file:_files" "-TARGET_USER_SIZE[<double>Cellsize]:file:_files" "-TARGET_USER_XMIN[<double>West]:file:_files" "-TARGET_USER_XMAX[<double>East]:file:_files" "-TARGET_USER_YMIN[<double>South]:file:_files" "-TARGET_USER_YMAX[<double>North]:file:_files" "-TARGET_USER_COLS[<num> Columns]:file:_files" "-TARGET_USER_ROWS[<num> Rows]:file:_files" "-TARGET_USER_FLAT[<str> Rounding]:file:_files" "-TARGET_USER_FITS[<str> Fit]:file:_files" "-TARGET_TEMPLATE[<str> Target System]:file:_files" "-FUNCTION[<str> Function]:file:_files"  
          ;;
        5)
          _arguments \
            "-RESULT[<str> Result]:file:_files" "-CELL_COUNT[<num> Cell Count]:file:_files" "-CELL_SIZE[<double>Cell Size]:file:_files" "-FIGURE[<str> Figure]:file:_files" "-PLANE[<double> Direction of Plane (Degree)]:file:_files"  
          ;;
        6)
          _arguments \
            "-RADIUS[<num> Radius (cells)]:file:_files" "-ITERATIONS[<num> Iterations]:file:_files" "-TARGET_DEFINITION[<str> Target Grid System]:file:_files" "-TARGET_USER_SIZE[<double>Cellsize]:file:_files" "-TARGET_USER_XMIN[<double>West]:file:_files" "-TARGET_USER_XMAX[<double>East]:file:_files" "-TARGET_USER_YMIN[<double>South]:file:_files" "-TARGET_USER_YMAX[<double>North]:file:_files" "-TARGET_USER_COLS[<num> Columns]:file:_files" "-TARGET_USER_ROWS[<num> Rows]:file:_files" "-TARGET_USER_FLAT[<str> Rounding]:file:_files" "-TARGET_USER_FITS[<str> Fit]:file:_files" "-TARGET_TEMPLATE[<str> Target System]:file:_files" "-TARGET_OUT_GRID[<str> Target Grid]:file:_files"  
          ;;
        7)
          _arguments \
            "-METHOD[<str> Method]:file:_files" "-RANGE_MIN[<double> Range]:file:_files" "-RANGE_MAX[<double> Range]:file:_files" "-MEAN[<double> Arithmetic Mean]:file:_files" "-STDDEV[<double> Standard Deviation]:file:_files" "-TARGET_DEFINITION[<str> Target Grid System]:file:_files" "-TARGET_USER_SIZE[<double>Cellsize]:file:_files" "-TARGET_USER_XMIN[<double>West]:file:_files" "-TARGET_USER_XMAX[<double>East]:file:_files" "-TARGET_USER_YMIN[<double>South]:file:_files" "-TARGET_USER_YMAX[<double>North]:file:_files" "-TARGET_USER_COLS[<num> Columns]:file:_files" "-TARGET_USER_ROWS[<num> Rows]:file:_files" "-TARGET_USER_FLAT[<str> Rounding]:file:_files" "-TARGET_USER_FITS[<str> Fit]:file:_files" "-TARGET_TEMPLATE[<str> Target System]:file:_files" "-OUT_GRID[<str> Random Field]:file:_files"  
          ;;
        8)
          _arguments \
            "-GRIDS[<str> Summands]:file:_files" "-RESULT[<str>Sum]:file:_files" "-NODATA[<str>Count No Data as Zero]:file:_files"  
          ;;
        9)
          _arguments \
            "-GRIDS[<str> Grids]:file:_files" "-RESULT[<str>Product]:file:_files" "-NODATA[<str>Count No Data as Zero]:file:_files"  
          ;;
        10)
          _arguments \
            "-INPUT[<str> Grid]:file:_files" "-OUTPUT[<str> Standardized Grid]:file:_files" "-STRETCH[<double>Stretch Factor]:file:_files"  
          ;;
        11)
          _arguments \
            "-INPUT[<str> Grid]:file:_files" "-OUTPUT[<str> Fuzzified Grid]:file:_files" "-INC_MIN[<double>From]:file:_files" "-INC_MAX[<double>To]:file:_files" "-DEC_MIN[<double>From]:file:_files" "-DEC_MAX[<double>To]:file:_files" "-METHOD[<str> Method]:file:_files" "-TRANSITION[<str>Transition]:file:_files" "-INVERT[<str> Invert]:file:_files" "-AUTOFIT[<str> Adjust]:file:_files"  
          ;;
        12)
          _arguments \
            "-GRIDS[<str>Grids]:file:_files" "-AND[<str> Intersection]:file:_files" "-TYPE[<str> Operator Type]:file:_files"  
          ;;
        13)
          _arguments \
            "-GRIDS[<str>Grids]:file:_files" "-OR[<str> Union]:file:_files" "-TYPE[<str> Operator Type]:file:_files"  
          ;;
        14)
          _arguments \
            "-GRID[<str> Grid]:file:_files" "-CONV[<str> Converted Grid]:file:_files" "-CONVERSION[<str>Conversion]:file:_files"  
          ;;
        15)
          _arguments \
            "-DX[<str> X Component]:file:_files" "-DY[<str> Y Component]:file:_files" "-DIR[<str> Direction]:file:_files" "-LEN[<str> Length]:file:_files" "-UNITS[<str> Polar Angle Units]:file:_files" "-SYSTEM[<str> Polar Coordinate System]:file:_files" "-SYSTEM_ZERO[<double>User defined Zero Direction]:file:_files" "-SYSTEM_ORIENT[<str> User defined Orientation]:file:_files"  
          ;;
        16)
          _arguments \
            "-DIR[<str> Direction]:file:_files" "-LEN[<str> Length]:file:_files" "-DX[<str> X Component]:file:_files" "-DY[<str> Y Component]:file:_files" "-UNITS[<str> Polar Angle Units]:file:_files" "-SYSTEM[<str> Polar Coordinate System]:file:_files" "-SYSTEM_ZERO[<double>User defined Zero Direction]:file:_files" "-SYSTEM_ORIENT[<str> User defined Orientation]:file:_files"  
          ;;
        17)
          _arguments \
            "-SCALING[<str> Scaling]:file:_files" "-MAX_SCALE[<double> Maximum Scale]:file:_files" "-STEPS[<num> Steps]:file:_files" "-RANGE_MIN[<double> Noise Range]:file:_files" "-RANGE_MAX[<double> Noise Range]:file:_files" "-TARGET_DEFINITION[<str> Target Grid System]:file:_files" "-TARGET_USER_SIZE[<double>Cellsize]:file:_files" "-TARGET_USER_XMIN[<double>West]:file:_files" "-TARGET_USER_XMAX[<double>East]:file:_files" "-TARGET_USER_YMIN[<double>South]:file:_files" "-TARGET_USER_YMAX[<double>North]:file:_files" "-TARGET_USER_COLS[<num> Columns]:file:_files" "-TARGET_USER_ROWS[<num> Rows]:file:_files" "-TARGET_USER_FLAT[<str> Rounding]:file:_files" "-TARGET_USER_FITS[<str> Fit]:file:_files" "-TARGET_TEMPLATE[<str> Target System]:file:_files" "-OUT_GRID[<str> Fractal Brownian Noise]:file:_files"  
          ;;
        18)
          _arguments \
            "-A[<str> Dividend]:file:_files" "-B[<str> Divisor]:file:_files" "-B_DEFAULT[<double>Default]:file:_files" "-C[<str> Quotient]:file:_files"  
          ;;
        19)
          _arguments \
            "-OUTPUT_GRID[<str> Synthesized Grid]:file:_files" "-FILE[<str> File with Coefficients]:file:_files" "-MINDEGREE[<num> Start Degree]:file:_files" "-MAXDEGREE[<num> Expansion Degree]:file:_files" "-LAT_START[<double> Start Latitude]:file:_files" "-END_LAT[<double> End Latitude]:file:_files" "-LONG_START[<double>Start Longitude]:file:_files" "-END_LONG[<double> End Longitude]:file:_files" "-INC[<double> Latitude / Longitude Increment]:file:_files"  
          ;;
        20)
          _arguments \
            "-RESAMPLING[<str>Resampling]:file:_files" "-FORMULA[<str> Formula]:file:_files" "-USE_NODATA[<str>Use No-Data]:file:_files" "-TYPE[<str> Data Type]:file:_files" "-GRIDS[<str> Grid Collections]:file:_files" "-XGRIDS[<str> Grid Collections from different Systems]:file:_files" "-RESULT[<str> Result]:file:_files"  
          ;;
        21)
          _arguments \
            "-GRID[<str> Grid]:file:_files" "-MATCHED[<str> Adjusted Grid]:file:_files" "-REFERENCE[<str> Reference Grid]:file:_files" "-METHOD[<str> Method]:file:_files" "-NCLASSES[<num> Histogramm Classes]:file:_files" "-MAXSAMPLES[<num>Maximum Sample Size]:file:_files"  
          ;;
        22)
          _arguments \
            "-A[<str> Summand 1]:file:_files" "-B[<str> Summand 2]:file:_files" "-B_DEFAULT[<double>Default]:file:_files" "-C[<str> Sum]:file:_files"  
          ;;
        23)
          _arguments \
            "-A[<str> Multiplicand]:file:_files" "-B[<str> Multiplier]:file:_files" "-B_DEFAULT[<double>Default]:file:_files" "-C[<str> Product]:file:_files"  
          ;;
     esac
     ;;
   esac
}

_grid_filter(){
  _arguments -C "1: :->cmds" "*::arg:->args" 
 
  case "$state" in
    cmds)
      _values "grid_filter command" \
        "0[Simple Filter]" \
        "1[Gaussian Filter]" \
        "2[Laplacian Filter]" \
        "3[Multi Direction Lee Filter]" \
        "4[User Defined Filter]" \
        "5[Filter Clumps]" \
        "6[Majority/Minority Filter]" \
        "7[DTM Filter (slope-based)]" \
        "8[Morphological Filter]" \
        "9[Rank Filter]" \
        "10[Mesh Denoise]" \
        "11[Resampling Filter]" \
        "12[Geodesic Morphological Reconstruction]" \
        "13[Binary Erosion-Reconstruction]" \
        "14[Connectivity Analysis]" \
        "15[Sieve Classes]" \
        "16[Wombling (Edge Detection)]" \
        "17[Wombling for Multiple Features (Edge Detection)]" \
        "18[Simple Filter (Restricted to Polygons)]" \
        "19[Sharpening Filter]" \
        "notch_filter[Notch Filter for Grids]" \
        "sieve_and_clump[Sieve and Clump]" \
        "simple_filter_bulk[Simple Filter for Multiple Grids]" \

      ;;
    args)
      case "$line[1]" in
        0)
          _arguments \
            "-INPUT[<str> Grid]:file:_files" "-RESULT[<str> Filtered Grid]:file:_files" "-METHOD[<str> Filter]:file:_files" "-KERNEL_TYPE[<str> Kernel Type]:file:_files" "-KERNEL_RADIUS[<num>Radius]:file:_files"  
          ;;
        1)
          _arguments \
            "-INPUT[<str> Grid]:file:_files" "-RESULT[<str> Filtered Grid]:file:_files" "-KERNEL_RADIUS[<num>Kernel Radius]:file:_files" "-SIGMA[<double> Standard Deviation]:file:_files"  
          ;;
        2)
          _arguments \
            "-INPUT[<str> Grid]:file:_files" "-RESULT[<str> Filtered Grid]:file:_files" "-METHOD[<str> Method]:file:_files" "-KERNEL_RADIUS[<num>Kernel Radius]:file:_files" "-SIGMA[<double> Standard Deviation]:file:_files"  
          ;;
        3)
          _arguments \
            "-INPUT[<str> Grid]:file:_files" "-RESULT[<str> Filtered Grid]:file:_files" "-STDDEV[<str> Minimum Standard Deviation]:file:_files" "-DIR[<str> Direction of Minimum Standard Deviation]:file:_files" "-NOISE_ABS[<double>Estimated Noise (absolute)]:file:_files" "-NOISE_REL[<double>Estimated Noise (relative)]:file:_files" "-WEIGHTED[<str> Weighted]:file:_files" "-METHOD[<str> Method]:file:_files"  
          ;;
        4)
          _arguments \
            "-INPUT[<str> Grid]:file:_files" "-RESULT[<str> Filtered Grid]:file:_files" "-FILTER[<str> Kernel]:file:_files" "-FILTER_3X3[<str>3x3 Kernel]:file:_files" "-ABSOLUTE[<str> Absolute Weighting]:file:_files"  
          ;;
        5)
          _arguments \
            "-GRID[<str> Input Grid]:file:_files" "-OUTPUT[<str> Filtered Grid]:file:_files" "-THRESHOLD[<num>Min. Size]:file:_files"  
          ;;
        6)
          _arguments \
            "-INPUT[<str> Grid]:file:_files" "-RESULT[<str> Filtered Grid]:file:_files" "-TYPE[<str> Type]:file:_files" "-THRESHOLD[<double> Threshold]:file:_files" "-KERNEL_TYPE[<str> Kernel Type]:file:_files" "-KERNEL_RADIUS[<num>Radius]:file:_files"  
          ;;
        7)
          _arguments \
            "-INPUT[<str> DEM]:file:_files" "-GROUND[<str> Bare Earth]:file:_files" "-NONGROUND[<str> Removed Objects]:file:_files" "-RADIUS[<num> Kernel Radius]:file:_files" "-TERRAINSLOPE[<double>Terrain Slope (%)]:file:_files" "-FILTERMOD[<str> Filter Modification]:file:_files" "-STDDEV[<double> Standard Deviation]:file:_files"  
          ;;
        8)
          _arguments \
            "-INPUT[<str> Grid]:file:_files" "-RESULT[<str> Filtered Grid]:file:_files" "-METHOD[<str> Method]:file:_files" "-KERNEL_TYPE[<str> Kernel Type]:file:_files" "-KERNEL_RADIUS[<num>Radius]:file:_files"  
          ;;
        9)
          _arguments \
            "-INPUT[<str> Grid]:file:_files" "-RESULT[<str> Filtered Grid]:file:_files" "-RANK[<double> Rank]:file:_files" "-KERNEL_TYPE[<str> Kernel Type]:file:_files" "-KERNEL_RADIUS[<num>Radius]:file:_files"  
          ;;
        10)
          _arguments \
            "-INPUT[<str> Grid]:file:_files" "-OUTPUT[<str> Denoised Grid]:file:_files" "-SIGMA[<double>Threshold]:file:_files" "-ITER[<num> Number of Iterations for Normal Updating]:file:_files" "-VITER[<num> Number of Iterations for Vertex Updating]:file:_files" "-NB_CV[<str> Common Edge Type of Face Neighbourhood]:file:_files" "-ZONLY[<str> Only Z-Direction Position is Updated]:file:_files"  
          ;;
        11)
          _arguments \
            "-GRID[<str> Grid]:file:_files" "-LOPASS[<str> Low Pass Filter]:file:_files" "-HIPASS[<str> High Pass Filter]:file:_files" "-SCALE[<double>Scale Factor]:file:_files"  
          ;;
        12)
          _arguments \
            "-INPUT_GRID[<str> Input Grid]:file:_files" "-DIFFERENCE_GRID[<str>Morphological Reconstruction]:file:_files" "-OBJECT_GRID[<str> Objects]:file:_files" "-SHIFT_VALUE[<double> Shift Value]:file:_files" "-BORDER_YES_NO[<str> Preserve 1px border]:file:_files" "-THRESHOLD[<double> Threshold]:file:_files"  
          ;;
        13)
          _arguments \
            "-INPUT_GRID[<str> Input Grid]:file:_files" "-OUTPUT_GRID[<str>Output Grid]:file:_files" "-RADIUS[<num> Filter Size (Radius)]:file:_files"  
          ;;
        14)
          _arguments \
            "-INPUT_GRID[<str> Input Binary Grid]:file:_files" "-FILTERED_MASK[<str> Filtered Image]:file:_files" "-FILTER[<str> Apply Filter?]:file:_files" "-SIZE[<num> Filter Size (Radius)]:file:_files" "-SYMBOLIC_IMAGE[<str> Symbolic Image]:file:_files" "-OUTLINES[<str> Outlines]:file:_files" "-BORDER_PIXEL_CENTERS[<str> Pixel Centers?]:file:_files" "-REMOVE_MARGINAL_REGIONS[<str>Remove Border Regions?]:file:_files"  
          ;;
        15)
          _arguments \
            "-INPUT[<str> Classes]:file:_files" "-OUTPUT[<str> Sieved Classes]:file:_files" "-MODE[<str> Neighbourhood]:file:_files" "-THRESHOLD[<num>Minimum Threshold]:file:_files" "-ALL[<str> Class Selection]:file:_files" "-CLASS[<double> Class Identifier]:file:_files"  
          ;;
        16)
          _arguments \
            "-TMAGNITUDE[<double>Minimum Magnitude]:file:_files" "-TDIRECTION[<double>Maximum Angle]:file:_files" "-TNEIGHBOUR[<num> Minimum Neighbours]:file:_files" "-ALIGNMENT[<str> Alignment]:file:_files" "-NEIGHBOUR[<str> Edge Connectivity]:file:_files" "-FEATURE[<str> Feature]:file:_files" "-EDGE_POINTS[<str> Edge Points]:file:_files" "-EDGE_LINES[<str> Edge Segments]:file:_files" "-GRADIENTS_OUT[<str>Output of Gradients]:file:_files" "-GRADIENTS[<str> Gradients]:file:_files"  
          ;;
        17)
          _arguments \
            "-TMAGNITUDE[<double> Minimum Magnitude]:file:_files" "-TDIRECTION[<double> Maximum Angle]:file:_files" "-TNEIGHBOUR[<num> Minimum Neighbours]:file:_files" "-ALIGNMENT[<str> Alignment]:file:_files" "-NEIGHBOUR[<str> Edge Connectivity]:file:_files" "-FEATURES[<str> Features]:file:_files" "-EDGE_CELLS[<str> Edges]:file:_files" "-OUTPUT_ADD[<str> Additional Output]:file:_files" "-OUTPUT[<str> Output]:file:_files" "-ZERO_AS_NODATA[<str>Zero as No-Data]:file:_files"  
          ;;
        18)
          _arguments \
            "-INPUT[<str> Grid]:file:_files" "-RESULT[<str> Filtered Grid]:file:_files" "-SHAPES[<str> Polygons]:file:_files" "-METHOD[<str> Filter]:file:_files" "-SKIP_OUTSIDE[<str> Skip Outside Cells]:file:_files" "-KERNEL_TYPE[<str> Kernel Type]:file:_files" "-KERNEL_RADIUS[<num>Radius]:file:_files"  
          ;;
        19)
          _arguments \
            "-INPUT[<str> Grid]:file:_files" "-RESULT[<str> Filtered Grid]:file:_files" "-METHOD[<str> Method]:file:_files" "-KERNEL_RADIUS[<num>Kernel Radius]:file:_files" "-SIGMA[<double> Standard Deviation]:file:_files"  
          ;;
        notch_filter)
          _arguments \
            "-INPUT_GRID[<str> Input Grid]:file:_files" "-LOWPASS_UPPER[<str>Lowpass Upper]:file:_files" "-HIPASS_LOWER[<str> Highpass Lower]:file:_files" "-FILTERED_GRID[<str>Notch]:file:_files" "-HIGH_PASS[<double> Upper search distance ]:file:_files" "-LOW_PASS[<double> Lower search distance]:file:_files"  
          ;;
        sieve_and_clump)
          _arguments \
            "-CLASSES[<str> Classes]:file:_files" "-FILTERED[<str>Sieve and Clump]:file:_files" "-MODE[<str> Filter]:file:_files" "-SIEVE[<num> Sieving Threshold]:file:_files" "-EXPAND[<num> Maximum Expansion Distance]:file:_files"  
          ;;
        simple_filter_bulk)
          _arguments \
            "-GRIDS[<str> Grids]:file:_files" "-FILTERED[<str> Filtered Grids]:file:_files" "-METHOD[<str> Filter]:file:_files" "-KERNEL_TYPE[<str> Kernel Type]:file:_files" "-KERNEL_RADIUS[<num>Kernel Radius]:file:_files"  
          ;;
     esac
     ;;
   esac
}

_grid_gridding(){
  _arguments -C "1: :->cmds" "*::arg:->args" 
 
  case "$state" in
    cmds)
      _values "grid_gridding command" \
        "0[Shapes to Grid]" \
        "1[Inverse Distance Weighted]" \
        "2[Nearest Neighbour]" \
        "3[Natural Neighbour]" \
        "4[Modifed Quadratic Shepard]" \
        "5[Triangulation]" \
        "6[Kernel Density Estimation]" \
        "7[Angular Distance Weighted]" \
        "8[Grid Cell Area Covered by Polygons]" \
        "9[Polygons to Grid]" \
        "10[Polygon Categories to Grid]" \

      ;;
    args)
      case "$line[1]" in
        0)
          _arguments \
            "-INPUT[<str> Shapes]:file:_files" "-FIELD[<str> Attribute]:file:_files" "-OUTPUT[<str> Output Values]:file:_files" "-MULTIPLE[<str> Method for Multiple Values]:file:_files" "-LINE_TYPE[<str> Lines]:file:_files" "-POLY_TYPE[<str> Polygon]:file:_files" "-GRID_TYPE[<str> Data Type]:file:_files" "-TARGET_DEFINITION[<str> Target Grid System]:file:_files" "-TARGET_USER_SIZE[<double>Cellsize]:file:_files" "-TARGET_USER_XMIN[<double>West]:file:_files" "-TARGET_USER_XMAX[<double>East]:file:_files" "-TARGET_USER_YMIN[<double>South]:file:_files" "-TARGET_USER_YMAX[<double>North]:file:_files" "-TARGET_USER_COLS[<num> Columns]:file:_files" "-TARGET_USER_ROWS[<num> Rows]:file:_files" "-TARGET_USER_FLAT[<str> Rounding]:file:_files" "-TARGET_USER_FITS[<str> Fit]:file:_files" "-TARGET_TEMPLATE[<str> Target System]:file:_files" "-GRID[<str> Grid]:file:_files" "-COUNT[<str> Number of Values]:file:_files"  
          ;;
        1)
          _arguments \
            "-POINTS[<str> Points]:file:_files" "-FIELD[<str> Attribute]:file:_files" "-CV_METHOD[<str> Cross Validation]:file:_files" "-CV_SUMMARY[<str> Cross Validation Summary]:file:_files" "-CV_RESIDUALS[<str> Cross Validation Residuals]:file:_files" "-CV_SAMPLES[<num> Cross Validation Subsamples]:file:_files" "-TARGET_DEFINITION[<str> Target Grid System]:file:_files" "-TARGET_USER_SIZE[<double>Cellsize]:file:_files" "-TARGET_USER_XMIN[<double>West]:file:_files" "-TARGET_USER_XMAX[<double>East]:file:_files" "-TARGET_USER_YMIN[<double>South]:file:_files" "-TARGET_USER_YMAX[<double>North]:file:_files" "-TARGET_USER_COLS[<num> Columns]:file:_files" "-TARGET_USER_ROWS[<num> Rows]:file:_files" "-TARGET_USER_FLAT[<str> Rounding]:file:_files" "-TARGET_USER_FITS[<str> Fit]:file:_files" "-TARGET_TEMPLATE[<str> Target System]:file:_files" "-TARGET_OUT_GRID[<str> Target Grid]:file:_files" "-SEARCH_RANGE[<str> Search Range]:file:_files" "-SEARCH_RADIUS[<double> Maximum Search Distance]:file:_files" "-SEARCH_POINTS_ALL[<str> Number of Points]:file:_files" "-SEARCH_POINTS_MIN[<num> Minimum]:file:_files" "-SEARCH_POINTS_MAX[<num> Maximum]:file:_files" "-DW_WEIGHTING[<str> Weighting Function]:file:_files" "-DW_IDW_POWER[<double> Power]:file:_files" "-DW_BANDWIDTH[<double> Bandwidth]:file:_files"  
          ;;
        2)
          _arguments \
            "-POINTS[<str> Points]:file:_files" "-FIELD[<str> Attribute]:file:_files" "-CV_METHOD[<str> Cross Validation]:file:_files" "-CV_SUMMARY[<str> Cross Validation Summary]:file:_files" "-CV_RESIDUALS[<str> Cross Validation Residuals]:file:_files" "-CV_SAMPLES[<num> Cross Validation Subsamples]:file:_files" "-TARGET_DEFINITION[<str> Target Grid System]:file:_files" "-TARGET_USER_SIZE[<double>Cellsize]:file:_files" "-TARGET_USER_XMIN[<double>West]:file:_files" "-TARGET_USER_XMAX[<double>East]:file:_files" "-TARGET_USER_YMIN[<double>South]:file:_files" "-TARGET_USER_YMAX[<double>North]:file:_files" "-TARGET_USER_COLS[<num> Columns]:file:_files" "-TARGET_USER_ROWS[<num> Rows]:file:_files" "-TARGET_USER_FLAT[<str> Rounding]:file:_files" "-TARGET_USER_FITS[<str> Fit]:file:_files" "-TARGET_TEMPLATE[<str> Target System]:file:_files" "-TARGET_OUT_GRID[<str> Target Grid]:file:_files"  
          ;;
        3)
          _arguments \
            "-POINTS[<str> Points]:file:_files" "-FIELD[<str> Attribute]:file:_files" "-TARGET_DEFINITION[<str> Target Grid System]:file:_files" "-TARGET_USER_SIZE[<double>Cellsize]:file:_files" "-TARGET_USER_XMIN[<double>West]:file:_files" "-TARGET_USER_XMAX[<double>East]:file:_files" "-TARGET_USER_YMIN[<double>South]:file:_files" "-TARGET_USER_YMAX[<double>North]:file:_files" "-TARGET_USER_COLS[<num> Columns]:file:_files" "-TARGET_USER_ROWS[<num> Rows]:file:_files" "-TARGET_USER_FLAT[<str> Rounding]:file:_files" "-TARGET_USER_FITS[<str> Fit]:file:_files" "-TARGET_TEMPLATE[<str> Target System]:file:_files" "-TARGET_OUT_GRID[<str> Target Grid]:file:_files" "-METHOD[<str> Method]:file:_files" "-WEIGHT[<double> Minimum Weight]:file:_files"  
          ;;
        4)
          _arguments \
            "-POINTS[<str> Points]:file:_files" "-FIELD[<str> Attribute]:file:_files" "-CV_METHOD[<str> Cross Validation]:file:_files" "-CV_SUMMARY[<str> Cross Validation Summary]:file:_files" "-CV_RESIDUALS[<str> Cross Validation Residuals]:file:_files" "-CV_SAMPLES[<num> Cross Validation Subsamples]:file:_files" "-TARGET_DEFINITION[<str> Target Grid System]:file:_files" "-TARGET_USER_SIZE[<double>Cellsize]:file:_files" "-TARGET_USER_XMIN[<double>West]:file:_files" "-TARGET_USER_XMAX[<double>East]:file:_files" "-TARGET_USER_YMIN[<double>South]:file:_files" "-TARGET_USER_YMAX[<double>North]:file:_files" "-TARGET_USER_COLS[<num> Columns]:file:_files" "-TARGET_USER_ROWS[<num> Rows]:file:_files" "-TARGET_USER_FLAT[<str> Rounding]:file:_files" "-TARGET_USER_FITS[<str> Fit]:file:_files" "-TARGET_TEMPLATE[<str> Target System]:file:_files" "-TARGET_OUT_GRID[<str> Target Grid]:file:_files" "-QUADRATIC_NEIGHBORS[<num>Quadratic Neighbors]:file:_files" "-WEIGHTING_NEIGHBORS[<num>Weighting Neighbors]:file:_files"  
          ;;
        5)
          _arguments \
            "-POINTS[<str> Points]:file:_files" "-FIELD[<str> Attribute]:file:_files" "-TARGET_DEFINITION[<str> Target Grid System]:file:_files" "-TARGET_USER_SIZE[<double>Cellsize]:file:_files" "-TARGET_USER_XMIN[<double>West]:file:_files" "-TARGET_USER_XMAX[<double>East]:file:_files" "-TARGET_USER_YMIN[<double>South]:file:_files" "-TARGET_USER_YMAX[<double>North]:file:_files" "-TARGET_USER_COLS[<num> Columns]:file:_files" "-TARGET_USER_ROWS[<num> Rows]:file:_files" "-TARGET_USER_FLAT[<str> Rounding]:file:_files" "-TARGET_USER_FITS[<str> Fit]:file:_files" "-TARGET_TEMPLATE[<str> Target System]:file:_files" "-TARGET_OUT_GRID[<str> Target Grid]:file:_files" "-FRAME[<str> Add Frame]:file:_files"  
          ;;
