  _arguments -C "1: :->cmds" "*::arg:->args" 
 
  case "$state" in
    cmds)
      _values "imagery_opencv command" \
        "0[Morphological Filter (OpenCV)]" \
        "1[Fourier Transformation]" \
        "2[Single Value Decomposition (OpenCV)]" \
        "4[Stereo Match (OpenCV)]" \
        "5[Normal Bayes Classification]" \
        "6[K-Nearest Neighbours Classification]" \
        "7[Support Vector Machine Classification]" \
        "8[Decision Tree Classification]" \
        "9[Boosting Classification]" \
        "10[Random Forest Classification]" \
        "11[Artificial Neural Network Classification]" \
        "12[Logistic Regression Classification]" \
