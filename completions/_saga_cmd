            "-FEATURES[<str> Features]:file:_files" "-NORMALIZE[<str> Normalize]:file:_files" "-CLASSES[<str> Classification]:file:_files" "-CLASSES_LUT[<str> Look-up Table]:file:_files" "-MODEL_TRAIN[<str> Training]:file:_files" "-TRAIN_SAMPLES[<str> Training Samples]:file:_files" "-TRAIN_AREAS[<str> Training Areas]:file:_files" "-TRAIN_CLASS[<str> Class Identifier]:file:_files" "-TRAIN_BUFFER[<double>Buffer Size]:file:_files" "-MODEL_LOAD[<str> Load Model]:file:_files" "-MODEL_SAVE[<str> Save Model]:file:_files" "-NEIGHBOURS[<num> Default Number of Neighbours]:file:_files" "-TRAINING[<str> Training Method]:file:_files" "-ALGORITHM[<str> Algorithm Type]:file:_files" "-EMAX[<num> Parameter for KD Tree implementation]:file:_files"  
