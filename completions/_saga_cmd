          ;;
        7)
          _arguments \
            "-POINTS[<str> Points]:file:_files" "-FIELD[<str> Attribute]:file:_files" "-CV_METHOD[<str> Cross Validation]:file:_files" "-CV_SUMMARY[<str> Cross Validation Summary]:file:_files" "-CV_RESIDUALS[<str> Cross Validation Residuals]:file:_files" "-CV_SAMPLES[<num> Cross Validation Subsamples]:file:_files" "-TARGET_DEFINITION[<str> Target Grid System]:file:_files" "-TARGET_USER_SIZE[<double>Cellsize]:file:_files" "-TARGET_USER_XMIN[<double>West]:file:_files" "-TARGET_USER_XMAX[<double>East]:file:_files" "-TARGET_USER_YMIN[<double>South]:file:_files" "-TARGET_USER_YMAX[<double>North]:file:_files" "-TARGET_USER_COLS[<num> Columns]:file:_files" "-TARGET_USER_ROWS[<num> Rows]:file:_files" "-TARGET_USER_FLAT[<str> Rounding]:file:_files" "-TARGET_USER_FITS[<str> Fit]:file:_files" "-TARGET_TEMPLATE[<str> Target System]:file:_files" "-TARGET_OUT_GRID[<str> Target Grid]:file:_files" "-SEARCH_RANGE[<str> Search Range]:file:_files" "-SEARCH_RADIUS[<double> Maximum Search Distance]:file:_files" "-SEARCH_POINTS_ALL[<str> Number of Points]:file:_files" "-SEARCH_POINTS_MIN[<num> Minimum]:file:_files" "-SEARCH_POINTS_MAX[<num> Maximum]:file:_files" "-DW_WEIGHTING[<str> Weighting Function]:file:_files" "-DW_IDW_POWER[<double> Power]:file:_files" "-DW_BANDWIDTH[<double> Bandwidth]:file:_files"  
          ;;
        8)
          _arguments \
            "-POLYGONS[<str> Polygons]:file:_files" "-METHOD[<str> Method]:file:_files" "-OUTPUT[<str> Output]:file:_files" "-SELECTION[<str> Only Selected Polygons]:file:_files" "-TARGET_DEFINITION[<str> Target Grid System]:file:_files" "-TARGET_USER_SIZE[<double>Cellsize]:file:_files" "-TARGET_USER_XMIN[<double>West]:file:_files" "-TARGET_USER_XMAX[<double>East]:file:_files" "-TARGET_USER_YMIN[<double>South]:file:_files" "-TARGET_USER_YMAX[<double>North]:file:_files" "-TARGET_USER_COLS[<num> Columns]:file:_files" "-TARGET_USER_ROWS[<num> Rows]:file:_files" "-TARGET_USER_FLAT[<str> Rounding]:file:_files" "-TARGET_USER_FITS[<str> Fit]:file:_files" "-TARGET_TEMPLATE[<str> Target System]:file:_files" "-AREA[<str> Area of Coverage]:file:_files"  
          ;;
        9)
          _arguments \
            "-POLYGONS[<str> Polygons]:file:_files" "-FIELD[<str> Attribute]:file:_files" "-OUTPUT[<str> Output Values]:file:_files" "-MULTIPLE[<str> Multiple Polygons]:file:_files" "-GRID_TYPE[<str> Data Type]:file:_files" "-TARGET_DEFINITION[<str> Target Grid System]:file:_files" "-TARGET_USER_SIZE[<double>Cellsize]:file:_files" "-TARGET_USER_XMIN[<double>West]:file:_files" "-TARGET_USER_XMAX[<double>East]:file:_files" "-TARGET_USER_YMIN[<double>South]:file:_files" "-TARGET_USER_YMAX[<double>North]:file:_files" "-TARGET_USER_COLS[<num> Columns]:file:_files" "-TARGET_USER_ROWS[<num> Rows]:file:_files" "-TARGET_USER_FLAT[<str> Rounding]:file:_files" "-TARGET_USER_FITS[<str> Fit]:file:_files" "-TARGET_TEMPLATE[<str> Target System]:file:_files" "-GRID[<str> Grid]:file:_files" "-COVERAGE[<str> Coverage]:file:_files"  
          ;;
        10)
          _arguments \
            "-POLYGONS[<str> Polygons]:file:_files" "-FIELD[<str> Category]:file:_files" "-METHOD[<str> Method]:file:_files" "-MULTIPLE[<str> Multiple Polygons]:file:_files" "-CLASSES[<str> Classification]:file:_files" "-TARGET_DEFINITION[<str> Target Grid System]:file:_files" "-TARGET_USER_SIZE[<double>Cellsize]:file:_files" "-TARGET_USER_XMIN[<double>West]:file:_files" "-TARGET_USER_XMAX[<double>East]:file:_files" "-TARGET_USER_YMIN[<double>South]:file:_files" "-TARGET_USER_YMAX[<double>North]:file:_files" "-TARGET_USER_COLS[<num> Columns]:file:_files" "-TARGET_USER_ROWS[<num> Rows]:file:_files" "-TARGET_USER_FLAT[<str> Rounding]:file:_files" "-TARGET_USER_FITS[<str> Fit]:file:_files" "-TARGET_TEMPLATE[<str> Target System]:file:_files" "-CATEGORY[<str> Category]:file:_files" "-COVERAGE[<str> Coverage]:file:_files"  
          ;;
     esac
     ;;
   esac
}

_grid_spline(){
  _arguments -C "1: :->cmds" "*::arg:->args" 
 
  case "$state" in
    cmds)
      _values "grid_spline command" \
        "1[Thin Plate Spline]" \
        "2[Thin Plate Spline (TIN)]" \
        "3[B-Spline Approximation]" \
        "4[Multilevel B-Spline]" \
        "5[Multilevel B-Spline from Grid Points]" \
        "6[Cubic Spline Approximation]" \
        "7[Multilevel B-Spline for Categories]" \
        "8[Multilevel B-Spline (3D)]" \

      ;;
    args)
      case "$line[1]" in
        1)
          _arguments \
            "-SHAPES[<str> Points]:file:_files" "-FIELD[<str> Attribute]:file:_files" "-TARGET_DEFINITION[<str> Target Grid System]:file:_files" "-TARGET_USER_SIZE[<double>Cellsize]:file:_files" "-TARGET_USER_XMIN[<double>West]:file:_files" "-TARGET_USER_XMAX[<double>East]:file:_files" "-TARGET_USER_YMIN[<double>South]:file:_files" "-TARGET_USER_YMAX[<double>North]:file:_files" "-TARGET_USER_COLS[<num> Columns]:file:_files" "-TARGET_USER_ROWS[<num> Rows]:file:_files" "-TARGET_USER_FLAT[<str> Rounding]:file:_files" "-TARGET_USER_FITS[<str> Fit]:file:_files" "-TARGET_TEMPLATE[<str> Target System]:file:_files" "-TARGET_OUT_GRID[<str> Target Grid]:file:_files" "-REGULARISATION[<double> Regularisation]:file:_files" "-SEARCH_RANGE[<str> Search Range]:file:_files" "-SEARCH_RADIUS[<double> Maximum Search Distance]:file:_files" "-SEARCH_POINTS_ALL[<str> Number of Points]:file:_files" "-SEARCH_POINTS_MIN[<num> Minimum]:file:_files" "-SEARCH_POINTS_MAX[<num> Maximum]:file:_files"  
          ;;
        2)
          _arguments \
            "-SHAPES[<str> Points]:file:_files" "-FIELD[<str> Attribute]:file:_files" "-TARGET_DEFINITION[<str> Target Grid System]:file:_files" "-TARGET_USER_SIZE[<double>Cellsize]:file:_files" "-TARGET_USER_XMIN[<double>West]:file:_files" "-TARGET_USER_XMAX[<double>East]:file:_files" "-TARGET_USER_YMIN[<double>South]:file:_files" "-TARGET_USER_YMAX[<double>North]:file:_files" "-TARGET_USER_COLS[<num> Columns]:file:_files" "-TARGET_USER_ROWS[<num> Rows]:file:_files" "-TARGET_USER_FLAT[<str> Rounding]:file:_files" "-TARGET_USER_FITS[<str> Fit]:file:_files" "-TARGET_TEMPLATE[<str> Target System]:file:_files" "-TARGET_OUT_GRID[<str> Target Grid]:file:_files" "-REGULARISATION[<double> Regularisation]:file:_files" "-LEVEL[<str> Neighbourhood]:file:_files" "-FRAME[<str> Add Frame]:file:_files"  
          ;;
        3)
          _arguments \
            "-SHAPES[<str> Points]:file:_files" "-FIELD[<str> Attribute]:file:_files" "-TARGET_DEFINITION[<str> Target Grid System]:file:_files" "-TARGET_USER_SIZE[<double>Cellsize]:file:_files" "-TARGET_USER_XMIN[<double>West]:file:_files" "-TARGET_USER_XMAX[<double>East]:file:_files" "-TARGET_USER_YMIN[<double>South]:file:_files" "-TARGET_USER_YMAX[<double>North]:file:_files" "-TARGET_USER_COLS[<num> Columns]:file:_files" "-TARGET_USER_ROWS[<num> Rows]:file:_files" "-TARGET_USER_FLAT[<str> Rounding]:file:_files" "-TARGET_USER_FITS[<str> Fit]:file:_files" "-TARGET_TEMPLATE[<str> Target System]:file:_files" "-TARGET_OUT_GRID[<str> Target Grid]:file:_files" "-LEVEL[<double> Range]:file:_files"  
          ;;
        4)
          _arguments \
            "-SHAPES[<str> Points]:file:_files" "-FIELD[<str> Attribute]:file:_files" "-TARGET_DEFINITION[<str> Target Grid System]:file:_files" "-TARGET_USER_SIZE[<double>Cellsize]:file:_files" "-TARGET_USER_XMIN[<double>West]:file:_files" "-TARGET_USER_XMAX[<double>East]:file:_files" "-TARGET_USER_YMIN[<double>South]:file:_files" "-TARGET_USER_YMAX[<double>North]:file:_files" "-TARGET_USER_COLS[<num> Columns]:file:_files" "-TARGET_USER_ROWS[<num> Rows]:file:_files" "-TARGET_USER_FLAT[<str> Rounding]:file:_files" "-TARGET_USER_FITS[<str> Fit]:file:_files" "-TARGET_TEMPLATE[<str> Target System]:file:_files" "-TARGET_OUT_GRID[<str> Target Grid]:file:_files" "-METHOD[<str> Refinement]:file:_files" "-EPSILON[<double> Threshold Error]:file:_files" "-LEVEL_MAX[<num> Maximum Level]:file:_files"  
          ;;
        5)
          _arguments \
            "-GRID[<str> Grid]:file:_files" "-TARGET_DEFINITION[<str> Target Grid System]:file:_files" "-TARGET_USER_SIZE[<double>Cellsize]:file:_files" "-TARGET_USER_XMIN[<double>West]:file:_files" "-TARGET_USER_XMAX[<double>East]:file:_files" "-TARGET_USER_YMIN[<double>South]:file:_files" "-TARGET_USER_YMAX[<double>North]:file:_files" "-TARGET_USER_COLS[<num> Columns]:file:_files" "-TARGET_USER_ROWS[<num> Rows]:file:_files" "-TARGET_USER_FLAT[<str> Rounding]:file:_files" "-TARGET_USER_FITS[<str> Fit]:file:_files" "-TARGET_TEMPLATE[<str> Target System]:file:_files" "-TARGET_OUT_GRID[<str> Target Grid]:file:_files" "-DATATYPE[<str> Data Type]:file:_files" "-METHOD[<str> Refinement]:file:_files" "-EPSILON[<double> Threshold Error]:file:_files" "-LEVEL_MAX[<num> Maximum Level]:file:_files"  
          ;;
        6)
          _arguments \
            "-SHAPES[<str> Points]:file:_files" "-FIELD[<str> Attribute]:file:_files" "-TARGET_DEFINITION[<str> Target Grid System]:file:_files" "-TARGET_USER_SIZE[<double>Cellsize]:file:_files" "-TARGET_USER_XMIN[<double>West]:file:_files" "-TARGET_USER_XMAX[<double>East]:file:_files" "-TARGET_USER_YMIN[<double>South]:file:_files" "-TARGET_USER_YMAX[<double>North]:file:_files" "-TARGET_USER_COLS[<num> Columns]:file:_files" "-TARGET_USER_ROWS[<num> Rows]:file:_files" "-TARGET_USER_FLAT[<str> Rounding]:file:_files" "-TARGET_USER_FITS[<str> Fit]:file:_files" "-TARGET_TEMPLATE[<str> Target System]:file:_files" "-TARGET_OUT_GRID[<str> Target Grid]:file:_files" "-NPMIN[<num> Minimal Number of Points]:file:_files" "-NPMAX[<num> Maximal Number of Points]:file:_files" "-NPPC[<double> Points per Square]:file:_files" "-K[<num> Tolerance]:file:_files"  
