          _arguments \
            "-GRIDS[<str> Grids]:file:_files" "-FILE[<str> File]:file:_files" "-SAVE_FILE[<str> Save to File]:file:_files" "-SAVE_PATH[<str> Save to Path]:file:_files" "-TRANSFORM[<str> Transformation]:file:_files" "-RESAMPLING[<str>Resampling]:file:_files"  
