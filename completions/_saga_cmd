  _arguments -C "1: :->cmds" "*::arg:->args" 
 
  case "$state" in
    cmds)
      _values "imagery_photogrammetry command" \
        "0[Resection (Terrestrial)]" \
        "1[Colorisation (PC)]" \

      ;;
    args)
      case "$line[1]" in
        0)
          _arguments \
            "-POINTS[<str> Measured Points (PC)]:file:_files" "-F[<double> Focal Length (mm)]:file:_files" "-W[<double> Pixel Width (um)]:file:_files" "-EST_OFFSETS[<str> Estimate Principal Point Offsets?]:file:_files" "-ppX[<double> Principal Point Offset in X (pixels)]:file:_files" "-ppY[<double> Principal Point Offset in Y (pixels)]:file:_files" "-GIVE_DISTORTIONS[<str>Provide Radial Distortion Parameters?]:file:_files" "-K1[<double> K1]:file:_files" "-K2[<double> K2]:file:_files" "-K3[<double> K3]:file:_files" "-Xc[<double> X]:file:_files" "-Yc[<double> Y]:file:_files" "-Zc[<double> Z]:file:_files" "-Xt[<double> X]:file:_files" "-Yt[<double> Y]:file:_files" "-Zt[<double> Z]:file:_files" "-OUTPUT_FILE[<str> Output Text File]:file:_files"  
          ;;
        1)
          _arguments \
            "-rgbImage[<str> RGB Image]:file:_files" "-PC_IN[<str> Points to be Colorised]:file:_files" "-GIVE_TIME[<str> Provide Time stamp of Image?]:file:_files" "-IMG_TIME[<double> Time stamp of Image]:file:_files" "-TIME_DIFF[<double> Time Difference between Image & Points]:file:_files" "-F[<double> Focal Length (mm)]:file:_files" "-W[<double> Pixel Width (um)]:file:_files" "-ppX[<double> Principal Point Offset in X (pixles)]:file:_files" "-ppY[<double> Principal Point Offset in Y (pixels)]:file:_files" "-GIVE_DISTORTIONS[<str>Provide Radial Distortion Parameters?]:file:_files" "-K1[<double> K1]:file:_files" "-K2[<double> K2]:file:_files" "-K3[<double> K3]:file:_files" "-Xc[<double> Projection Centre-X]:file:_files" "-Yc[<double> Projection Centre-Y]:file:_files" "-Zc[<double> Projection Centre-Z]:file:_files" "-omega[<double> Omega]:file:_files" "-kappa[<double> Kappa]:file:_files" "-alpha[<double> Alpha]:file:_files" "-PC_OUT[<str> Colorised Point Cloud]:file:_files"  
          ;;
     esac
     ;;
   esac
}

_imagery_segmentation(){
  _arguments -C "1: :->cmds" "*::arg:->args" 
 
  case "$state" in
    cmds)
      _values "imagery_segmentation command" \
        "0[Watershed Segmentation]" \
        "1[Grid Skeletonization]" \
        "2[Seed Generation]" \
        "3[Seeded Region Growing]" \
        "4[Superpixel Segmentation]" \
        "5[Connected Component Labeling]" \
        "obia[Object Based Image Segmentation]" \

      ;;
    args)
      case "$line[1]" in
        0)
          _arguments \
            "-GRID[<str> Grid]:file:_files" "-SEGMENTS[<str> Segments]:file:_files" "-SEEDS[<str> Seed Points]:file:_files" "-BORDERS[<str> Borders]:file:_files" "-OUTPUT[<str> Output]:file:_files" "-DOWN[<str> Method]:file:_files" "-JOIN[<str> Join Segments based on Threshold Value]:file:_files" "-THRESHOLD[<double>Threshold]:file:_files" "-EDGE[<str> Allow Edge Pixels to be Seeds]:file:_files" "-BBORDERS[<str> Borders]:file:_files"  
          ;;
        1)
          _arguments \
            "-INPUT[<str> Grid]:file:_files" "-RESULT[<str> Skeleton]:file:_files" "-VECTOR[<str> Skeleton]:file:_files" "-METHOD[<str> Method]:file:_files" "-INIT_METHOD[<str> Initialisation]:file:_files" "-INIT_THRESHOLD[<double>Threshold]:file:_files" "-CONVERGENCE[<num> Convergence]:file:_files"  
          ;;
        2)
          _arguments \
            "-FEATURES[<str> Features]:file:_files" "-VARIANCE[<str> Variance]:file:_files" "-SEED_GRID[<str> Seeds Grid]:file:_files" "-SEED_POINTS[<str> Seed Points]:file:_files" "-SEED_TYPE[<str> Seed Type]:file:_files" "-METHOD[<str> Method]:file:_files" "-BAND_WIDTH[<double> Bandwidth (Cells)]:file:_files" "-NORMALIZE[<str> Normalize Features]:file:_files" "-DW_WEIGHTING[<str> Weighting Function]:file:_files" "-DW_IDW_POWER[<double>Power]:file:_files" "-DW_BANDWIDTH[<double>Bandwidth]:file:_files"  
          ;;
        3)
          _arguments \
            "-SEEDS[<str> Seeds]:file:_files" "-FEATURES[<str> Features]:file:_files" "-SEGMENTS[<str> Segments]:file:_files" "-SIMILARITY[<str> Similarity]:file:_files" "-TABLE[<str> Seeds]:file:_files" "-NORMALIZE[<str> Normalize Features]:file:_files" "-NEIGHBOUR[<str> Neighbourhood]:file:_files" "-METHOD[<str> Method]:file:_files" "-SIG_1[<double> Variance in Feature Space]:file:_files" "-SIG_2[<double> Variance in Position Space]:file:_files" "-THRESHOLD[<double>Similarity Threshold]:file:_files" "-REFRESH[<str> Refresh]:file:_files" "-LEAFSIZE[<num> Leaf Size (for Speed Optimisation)]:file:_files"  
          ;;
        4)
          _arguments \
            "-FEATURES[<str> Features]:file:_files" "-NORMALIZE[<str> Normalize]:file:_files" "-POLYGONS[<str> Segments]:file:_files" "-MAX_ITERATIONS[<num> Maximum Iterations]:file:_files" "-REGULARIZATION[<double>Regularization]:file:_files" "-SIZE[<num> Region Size]:file:_files" "-SIZE_MIN[<num> Minimum Region Size]:file:_files" "-SUPERPIXELS_DO[<str> Create Superpixel Grids]:file:_files" "-SUPERPIXELS[<str> Superpixels]:file:_files" "-POSTPROCESSING[<str> Post-Processing]:file:_files" "-NCLUSTER[<num> Number of Clusters]:file:_files" "-SPLIT_CLUSTERS[<str> Split Clusters]:file:_files"  
          ;;
        5)
          _arguments \
            "-INPUT[<str> Input]:file:_files" "-OUTPUT[<str> Output]:file:_files" "-NEIGHBOUR[<str>Neighbourhood]:file:_files"  
          ;;
        obia)
          _arguments \
            "-FEATURES[<str> Features]:file:_files" "-NORMALIZE[<str> Normalize]:file:_files" "-OBJECTS[<str> Segments]:file:_files" "-SEEDS_BAND_WIDTH[<double>Band Width for Seed Point Generation]:file:_files" "-RGA_NEIGHBOUR[<str> Neighbourhood]:file:_files" "-RGA_METHOD[<str> Distance]:file:_files" "-RGA_SIG_1[<double> Variance in Feature Space]:file:_files" "-RGA_SIG_2[<double> Variance in Position Space]:file:_files" "-RGA_SIMILARITY[<double> Similarity Threshold]:file:_files" "-MAJORITY_RADIUS[<num> Generalization]:file:_files" "-CLASSIFICATION[<str> Classification]:file:_files" "-SPLIT_POLYGONS[<str> Split Distinct Polygon Parts]:file:_files" "-NCLUSTER[<num> Number of Clusters]:file:_files" "-CLASSIFIER[<str> Method]:file:_files" "-SAMPLES[<str> Training Samples]:file:_files"  
          ;;
     esac
     ;;
   esac
}

_imagery_svm(){
  _arguments -C "1: :->cmds" "*::arg:->args" 
 
  case "$state" in
    cmds)
      _values "imagery_svm command" \
        "0[SVM Classification]" \

      ;;
    args)
      case "$line[1]" in
        0)
          _arguments \
            "-GRIDS[<str> Grids]:file:_files" "-CLASSES[<str> Classification]:file:_files" "-CLASSES_LUT[<str> Look-up Table]:file:_files" "-SCALING[<str> Scaling]:file:_files" "-MESSAGE[<str> Verbose Messages]:file:_files" "-MODEL_SRC[<str> Model Source]:file:_files" "-MODEL_LOAD[<str> Restore Model from File]:file:_files" "-ROI[<str> Training Areas]:file:_files" "-ROI_ID[<str> Class Identifier]:file:_files" "-MODEL_SAVE[<str> Store Model to File]:file:_files" "-SVM_TYPE[<str> SVM Type]:file:_files" "-KERNEL_TYPE[<str> Kernel Type]:file:_files" "-DEGREE[<num> Degree]:file:_files" "-GAMMA[<double> Gamma]:file:_files" "-COEF0[<double> coef0]:file:_files" "-COST[<double> C]:file:_files" "-NU[<double> nu-SVR]:file:_files" "-EPS_SVR[<double> SVR Epsilon]:file:_files" "-CACHE_SIZE[<double>Cache Size]:file:_files" "-EPS[<double> Epsilon]:file:_files" "-SHRINKING[<str> Shrinking]:file:_files" "-PROBABILITY[<str> Probability Estimates]:file:_files" "-CROSSVAL[<num> Cross Validation]:file:_files"  
          ;;
     esac
     ;;
   esac
}

_imagery_tools(){
  _arguments -C "1: :->cmds" "*::arg:->args" 
 
  case "$state" in
    cmds)
      _values "imagery_tools command" \
        "0[Vegetation Index (Distance Based)]" \
        "1[Vegetation Index (Slope Based)]" \
        "2[Enhanced Vegetation Index]" \
        "3[Tasseled Cap Transformation]" \
        "4[IHS Sharpening]" \
        "5[Colour Normalized Brovey Sharpening]" \
        "6[Colour Normalized Spectral Sharpening]" \
        "7[Principal Component Based Image Sharpening]" \
        "8[Top of Atmosphere Reflectance]" \
        "9[Automated Cloud Cover Assessment]" \
        "10[Landsat Import with Options]" \
        "11[Textural Features]" \
        "12[Local Statistical Measures]" \
