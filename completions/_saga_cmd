      case "$line[1]" in
        0)
          _arguments \
            "-TABLE[<str> Table]:file:_files" "-HEADLINE[<str> Headline]:file:_files" "-STRQUOTA[<str> Strings in Quota]:file:_files" "-SEPARATOR[<str>Separator]:file:_files" "-SEP_OTHER[<str>other]:file:_files" "-FILENAME[<str> File]:file:_files"  
          ;;
