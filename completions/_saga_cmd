            "-TABLE[<str> Table]:file:_files" "-HEADLINE[<str> Headline]:file:_files" "-SEPARATOR[<str>Separator]:file:_files" "-ENCODING[<str> Encoding]:file:_files" "-SEP_OTHER[<str>Separator (other)]:file:_files" "-FILENAME[<str> File]:file:_files"  
