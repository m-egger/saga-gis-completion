            "-POLYGONS[<str> Polygons]:file:_files" "-POINTS[<str> Points]:file:_files" "-NUMFIELD[<str> Number of Points]:file:_files" "-NUMPOINTS[<num>Number of Points]:file:_files" "-MAXITER[<num> Maximum Iterations]:file:_files"  
          ;;
        5)
          _arguments \
            "-LINES[<str> Lines]:file:_files" "-POINTS[<str> Points]:file:_files" "-ADD_POINT_ORDER[<str>Add Point Order]:file:_files" "-ADD[<str> Insert Additional Points]:file:_files" "-METHOD_INSERT[<str> Insertion]:file:_files" "-DIST[<double> Insertion Distance]:file:_files"  
          ;;
        6)
          _arguments \
            "-INPUT[<str> Points]:file:_files" "-OUTPUT[<str>Output]:file:_files" "-X[<str> X]:file:_files" "-Y[<str> Y]:file:_files" "-Z[<str> Z]:file:_files" "-M[<str> M]:file:_files" "-LON[<str> Longitude]:file:_files" "-LAT[<str> Latitude]:file:_files"  
          ;;
        7)
          _arguments \
            "-POINTS[<str> Points]:file:_files" "-FIELD[<str> Attribute]:file:_files" "-RESULT[<str> Result]:file:_files" "-NUMERIC[<str>Value Aggregation]:file:_files" "-METHOD[<str> Point to Keep]:file:_files"  
          ;;
        8)
          _arguments \
            "-POINTS[<str> Points]:file:_files" "-POLYGONS[<str>Polygons]:file:_files" "-FIELD[<str> Add Attribute to Clipped Points]:file:_files" "-CLIPS[<str> Clipped Points]:file:_files" "-METHOD[<str> Clipping Options]:file:_files"  
          ;;
        9)
          _arguments \
            "-OUTPUT[<str> Ouput]:file:_files" "-POINTS[<str> Points]:file:_files" "-DIRECTIONS[<num> Number of Directions]:file:_files" "-TOLERANCE[<double>Tolerance (Degree)]:file:_files"  
          ;;
        10)
          _arguments \
            "-INPUT[<str> Points]:file:_files" "-OUTPUT[<str> Result]:file:_files" "-POLYGONS[<str>Polygons]:file:_files" "-FIELDS[<str> Attributes]:file:_files"  
          ;;
        11)
          _arguments \
            "-POINTS[<str> Points]:file:_files" "-FIELD[<str> Attribute]:file:_files" "-FILTER[<str> Filtered Points]:file:_files" "-RADIUS[<double> Radius]:file:_files" "-MINNUM[<num> Minimum Number of Points]:file:_files" "-MAXNUM[<num> Maximum Number of Points]:file:_files" "-QUADRANTS[<str> Quadrants]:file:_files" "-METHOD[<str> Filter Criterion]:file:_files" "-TOLERANCE[<double>Tolerance]:file:_files" "-PERCENT[<double> Percentile]:file:_files"  
          ;;
        12)
          _arguments \
            "-SHAPES[<str> Points]:file:_files" "-HULLS[<str> Convex Hull]:file:_files" "-BOXES[<str> Minimum Bounding Box]:file:_files" "-POLYPOINTS[<str>Hull Construction]:file:_files" "-POLYGONCVX[<str>Polygon Convexity]:file:_files"  
          ;;
        14)
          _arguments \
            "-POINTS[<str> Points]:file:_files" "-FIELD[<str> Attribute]:file:_files" "-OUTPUT_PC[<str> Output to Point Cloud]:file:_files" "-THINNED[<str> Thinned Points]:file:_files" "-THINNED_PC[<str> Thinned Points]:file:_files" "-RESOLUTION[<double>Resolution]:file:_files" "-METHOD[<str> Method]:file:_files"  
          ;;
        15)
          _arguments \
            "-MULTIPOINTS[<str>Multipoints]:file:_files" "-POINTS[<str> Points]:file:_files" "-ADD_INDEX[<str> Add Part and Point Index]:file:_files"  
          ;;
        16)
          _arguments \
            "-POINTS[<str> Points]:file:_files" "-POLYGONS[<str>Polygons]:file:_files" "-FRAME[<double>Frame Size]:file:_files"  
          ;;
        17)
          _arguments \
            "-REFERENCE[<str> Reference Points]:file:_files" "-REFERENCE_ID[<str>ID]:file:_files" "-OBSERVATIONS[<str>Observations]:file:_files" "-X[<str> X]:file:_files" "-Y[<str> Y]:file:_files" "-TRACK[<str> Track]:file:_files" "-DATE[<str> Date]:file:_files" "-TIME[<str> Time]:file:_files" "-PARAMETER[<str> Parameter]:file:_files" "-AGGREGATED[<str> Aggregated]:file:_files" "-TIME_SPAN[<str> Time Span Aggregation]:file:_files" "-FIX_TIME[<double> Fixed Time Span (minutes)]:file:_files" "-OFF_TIME[<double> Fixed Time Span Offset (minutes)]:file:_files" "-EPS_TIME[<double> Maximum Time Span (Seconds)]:file:_files" "-EPS_SPACE[<double>Maximum Distance]:file:_files" "-VERBOSE[<str> Verbose]:file:_files" "-POLAR[<str> Polar Coordinates]:file:_files"  
          ;;
        18)
          _arguments \
            "-INPUT[<str> Points]:file:_files" "-SNAP[<str> Snap Features]:file:_files" "-OUTPUT[<str> Result]:file:_files" "-MOVES[<str> Moves]:file:_files" "-DISTANCE[<double>Search Distance]:file:_files"  
          ;;
        19)
          _arguments \
            "-INPUT[<str> Points]:file:_files" "-SNAP[<str> Snap Features]:file:_files" "-OUTPUT[<str> Result]:file:_files" "-MOVES[<str> Moves]:file:_files" "-DISTANCE[<double>Search Distance]:file:_files"  
          ;;
        20)
          _arguments \
            "-INPUT[<str> Points]:file:_files" "-GRID[<str> Grid]:file:_files" "-OUTPUT[<str> Result]:file:_files" "-MOVES[<str> Moves]:file:_files" "-DISTANCE[<double>Search Distance]:file:_files" "-SHAPE[<str> Search Shape]:file:_files" "-EXTREME[<str> Extreme]:file:_files"  
          ;;
        21)
          _arguments \
            "-POINTS[<str> Points]:file:_files" "-EXTENT[<str> Target Area]:file:_files" "-GRIDSYSTEM_D[<double>Cell Size]:file:_files" "-GRIDSYSTEM_X[<double>Lower Left Center Cell X-Coordinate]:file:_files" "-GRIDSYSTEM_Y[<double>Lower Left Center Cell Y-Coordinate]:file:_files" "-GRIDSYSTEM_NX[<num> Number of Columns]:file:_files" "-GRIDSYSTEM_NY[<num> Number of Rows]:file:_files" "-GRIDSYSTEM_FILE[<str>Grid File]:file:_files" "-SHAPES[<str> Shapes Extent]:file:_files" "-POLYGONS[<str> Polygons]:file:_files" "-XMIN[<double> Left]:file:_files" "-XMAX[<double> Right]:file:_files" "-YMIN[<double> Bottom]:file:_files" "-YMAX[<double> Top]:file:_files" "-NX[<num> Columns]:file:_files" "-NY[<num> Rows]:file:_files" "-BUFFER[<double> Buffer]:file:_files" "-COUNT[<num> Number of Points]:file:_files" "-DISTRIBUTE[<str> Number for...]:file:_files" "-ITERATIONS[<num> Iterations]:file:_files" "-DISTANCE[<double> Distance]:file:_files"  
          ;;
        22)
          _arguments \
            "-INPUT[<str> Points]:file:_files" "-SNAP[<str> Snap Features]:file:_files" "-OUTPUT[<str> Result]:file:_files" "-MOVES[<str> Moves]:file:_files" "-DISTANCE[<double>Search Distance]:file:_files"  
          ;;
        23)
          _arguments \
            "-LOWER[<str> Lower Surface]:file:_files" "-UPPER[<str> Upper Surface]:file:_files" "-POINTS[<str> Points]:file:_files" "-Z_FIELD[<str>Z]:file:_files" "-COPY[<str> Copy Selection]:file:_files"  
          ;;
        24)
          _arguments \
            "-POINTS[<str> Points]:file:_files" "-RESULT[<str> Result]:file:_files" "-DISTANCES[<str>Distances]:file:_files" "-LINES[<str> Lines]:file:_files" "-LINE_ID[<str> Identifier]:file:_files"  
          ;;
        points_to_contour)
          _arguments \
            "-SHAPES[<str> Points]:file:_files" "-CONTOUR[<str> Contour Lines]:file:_files" "-ATTRIBUTE[<str> Attribute]:file:_files" "-CELL_SIZE[<double>Precision]:file:_files" "-ZSTEP[<double> Equidistance]:file:_files"  
          ;;
     esac
     ;;
   esac
}

_shapes_polygons(){
  _arguments -C "1: :->cmds" "*::arg:->args" 
 
  case "$state" in
    cmds)
      _values "shapes_polygons command" \
        "1[Polygon Centroids]" \
        "2[Polygon Properties]" \
        "3[Convert Lines to Polygons]" \
        "4[Point Statistics for Polygons]" \
        "5[Polygon Dissolve]" \
        "6[Convert Polygon/Line Vertices to Points]" \
        "7[Polygon Shape Indices]" \
        "8[Polygon-Line Intersection]" \
        "9[Polygons to Edges and Nodes]" \
        "10[Polygon Parts to Separate Polygons]" \
        "11[Polygon Clipping]" \
        "12[Polygon Self-Intersection]" \
        "14[Intersect]" \
        "15[Difference]" \
        "16[Symmetrical Difference]" \
        "17[Union]" \
        "18[Update]" \
        "19[Identity]" \
        "20[Add Point Attributes to Polygons]" \
        "21[Flatten Polygon Layer]" \
        "22[Shared Polygon Edges]" \
        "23[Polygon Generalization]" \
        "length_of_lines[Total Length of Lines per Polygon]" \
        "max_interior_circles[Largest Circles in Polygons]" \
        "remove_from_boundary[Remove Boundary Polygons]" \

      ;;
    args)
      case "$line[1]" in
        1)
          _arguments \
            "-POLYGONS[<str> Polygons]:file:_files" "-CENTROIDS[<str>Centroids]:file:_files" "-METHOD[<str> Centroids for each part]:file:_files" "-INSIDE[<str> Force Inside]:file:_files"  
          ;;
        2)
          _arguments \
            "-POLYGONS[<str> Polygons]:file:_files" "-OUTPUT[<str> Polygons with Property Attributes]:file:_files" "-FIELDS[<str> Copy Attributes]:file:_files" "-BPARTS[<str> Number of Parts]:file:_files" "-BPOINTS[<str> Number of Vertices]:file:_files" "-BEXTENT[<str> Extent]:file:_files" "-BCENTER[<str> Centroid]:file:_files" "-BLENGTH[<str> Perimeter]:file:_files" "-BAREA[<str> Area]:file:_files" "-SCALING[<double>Scaling]:file:_files"  
          ;;
        3)
          _arguments \
            "-LINES[<str> Lines]:file:_files" "-POLYGONS[<str>Polygons]:file:_files" "-RINGS[<str> Rings]:file:_files" "-SINGLE[<str> Create One Single Multipart Polygon]:file:_files" "-MERGE[<str> Merge Connected Line Parts]:file:_files" "-SPLIT[<str> Line Parts as Individual Polygons]:file:_files"  
          ;;
        4)
          _arguments \
            "-POINTS[<str> Points]:file:_files" "-FIELDS[<str> Attributes]:file:_files" "-POLYGONS[<str> Polygons]:file:_files" "-STATISTICS[<str>Statistics]:file:_files" "-SUM[<str> Sum]:file:_files" "-AVG[<str> Mean]:file:_files" "-VAR[<str> Variance]:file:_files" "-DEV[<str> Deviation]:file:_files" "-MIN[<str> Minimum]:file:_files" "-MAX[<str> Maximum]:file:_files" "-NUM[<str> Count]:file:_files" "-FIELD_NAME[<str>Field Naming]:file:_files"  
          ;;
        5)
          _arguments \
            "-POLYGONS[<str> Polygons]:file:_files" "-DISSOLVED[<str> Dissolved Polygons]:file:_files" "-FIELDS[<str> Dissolve Field(s)]:file:_files" "-STATISTICS[<str> Statistics Field(s)]:file:_files" "-STAT_SUM[<str> Sum]:file:_files" "-STAT_AVG[<str> Mean]:file:_files" "-STAT_MIN[<str> Minimum]:file:_files" "-STAT_MAX[<str> Maximum]:file:_files" "-STAT_RNG[<str> Range]:file:_files" "-STAT_DEV[<str> Deviation]:file:_files" "-STAT_VAR[<str> Variance]:file:_files" "-STAT_LST[<str> Listing]:file:_files" "-STAT_NUM[<str> Count]:file:_files" "-STAT_NAMING[<str> Field Naming]:file:_files" "-BND_KEEP[<str> Keep Boundaries]:file:_files" "-MIN_AREA[<double> Minimum Area]:file:_files" "-SPLIT_DISTINCT[<str>Split Distinct Polygons]:file:_files"  
          ;;
        6)
          _arguments \
            "-SHAPES[<str>Shapes]:file:_files" "-POINTS[<str>Points]:file:_files"  
          ;;
        7)
          _arguments \
            "-SHAPES[<str> Shapes]:file:_files" "-INDEX[<str> Shape Indices]:file:_files" "-DMAX[<str> Maximum Diameter]:file:_files" "-GYROS[<str> Diameter of Gyration]:file:_files" "-FERET[<str> Feret Diameters]:file:_files" "-FERET_DIRS[<num>Number of Directions]:file:_files"  
          ;;
        8)
          _arguments \
            "-POLYGONS[<str> Polygons]:file:_files" "-LINES[<str> Lines]:file:_files" "-INTERSECT[<str> Intersection]:file:_files" "-SPLIT_PARTS[<str>Split Parts]:file:_files" "-METHOD[<str> Method]:file:_files"  
          ;;
        9)
          _arguments \
            "-POLYGONS[<str>Polygons]:file:_files" "-EDGES[<str> Edges]:file:_files" "-NODES[<str> Nodes]:file:_files"  
          ;;
        10)
          _arguments \
            "-POLYGONS[<str>Polygons]:file:_files" "-PARTS[<str> Polygon Parts]:file:_files" "-LAKES[<str> Lakes]:file:_files"  
          ;;
        11)
          _arguments \
            "-CLIP[<str> Clip Features]:file:_files" "-S_INPUT[<str> Input Features]:file:_files" "-S_OUTPUT[<str>Output Features]:file:_files" "-M_INPUT[<str> Input Features]:file:_files" "-M_OUTPUT[<str>Output Features]:file:_files" "-DISSOLVE[<str>Dissolve Clip Features]:file:_files" "-MULTIPLE[<str>Multiple Input Features]:file:_files"  
          ;;
        12)
          _arguments \
            "-POLYGONS[<str> Polygons]:file:_files" "-ID[<str> Identifier]:file:_files" "-INTERSECT[<str>Intersection]:file:_files"  
          ;;
        14)
          _arguments \
            "-A[<str> Layer A]:file:_files" "-B[<str> Layer B]:file:_files" "-RESULT[<str>Intersect]:file:_files" "-SPLIT[<str> Split Parts]:file:_files"  
          ;;
        15)
          _arguments \
            "-A[<str> Layer A]:file:_files" "-B[<str> Layer B]:file:_files" "-RESULT[<str>Difference]:file:_files" "-SPLIT[<str> Split Parts]:file:_files"  
          ;;
        16)
          _arguments \
            "-A[<str> Layer A]:file:_files" "-B[<str> Layer B]:file:_files" "-RESULT[<str>Symmetrical Difference]:file:_files" "-SPLIT[<str> Split Parts]:file:_files"  
          ;;
        17)
          _arguments \
            "-A[<str> Layer A]:file:_files" "-B[<str> Layer B]:file:_files" "-RESULT[<str>Union]:file:_files" "-SPLIT[<str> Split Parts]:file:_files"  
          ;;
        18)
          _arguments \
            "-A[<str> Layer A]:file:_files" "-B[<str> Layer B]:file:_files" "-RESULT[<str>Update]:file:_files" "-SPLIT[<str> Split Parts]:file:_files"  
          ;;
        19)
          _arguments \
            "-A[<str> Layer A]:file:_files" "-B[<str> Layer B]:file:_files" "-RESULT[<str>Identity]:file:_files" "-SPLIT[<str> Split Parts]:file:_files"  
          ;;
        20)
          _arguments \
            "-INPUT[<str> Polygons]:file:_files" "-POINTS[<str> Points]:file:_files" "-FIELDS[<str> Attributes]:file:_files" "-OUTPUT[<str> Result]:file:_files" "-ADD_LOCATION_INFO[<str>Add Location Info]:file:_files"  
          ;;
        21)
          _arguments \
            "-INPUT[<str> Input]:file:_files" "-OUTPUT[<str>Output]:file:_files"  
          ;;
        22)
          _arguments \
            "-POLYGONS[<str> Polygons]:file:_files" "-ATTRIBUTE[<str> Attribute]:file:_files" "-EDGES[<str> Edges]:file:_files" "-VERTICES[<str> Check Vertices]:file:_files" "-EPSILON[<double>Tolerance]:file:_files" "-DOUBLE[<str> Double Edges]:file:_files"  
          ;;
        23)
          _arguments \
            "-POLYGONS[<str> Shapes]:file:_files" "-GENERALIZED[<str> Generalized Shapes]:file:_files" "-THRESHOLD[<double>Area Threshold]:file:_files" "-JOIN_TO[<str> Join to Neighbour with ...]:file:_files" "-VERTICES[<str> Check Vertices]:file:_files" "-EPSILON[<double> Tolerance]:file:_files"  
          ;;
        length_of_lines)
          _arguments \
            "-POLYGONS[<str>Polygons]:file:_files" "-LINES[<str> Lines]:file:_files"  
          ;;
        max_interior_circles)
          _arguments \
            "-POLYGONS[<str> Polygons]:file:_files" "-CIRCLES[<str> Circles]:file:_files" "-RESOLUTION[<double>Resolution]:file:_files"  
          ;;
        remove_from_boundary)
          _arguments \
            "-POLYGONS[<str>Polygons]:file:_files" "-RESULT[<str> Result]:file:_files" "-METHOD[<str> Method]:file:_files"  
          ;;
     esac
     ;;
   esac
}

_shapes_tools(){
  _arguments -C "1: :->cmds" "*::arg:->args" 
 
  case "$state" in
    cmds)
      _values "shapes_tools command" \
        "0[Create New Shapes Layer]" \
        "2[Merge Layers]" \
        "3[Select by Attributes... (Numerical Expression)]" \
        "4[Select by Attributes... (String Expression)]" \
        "5[Select by Location...]" \
        "6[Copy Selection to New Shapes Layer]" \
        "7[Delete Selection from Shapes Layer]" \
        "8[Invert Selection of Shapes Layer]" \
        "9[Split Shapes Layer Completely]" \
        "10[Transform Shapes]" \
        "11[Create Chart Layer (Bars/Sectors)]" \
        "12[Create Graticule]" \
        "13[Copy Shapes from Region]" \
        "15[Split Shapes Layer]" \
        "16[Split Shapes Layer Randomly]" \
        "17[Split Table/Shapes by Attribute]" \
        "18[Shapes Buffer]" \
        "19[Get Shapes Extents]" \
        "20[QuadTree Structure to Shapes]" \
        "21[Polar to Cartesian Coordinates]" \
        "22[Generate Shapes]" \
        "23[Convert Vertex Type (2D/3D)]" \
        "24[Merge Tables]" \
        "25[Land Use Scenario Generator]" \
        "26[Select Shapes from List]" \
        "27[Remove Invalid Shapes]" \
        "28[Copy Shapes]" \
        "29[Focal Mechanism (Beachball Plots)]" \
        "30[Gradient Lines from Points]" \
        "select_and_delete[Select and Delete]" \

      ;;
    args)
      case "$line[1]" in
        0)
          _arguments \
            "-SHAPES[<str> Shapes]:file:_files" "-NAME[<str> Name]:file:_files" "-TYPE[<str> Geometry Type]:file:_files" "-VERTEX[<str> Vertex Type]:file:_files" "-CRS_STRING[<str> Coordinate System Definition]:file:_files" "-NFIELDS[<num> Number of Attributes]:file:_files" "-FIELDS_NAME0[<str>Field 1]:file:_files" "-FIELDS_TYPE0[<str>Type]:file:_files" "-FIELDS_NAME1[<str>Field 2]:file:_files" "-FIELDS_TYPE1[<str>Type]:file:_files"  
          ;;
        2)
          _arguments \
            "-INPUT[<str> Layers]:file:_files" "-MERGED[<str> Merged Layer]:file:_files" "-SRCINFO[<str>Add Source Information]:file:_files" "-MATCH[<str> Match Fields by Name]:file:_files" "-DELETE[<str> Delete]:file:_files"  
          ;;
        3)
          _arguments \
            "-SHAPES[<str> Shapes]:file:_files" "-FIELD[<str> Attribute]:file:_files" "-EXPRESSION[<str>Expression]:file:_files" "-USE_NODATA[<str>Use No-Data]:file:_files" "-METHOD[<str> Method]:file:_files"  
          ;;
        4)
          _arguments \
            "-SHAPES[<str> Shapes]:file:_files" "-FIELD[<str> Attribute]:file:_files" "-EXPRESSION[<str>Expression]:file:_files" "-CASE[<str> Case Sensitive]:file:_files" "-COMPARE[<str> Select if...]:file:_files" "-METHOD[<str> Method]:file:_files"  
          ;;
        5)
          _arguments \
            "-SHAPES[<str> Shapes to Select From]:file:_files" "-LOCATIONS[<str>Locations]:file:_files" "-CONDITION[<str>Condition]:file:_files" "-METHOD[<str> Method]:file:_files"  
          ;;
        6)
          _arguments \
            "-INPUT[<str> Input]:file:_files" "-OUTPUT[<str>Output]:file:_files"  
          ;;
        7)
          _arguments \
            "-INPUT[<str>Input]:file:_files"  
          ;;
        8)
          _arguments \
            "-INPUT[<str>Input]:file:_files"  
          ;;
        9)
          _arguments \
            "-SHAPES[<str>Input]:file:_files" "-FIELD[<str> Attribute]:file:_files" "-LIST[<str> Output]:file:_files" "-NAMING[<str>Name by...]:file:_files"  
          ;;
        10)
          _arguments \
            "-SHAPES[<str> Shapes]:file:_files" "-TRANSFORM[<str> Transformed Shapes]:file:_files" "-MOVEX[<double> dX]:file:_files" "-MOVEY[<double> dY]:file:_files" "-MOVEZ[<double> dZ]:file:_files" "-ANCHORX[<double> X]:file:_files" "-ANCHORY[<double> Y]:file:_files" "-ANCHORZ[<double> Z]:file:_files" "-ROTATEX[<double> Angle X]:file:_files" "-ROTATEY[<double> Angle Y]:file:_files" "-ROTATEZ[<double> Angle Z]:file:_files" "-SCALEX[<double> Scale Factor X]:file:_files" "-SCALEY[<double> Scale Factor Y]:file:_files" "-SCALEZ[<double> Scale Factor Z]:file:_files" "-REFLECTION_TYPE[<str>Type of Reflection]:file:_files"  
          ;;
        11)
          _arguments \
            "-INPUT[<str> Shapes]:file:_files" "-OUTPUT[<str> Chart]:file:_files" "-SIZE[<str> Size]:file:_files" "-FIELDS[<str> Attributes]:file:_files" "-MAXSIZE[<double>Maximum size]:file:_files" "-MINSIZE[<double>Minimum size]:file:_files" "-TYPE[<str> Type]:file:_files"  
          ;;
        12)
          _arguments \
            "-GRATICULE_LINE[<str> Graticule]:file:_files" "-GRATICULE_RECT[<str> Graticule]:file:_files" "-TYPE[<str> Type]:file:_files" "-EXTENT[<str> Extent]:file:_files" "-EXTENT_X_MIN[<double>Width]:file:_files" "-EXTENT_X_MAX[<double>Width]:file:_files" "-EXTENT_Y_MIN[<double>Height]:file:_files" "-EXTENT_Y_MAX[<double>Height]:file:_files" "-DIVISION_X[<double> Division Width]:file:_files" "-DIVISION_Y[<double> Division Height]:file:_files" "-ALIGNMENT[<str> Alignment]:file:_files" "-ROUND[<str> Round]:file:_files"  
          ;;
        13)
          _arguments \
            "-SHAPES[<str> Shapes]:file:_files" "-CUT[<str> Copy]:file:_files" "-METHOD[<str> Method]:file:_files" "-EXTENT[<str> Region]:file:_files" "-AX[<double> Left]:file:_files" "-BX[<double> Right]:file:_files" "-AY[<double> Bottom]:file:_files" "-BY[<double> Top]:file:_files" "-DX[<double> Horizontal Range]:file:_files" "-DY[<double> Vertical Range]:file:_files" "-GRID_SYS_D[<double>Cell Size]:file:_files" "-GRID_SYS_X[<double>Lower Left Center Cell X-Coordinate]:file:_files" "-GRID_SYS_Y[<double>Lower Left Center Cell Y-Coordinate]:file:_files" "-GRID_SYS_NX[<num> Number of Columns]:file:_files" "-GRID_SYS_NY[<num> Number of Rows]:file:_files" "-GRID_SYS_FILE[<str>Grid File]:file:_files" "-SHAPES_EXT[<str> Shapes]:file:_files" "-POLYGONS[<str> Polygons]:file:_files" "-OVERLAP[<double> Minimum Overlap]:file:_files"  
          ;;
        15)
          _arguments \
            "-SHAPES[<str>Shapes]:file:_files" "-CUTS[<str> Tiles]:file:_files" "-EXTENT[<str>Extent]:file:_files" "-NX[<num> Number of horizontal tiles]:file:_files" "-NY[<num> Number of vertical tiles]:file:_files" "-METHOD[<str>Method]:file:_files"  
          ;;
        16)
          _arguments \
            "-SHAPES[<str> Shapes]:file:_files" "-FIELD[<str> Categories]:file:_files" "-A[<str> Group A]:file:_files" "-B[<str> Group B]:file:_files" "-PERCENT[<double>Relation B / A]:file:_files" "-EXACT[<str> Exact]:file:_files"  
          ;;
        17)
