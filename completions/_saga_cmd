          ;;
        15)
          _arguments \
            "-PG_HOST[<str> Host]:file:_files" "-PG_PORT[<num> Port]:file:_files" "-PG_DB[<str> Database]:file:_files" "-PG_USER[<str> User]:file:_files" "-PG_PWD[<str> Password]:file:_files" "-TABLE[<str> Query Result]:file:_files" "-TABLES[<str> Tables]:file:_files" "-FIELDS[<str> Fields]:file:_files" "-WHERE[<str> Where]:file:_files" "-GROUP[<str> Group by]:file:_files" "-HAVING[<str> Having]:file:_files" "-ORDER[<str> Order by]:file:_files" "-DISTINCT[<str>Distinct]:file:_files"  
          ;;
        16)
          _arguments \
            "-PG_HOST[<str> Host]:file:_files" "-PG_PORT[<num> Port]:file:_files" "-PG_DB[<str> Database]:file:_files" "-PG_USER[<str> User]:file:_files" "-PG_PWD[<str> Password]:file:_files" "-TABLE[<str> Query Result]:file:_files" "-WHERE[<str> Where]:file:_files" "-HAVING[<str> Having]:file:_files" "-ORDER[<str> Order by]:file:_files" "-DISTINCT[<str>Distinct]:file:_files"  
          ;;
        20)
          _arguments \
            "-PG_HOST[<str> Host]:file:_files" "-PG_PORT[<num> Port]:file:_files" "-PG_DB[<str> Database]:file:_files" "-PG_USER[<str> User]:file:_files" "-PG_PWD[<str> Password]:file:_files" "-SHAPES[<str> Shapes]:file:_files" "-COLLECTION[<str>Shapes]:file:_files" "-DB_TABLE[<str> Table]:file:_files"  
          ;;
        21)
          _arguments \
            "-PG_HOST[<str> Host]:file:_files" "-PG_PORT[<num> Port]:file:_files" "-PG_DB[<str> Database]:file:_files" "-PG_USER[<str> User]:file:_files" "-PG_PWD[<str> Password]:file:_files" "-SHAPES[<str> Shapes]:file:_files" "-SHAPES_PK[<str> Primary Key]:file:_files" "-SHAPES_NN[<str> Not Null]:file:_files" "-SHAPES_UQ[<str> Unique]:file:_files" "-NAME[<str> Table Name]:file:_files" "-EXISTS[<str> If table exists...]:file:_files" "-CRS_CODE[<num> Authority Code]:file:_files" "-CRS_AUTHORITY[<str>Authority]:file:_files"  
