          ;;
        7)
          _arguments \
            "-LINES_IN[<str> Lines]:file:_files" "-LINES_OUT[<str> Smoothed Lines]:file:_files" "-METHOD[<str> Method]:file:_files" "-SENSITIVITY[<num> Sensitivity]:file:_files" "-ITERATIONS[<num> Iterations]:file:_files" "-PRESERVATION[<double>Preservation]:file:_files" "-SIGMA[<double> Sigma]:file:_files"  
          ;;
