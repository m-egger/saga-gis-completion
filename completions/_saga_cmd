          ;;
        11)
          _arguments \
            "-INPUT[<str> Input]:file:_files" "-FIELDS[<str> Attributes]:file:_files" "-EXTREME_ID[<str> Attribute]:file:_files" "-EXTREME_VALUE[<str>Value]:file:_files" "-OUTPUT[<str> Output]:file:_files" "-TYPE[<str> TYPE]:file:_files" "-IDENTIFY[<str> Attribute Identification]:file:_files"  
          ;;
        15)
          _arguments \
            "-TABLE[<str> Table]:file:_files" "-FIELDS[<str> Attributes]:file:_files" "-STATISTICS[<str>Statistics]:file:_files"  
          ;;
        16)
          _arguments \
            "-TABLE[<str> Table]:file:_files" "-FIELDS[<str> Attributes]:file:_files" "-RESULT_TABLE[<str> Result]:file:_files" "-RESULT_SHAPES[<str>Result]:file:_files" "-MEAN[<str> Arithmetic Mean]:file:_files" "-MIN[<str> Minimum]:file:_files" "-MAX[<str> Maximum]:file:_files" "-RANGE[<str> Range]:file:_files" "-SUM[<str> Sum]:file:_files" "-NUM[<str> Number of Values]:file:_files" "-VAR[<str> Variance]:file:_files" "-STDDEV[<str> Standard Deviation]:file:_files" "-PCTL[<str> Percentile]:file:_files" "-PCTL_VAL[<double> Value]:file:_files"  
