            "-GRID_X[<str> X Coordinates]:file:_files" "-GRID_Y[<str> Y Coordinates]:file:_files" "-GRIDS[<str> Grids]:file:_files" "-OUTPUT[<str> Grids]:file:_files" "-RESAMPLING[<str> Resampling]:file:_files" "-BYTEWISE[<str> Bytewise Interpolation]:file:_files" "-KEEP_TYPE[<str> Preserve Data Type]:file:_files" "-TARGET_DEFINITION[<str> Target Grid System]:file:_files" "-TARGET_USER_SIZE[<double>Cellsize]:file:_files" "-TARGET_USER_XMIN[<double>West]:file:_files" "-TARGET_USER_XMAX[<double>East]:file:_files" "-TARGET_USER_YMIN[<double>South]:file:_files" "-TARGET_USER_YMAX[<double>North]:file:_files" "-TARGET_USER_COLS[<num> Columns]:file:_files" "-TARGET_USER_ROWS[<num> Rows]:file:_files" "-TARGET_USER_FLAT[<str> Rounding]:file:_files" "-TARGET_USER_FITS[<str> Fit]:file:_files" "-TARGET_TEMPLATE[<str> Target System]:file:_files"  
          ;;
        10)
          _arguments \
            "-REF_SOURCE[<str> Reference Points (Origin)]:file:_files" "-REF_TARGET[<str> Reference Points (Projection)]:file:_files" "-XFIELD[<str> x Position]:file:_files" "-YFIELD[<str> y Position]:file:_files" "-METHOD[<str> Method]:file:_files" "-ORDER[<num> Polynomial Order]:file:_files" "-RESAMPLING[<str> Resampling]:file:_files" "-BYTEWISE[<str> Bytewise Interpolation]:file:_files" "-DATA_TYPE[<str> Data Type]:file:_files" "-CRS_STRING[<str> Coordinate System Definition]:file:_files" "-GRIDS[<str> Grids]:file:_files" "-TARGET_GRIDS[<str> Target]:file:_files" "-TARGET_DEFINITION[<str> Target Grid System]:file:_files" "-TARGET_USER_SIZE[<double>Cellsize]:file:_files" "-TARGET_USER_XMIN[<double>West]:file:_files" "-TARGET_USER_XMAX[<double>East]:file:_files" "-TARGET_USER_YMIN[<double>South]:file:_files" "-TARGET_USER_YMAX[<double>North]:file:_files" "-TARGET_USER_COLS[<num> Columns]:file:_files" "-TARGET_USER_ROWS[<num> Rows]:file:_files" "-TARGET_USER_FLAT[<str> Rounding]:file:_files" "-TARGET_USER_FITS[<str> Fit]:file:_files" "-TARGET_TEMPLATE[<str> Target System]:file:_files"  
          ;;
     esac
     ;;
   esac
}

