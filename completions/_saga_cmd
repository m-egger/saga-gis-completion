            "-VALUES[<str> Values]:file:_files" "-COUNT[<str> Number of Categories]:file:_files" "-INDEX[<str> Rao's Q]:file:_files" "-KERNEL_TYPE[<str> Kernel Type]:file:_files" "-KERNEL_RADIUS[<num>Radius]:file:_files"  
          ;;
        25)
          _arguments \
            "-VALUES[<str> Values]:file:_files" "-NORMALIZE[<str> Normalize]:file:_files" "-INDEX[<str> Rao's Q]:file:_files" "-DISTANCE[<str> Distance]:file:_files" "-LAMBDA[<double> Lambda]:file:_files" "-KERNEL_TYPE[<str> Kernel Type]:file:_files" "-KERNEL_RADIUS[<num>Radius]:file:_files"  
