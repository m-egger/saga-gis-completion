      ;;
    args)
      case "$line[1]" in
        0)
          _arguments \
            "-RED[<str> Red Reflectance]:file:_files" "-NIR[<str> Near Infrared Reflectance]:file:_files" "-PVI0[<str> Perpendicular Vegetation Index (Richardson and Wiegand, 1977)]:file:_files" "-PVI1[<str> Perpendicular Vegetation Index (Perry and Lautenschlager, 1984)]:file:_files" "-PVI2[<str> Perpendicular Vegetation Index (Walther and Shabaani)]:file:_files" "-PVI3[<str> Perpendicular Vegetation Index (Qi, et al., 1994)]:file:_files" "-TSAVI[<str> Transformed Soil Adjusted Vegetation Index (Baret et al. 1989)]:file:_files" "-ATSAVI[<str> Transformed Soil Adjusted Vegetation Index (Baret and Guyot, 1991)]:file:_files" "-INTERCEPT[<double>Intercept of Soil Line]:file:_files" "-SLOPE[<double> Slope of Soil Line]:file:_files"  
