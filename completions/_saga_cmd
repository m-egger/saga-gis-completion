            "-SHAPES[<str> Shapes]:file:_files" "-VARIABLE[<str> Variable]:file:_files" "-SUMMARY[<str> Summary]:file:_files" "-DW_WEIGHTING[<str> Weighting Function]:file:_files" "-DW_IDW_POWER[<double> Power]:file:_files" "-DW_BANDWIDTH[<double> Bandwidth]:file:_files" "-SEARCH_RANGE[<str> Search Range]:file:_files" "-SEARCH_RADIUS[<double> Maximum Search Distance]:file:_files" "-SEARCH_POINTS_ALL[<str>Number of Points]:file:_files" "-SEARCH_POINTS_MAX[<num>Maximum]:file:_files"  
          ;;
     esac
     ;;
   esac
}

_table_calculus(){
  _arguments -C "1: :->cmds" "*::arg:->args" 
 
  case "$state" in
    cmds)
      _values "table_calculus command" \
        "0[Function Fit]" \
        "1[Field Calculator]" \
        "5[Moving Statistics]" \
        "7[Principal Component Analysis]" \
        "8[Fill Gaps in Ordered Records]" \
        "9[Fill Gaps in Records]" \
        "11[Find Field of Extreme Value]" \
        "15[Field Statistics]" \
        "16[Record Statistics]" \
        "18[Aggregate Values by Attributes]" \
        "19[Confusion Matrix (Table Fields)]" \
        "20[Record Aggregation]" \

      ;;
    args)
      case "$line[1]" in
        0)
          _arguments \
            "-SOURCE[<str> Source]:file:_files" "-YFIELD[<str> y-Values]:file:_files" "-USE_X[<str> Use x-Values]:file:_files" "-XFIELD[<str> x-Values]:file:_files" "-FORMEL[<str> Formula]:file:_files" "-ITER[<num> Iterationen]:file:_files" "-LAMDA[<double>Max Lamda]:file:_files"  
          ;;
        1)
          _arguments \
            "-TABLE[<str> Table]:file:_files" "-FIELD[<str> Result Field]:file:_files" "-RESULT_TABLE[<str> Result]:file:_files" "-RESULT_SHAPES[<str>Result]:file:_files" "-NAME[<str> Field Name]:file:_files" "-FORMULA[<str> Formula]:file:_files" "-SELECTION[<str> Selection]:file:_files" "-USE_NODATA[<str> Use No-Data]:file:_files"  
          ;;
        5)
          _arguments \
            "-INPUT[<str> Input]:file:_files" "-OUTPUT[<str> Output]:file:_files" "-FIELD[<str> Field]:file:_files" "-INDEX[<str> Order by...]:file:_files" "-MISSING[<str> Missing Records]:file:_files" "-OFFSET[<num> Offset]:file:_files" "-LENGTH[<num> Number of Records]:file:_files" "-MEAN[<str> Mean]:file:_files" "-FIELD_MEAN[<str> Mean]:file:_files" "-MEDIAN[<str> Median]:file:_files" "-FIELD_MEDIAN[<str> Median]:file:_files" "-MIN[<str> Minimum]:file:_files" "-FIELD_MIN[<str> Minimum]:file:_files" "-MAX[<str> Maximum]:file:_files" "-FIELD_MAX[<str> Maximum]:file:_files" "-STDV[<str> Standard Deviation]:file:_files" "-FIELD_STDV[<str> Standard Deviation]:file:_files" "-STDV_LO[<str> Lower Standard Deviation]:file:_files" "-FIELD_STDV_LO[<str>Lower Standard Deviation]:file:_files" "-STDV_HI[<str> Higher Standard Deviation]:file:_files" "-FIELD_STDV_HI[<str>Higher Standard Deviation]:file:_files"  
          ;;
        7)
          _arguments \
            "-TABLE[<str> Table]:file:_files" "-PCA[<str> Principal Components]:file:_files" "-METHOD[<str>Method]:file:_files" "-NFIRST[<num>Number of Components]:file:_files"  
