        20)
          _arguments \
            "-GRIDS[<str> Values]:file:_files" "-MAJORITY[<str> Majority]:file:_files" "-MAJORITY_COUNT[<str>Majority Count]:file:_files" "-MINORITY[<str> Minority]:file:_files" "-MINORITY_COUNT[<str>Minority Count]:file:_files" "-NUNIQUES[<str> Number of Unique Values]:file:_files" "-UNAMBIGUOUS[<str> Unambiguous]:file:_files"  
          ;;
        21)
          _arguments \
            "-GRID[<str> Grid]:file:_files" "-HISTOGRAM[<str> Histogram]:file:_files" "-CLASSIFY[<str> Classification]:file:_files" "-BINS[<num> Number of Classes]:file:_files" "-RANGE_MIN[<double> Value Range]:file:_files" "-RANGE_MAX[<double> Value Range]:file:_files" "-LUT[<str> Lookup Table]:file:_files" "-UNCLASSED[<str> Report Unclassified Cells]:file:_files" "-PARALLEL[<str> Parallelized]:file:_files" "-MAXSAMPLES[<double>Maximum Samples]:file:_files"  
          ;;
        directional_grid_statistics)
          _arguments \
            "-GRID[<str> Grid]:file:_files" "-MEAN[<str> Arithmetic Mean]:file:_files" "-BEGIN[<double> Start Direction]:file:_files" "-END[<double> Stop Direction]:file:_files" "-STEP[<double> Directional Step]:file:_files" "-TOLERANCE[<double> Tolerance]:file:_files" "-MAXDISTANCE[<num> Maximum Distance]:file:_files" "-DW_WEIGHTING[<str> Weighting]:file:_files" "-DW_IDW_POWER[<double>Inverse Distance Weighting Power]:file:_files" "-DW_BANDWIDTH[<double>Gaussian and Exponential Weighting Bandwidth]:file:_files"  
          ;;
     esac
     ;;
   esac
}

