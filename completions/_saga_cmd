          ;;
        9)
          _arguments \
            "-LINES_A[<str> 1st Lines Layer]:file:_files" "-LINES_B[<str> 2nd Lines Layer]:file:_files" "-CROSSINGS[<str> Crossings]:file:_files" "-ATTRIBUTES[<str>Parent Attributes]:file:_files"  
          ;;
        10)
          _arguments \
            "-LINES_IN[<str> Lines]:file:_files" "-LINES_OUT[<str> Closed Lines]:file:_files" "-TOLERANCE[<double> Tolerance]:file:_files" "-MAX_LENGTH[<double>Maximum Length]:file:_files"  
          ;;
