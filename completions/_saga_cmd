_sim_ihacres(){
  _arguments -C "1: :->cmds" "*::arg:->args" 
 
  case "$state" in
    cmds)
      _values "sim_ihacres command" \
        "0[IHACRES Calibration (2)]" \
        "1[IHACRES Version 1.0]" \
        "2[IHACRES Basin]" \
        "3[IHACRES Elevation Bands]" \
        "4[IHACRES Elevation Bands Calibration]" \

      ;;
    args)
      case "$line[1]" in
        0)
          _arguments \
            "-TABLE[<str> Table]:file:_files" "-DATE_Field[<str> Date Column]:file:_files" "-DISCHARGE_Field[<str>Streamflow Column]:file:_files" "-PCP_Field[<str> Precipitation Column]:file:_files" "-TMP_Field[<str> Temperature Column]:file:_files" "-INFLOW_Field[<str> Subbasin Inflow]:file:_files" "-bUPSTREAM[<str> Is the subbasin upstream (no external inflow)]:file:_files" "-USE_TMP[<str> Using temperature data?]:file:_files" "-NSIM[<num> Number of Simulations]:file:_files" "-AREA[<double> Area of the Watershed in (km2)]:file:_files" "-STORAGE[<str> Storage]:file:_files" "-IHACVERS[<str> IHACRES Version]:file:_files" "-SNOW_TOOL[<str> Snow Tool on/off]:file:_files" "-TABLEout[<str> Table]:file:_files" "-TABLEparms[<str> Table]:file:_files"  
          ;;
        1)
          _arguments \
            "-TABLE[<str> Table]:file:_files" "-DATE_Field[<str> Date Column]:file:_files" "-DISCHARGE_Field[<str>Streamflow Column]:file:_files" "-PCP_Field[<str> Precipitation Column]:file:_files" "-TMP_Field[<str> Temperature Column]:file:_files" "-USE_TMP[<str> Using temperature data?]:file:_files" "-AREA[<double> Area of the Watershed in (km2)]:file:_files" "-CFAC[<double> Parameter (c)]:file:_files" "-TwFAC[<double> (Tw) wetness decline time constant]:file:_files" "-STORAGE[<str> Storage]:file:_files" "-IHACVERS[<str> IHACRES Version]:file:_files" "-SNOW_TOOL[<str> Using the snow-melt module?]:file:_files" "-WRITEALL_TS[<str> Write all calculated Time Series in a table?]:file:_files" "-TABLEout[<str> Table]:file:_files" "-TABLEparms[<str> Table]:file:_files" "-TABLEsettings[<str> Table]:file:_files"  
          ;;
        2)
          _arguments \
            "-TABLEout[<str> Table]:file:_files" "-NSUBBASINS[<str>Number of sub-basins]:file:_files" "-IHACVERS[<str> IHACRES Version]:file:_files" "-STORAGE[<str> Storage]:file:_files" "-SNOW_TOOL[<str> Using the snow-melt module?]:file:_files"  
          ;;
        3)
          _arguments \
            "-TABLEout[<str> Table]:file:_files" "-NELEVBANDS[<str> Number of elevation bands]:file:_files" "-AREA_tot[<double>Total Catchment Area (km2)]:file:_files" "-IHACVERS[<str> IHACRES Version]:file:_files" "-STORAGE[<str> Storage]:file:_files" "-SNOW_TOOL[<str> Using the snow-melt module?]:file:_files"  
          ;;
        4)
          _arguments \
            "-TABLEout[<str> Table]:file:_files" "-TABLEparms[<str> Table]:file:_files" "-NELEVBANDS[<str> Number of elevation bands]:file:_files" "-NSIM[<num> Number of Simulations]:file:_files" "-AREA_tot[<double>Total Catchment Area (km2)]:file:_files" "-IHACVERS[<str> IHACRES Version]:file:_files" "-STORAGE[<str> Storage]:file:_files" "-SNOW_TOOL[<str> Using the snow-melt module?]:file:_files" "-OBJ_FUNC[<str> Objective Function]:file:_files" "-NSEMIN[<double> Minimum Nash-Sutcliffe Efficiency]:file:_files"  
          ;;
     esac
     ;;
   esac
}

