          ;;
        13)
          _arguments \
            "-PG_HOST[<str> Host]:file:_files" "-PG_PORT[<num> Port]:file:_files" "-PG_DB[<str> Database]:file:_files" "-PG_USER[<str> User]:file:_files" "-PG_PWD[<str> Password]:file:_files" "-TABLE[<str> Table]:file:_files" "-TABLE_PK[<str>Primary Key]:file:_files" "-TABLE_NN[<str>Not Null]:file:_files" "-TABLE_UQ[<str>Unique]:file:_files" "-NAME[<str> Table Name]:file:_files" "-EXISTS[<str> If table exists...]:file:_files"  
          ;;
        14)
